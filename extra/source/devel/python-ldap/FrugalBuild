pkgname=python-ldap
pkgver=2.0.11
pkgrel=1
pkgdesc="This is a python module to access an LDAP server"
groups=('devel-extra')
archs=('i686')
depends=('python' 'openldap')
Finclude sourceforge
source=(${source[@]} setup.cfg)
sha1sums=('a19dca037ce1aedafb7f321a3938c3380ee1c91a' \
	  '2b5a73c5de50c08f2dcb54bd7052550f70f3d311')

build() {
	Fcd
	cp -f $Fsrcdir/setup.cfg setup.cfg
	Fmake
	Fmakeinstall
}

# optimalization OK
