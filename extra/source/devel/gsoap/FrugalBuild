pkgname=gsoap
pkgver=2.7.6d
pkgrel=1
pkgdesc="A cross-platform development environment for C and C++ SOAP/XML Web services."
url="http://gsoap2.sourceforge.net/"
depends=('libstdc++')
groups=('devel-extra')
archs=('i686')
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=52781|grep stable |sed 's/.*P \(.*\) s.*/\1/;q'"
source=(http://dl.sourceforge.net/sourceforge/gsoap2/${pkgname}_$pkgver.tar.gz)
sha1sums=('2ea7f4651a63d33e619df9d028b36c9358f73f1c')

build()
{
	Fcd $pkgname-`echo $pkgver|sed 's/\.[^\.]*$//'`
	Fbuild
}

# optimalization OK
