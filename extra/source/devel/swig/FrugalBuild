pkgname=swig
pkgver=1.3.24
pkgrel=1
pkgdesc="SWIG interface compiler"
url="http://www.swig.org"
archs=('i686' 'x86_64')
groups=('devel-extra')
up2date="lynx -dump $url/download.html|grep devel|sed 's/.*G \(.*\) i.*/\1/;q'"
source=(http://dl.sourceforge.net/sourceforge/$pkgname/$pkgname-$pkgver.tar.gz)
sha1sums=('0a8344e7e096a84f07be731085de386c2555d934')

build()
{
    Fcd SWIG-$pkgver
    Fbuild
}

# optimalization OK
