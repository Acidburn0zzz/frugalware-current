pkgname=python-logilab-common
pkgver=0.16.1
pkgrel=1
pkgdesc="Providing low level functionnalities needed for logilab python programs"
url="http://www.logilab.org/projects/common/project_view"
source=(ftp://ftp.logilab.org/pub/common/common-$pkgver.tar.gz)
depends=('python' 'pyxml')
archs=('i686' 'x86_64')
groups=('devel-extra')
up2date="lynx -dump http://www.logilab.org/ftp/pub/common | Flasttar"
