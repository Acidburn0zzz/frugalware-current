pkgname=bzr
pkgver=0.0.6
pkgrel=1
pkgdesc="Next generation distributed version control"
url="http://bazaar-ng.org/"
depends=('elementtree' 'urlgrabber')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url |grep Latest|sed 's/.*-\(.*\) .*/\1/'"
source=($url/pkg/$pkgname-$pkgver.tar.gz)
sha1sums=('6694a61db344820cf38237be57dc556bb7b4f93d')

