pkgname=ctags
pkgver=5.5.4
pkgrel=1
pkgdesc="A multilanguage implementation of Ctags"
url="http://ctags.sourceforge.net/"
license="GPL2"
up2date="lynx -dump $url | grep 'Source only' | cut -d - -f 2 | sed 's|.tar.gz||'"
source=(http://dl.sourceforge.net/sourceforge/$pkgname/$pkgname-$pkgver.tar.gz)
sha1sums=('1f912ef66c44d5527ce44c447a13fc33188bf16e')
groups=('devel-extra')
archs=('i686')
depends=('glibc')

build()
{
	Fbuild
	Fln /usr/man/man1/ctags.1.gz /usr/man/man1/etags.1.gz
}

