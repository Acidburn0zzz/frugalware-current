pkgname=ctags
pkgver=5.6
pkgrel=1
pkgdesc="A multilanguage implementation of Ctags"
url="http://ctags.sourceforge.net/"
license="GPL2"
up2date="lynx -dump $url | grep 'Source only' | cut -d - -f 2 | sed 's|.tar.gz||'"
source=(http://heanet.dl.sourceforge.net/sourceforge/$pkgname/$pkgname-$pkgver.tar.gz)
sha1sums=('930afaa138624717393fe475201f795251bd5e5e')
groups=('devel-extra')
archs=('i686' 'x86_64')
depends=('glibc')
conflicts=('emacs')

build()
{
	Fbuild
	Fln /usr/man/man1/ctags.1.gz /usr/man/man1/etags.1.gz
}

# optimization OK
