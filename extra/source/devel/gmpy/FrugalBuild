# Last Modified: Wed, 20 Jul 2005 03:28:53 +0200
# Compiling Time: ~0.00 SBU
# Maintainer: BMH1980 <bmh1980@frugalware.org>

pkgname=gmpy
pkgver=1.0
tarver=`echo $pkgver | tr -d .`
pkgrel=1
pkgdesc="Python-usable modules providing multiprecision arithmetic functionality"
url="http://gmpy.sourceforge.net/"
license="LGPL"
up2date="lynx -dump $url | grep 'release:' | cut -d ' ' -f 13"
source=(http://dl.sourceforge.net/sourceforge/$pkgname/$pkgname-sources-$tarver.zip)
md5sums=('a34de2f217f89d82e8d9dd1abe4f1122')
groups=('devel-extra')
archs=('i686' 'x86_64')
depends=('gmp' 'python')

build()
{
	Fcd $pkgname
	Fbuild
	Fdocrel doc/*
}

# vim: ft=sh

# optimalization OK
