pkgname=twisted
pkgver=2.0.1
pkgrel=1
pkgdesc="An event-driven networking framework written in Python."
url="http://twistedmatrix.com/"
depends=('pyopenssl>=0.5.1' 'pyserial' 'pycrypto>=1.9a6' 'zopeinterface')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump ${url}projects/core/|grep bz2$|sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=(http://tmrc.mit.edu/mirror/twisted/Twisted/2.0/Twisted-$pkgver.tar.bz2)
md5sums=('6cf0ebb84731f8c2c45ea125efda77fe')

build()
{
	Fcd Twisted-$pkgver
	Fbuild
}

