# Last Modified: Fri, 29 Jul 2005 18:41:01 +0200
# Compiling Time: ~1 minute
# Maintainer: VMiklos <mamajom@axelero.hu>

pkgname=codeville
pkgver=0.1.13
pkgrel=1
pkgdesc="A distributed version control system."
url="http://codeville.org/"
depends=('python')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/download.php|grep gz$|sed 's/.*-\(.*\)\.t.*/\1/'"
source=($url/download/Codeville-$pkgver.tar.gz)
sha1sums=('3059289c64d98d90fdf07c22ed7ce043b45110ff')

build()
{
	Fcd Codeville-$pkgver
	Fbuild
}

# vim: ft=sh
