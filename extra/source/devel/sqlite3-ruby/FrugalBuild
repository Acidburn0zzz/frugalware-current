pkgname=sqlite3-ruby
pkgver=1.1.0
pkgrel=1
pkgdesc="A Ruby interface for the SQLite database engine"
url="http://rubyforge.org/projects/sqlite-ruby/"
depends=('ruby' 'sqlite3')
groups=('devel-extra')
archs=('i686')

Finclude gem
sha1sums=('7e803b1183ba276de7288acc2336a82efc2c053e')

build() {
	Finstallgem
	Frm "$archdir"
}

