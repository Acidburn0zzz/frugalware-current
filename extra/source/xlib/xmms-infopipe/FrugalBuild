pkgname=xmms-infopipe
pkgver=1.3
pkgrel=1
pkgdesc="XMMS infopipe plugin "
url="http://www.beastwithin.org/users/wwwwolf/code/xmms/infopipe.html"
depends=('xmms')
groups=('xlib-extra')
archs=('i686')
up2date="lynx -dump http://www.beastwithin.org/users/wwwwolf/code/xmms/infopipe.html |grep tar.gz$|sed -ne 's/.*e-\(.*\)\.t.*/\1/;1 p'"
source=(http://www.beastwithin.org/users/wwwwolf/code/xmms/$pkgname-$pkgver.tar.gz)
md5sums=('1ccc90254c58a81f87abc43720fe71bf')


build() {
        Fcd
        Fmake || return 1
        Fmakeinstall
}

# optimalization OK

