pkgname=qca
pkgver=1.0
pkgrel=1
pkgdesc="QT Cryptography Architecture"
url="http://delta.affinix.com/qca/"
groups=('xlib-extra')
archs=('i686')
license="LGPL"
depends=('qt' 'gcc')
source=(http://delta.affinix.com/qca/$pkgname-$pkgver.tar.bz2)
md5sums=('ee44022eb0e5b8b5df64c62630f6e6b6')
up2date="lynx -dump http://delta.affinix.com/qca/ |grep qca.*.tar.bz2 |sed -n 's/.*a-\(.*\).t.*/\1/;1 p'"

build() {
	Fcd
	./configure --prefix=$Fprefix
	Fmake
	Fmakeinstall INSTALL_ROOT=$Fdestdir
}

# optimalization OK

