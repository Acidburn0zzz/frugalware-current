# Last Modified: Wed, 09 Nov 2005 08:59:09 +0100
# Compiling Time: 0.04 SBU
# Maintainer: Tuxbubling <tuxbubling@tiscali.fr>

pkgname=xmms-jack
pkgver=0.15
pkgrel=1
pkgdesc="XMMS audio output plugin for the jack audio server."
url="http://sourceforge.net/projects/xmms-jack"
depends=('jack' 'xmms' 'libsamplerate')
groups=('xlib-extra')
archs=('i686')
up2date="lynx -dump $url | grep 'xmms-jack [0-9]' | sed 's/.*jack //' | cut -f 1 -d ' '"
source=(http://dl.sourceforge.net/xmms-jack/$pkgname-$pkgver.tar.gz)
sha1sums=('7d16ed2117635a487078982047113c9cb8ae7057')

build()
{
	Fcd $pkgname
	Fsed "CFLAGS =" "CFLAGS = $CFLAGS" Makefile.in
	Fbuild
}

# vim: ft=sh
# optimalization OK
