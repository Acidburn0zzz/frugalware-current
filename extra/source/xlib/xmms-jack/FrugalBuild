pkgname=xmms-jack
pkgver=0.16
pkgrel=1
pkgdesc="XMMS audio output plugin for the jack audio server."
url="http://sourceforge.net/projects/xmms-jack"
depends=('jack' 'xmms' 'libsamplerate')
groups=('xlib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=70263 |grep Latest |sed 's/ //g'| sed 's/.*ck//g'|sed 's/.....No.*//g'|sed 's/.*ses//g'|sed -e '/^$/d'"
source=(http://dl.sourceforge.net/xmms-jack/$pkgname-$pkgver.tar.gz)
sha1sums=('6439b0afb9b11235c701d24057901968a747ddca')

build()
{
	Fcd $pkgname
	Fsed "CFLAGS =" "CFLAGS = $CFLAGS" Makefile.in
	Fbuild
}

# optimalization OK
