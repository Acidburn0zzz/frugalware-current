pkgname=libpypac
pkgver=0.4.1
pkgrel=1
pkgdesc="Libpypac is a package management library framework for pacman."
url="http://home.gna.org/libpypac/"
depends=('python')
groups=('xlib-extra')
archs=('i686')
up2date="lynx -dump http://download.gna.org/libpypac/libpypac/ | grep 'tar.gz' | sed -ne 's/.*-//;s/\.tar\.gz//;2 p'"
source=(http://download.gna.org/libpypac/libpypac/$pkgname-$pkgver.tar.gz)
sha1sums=('c429a2c0e36de2993108222088de28d05a399de5')

