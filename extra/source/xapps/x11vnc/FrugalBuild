pkgname=x11vnc
pkgver=0.7.1
pkgrel=1
pkgdesc="a VNC server for real X displays"
url="http://www.karlrunge.com/x11vnc/"
license="GPL"
depends=('x' 'libjpeg' 'zlib')
archs=('i686')
groups=('xapps-extra')
up2date="lynx -dump $url|grep $pkgname-[0-9\.]*\.tar.gz|sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=(http://dl.sourceforge.net/sourceforge/libvncserver/$pkgname-$pkgver.tar.gz)
md5sums=('84e54a694aad6f122813b0930fb4402c')

# optimalization ok

