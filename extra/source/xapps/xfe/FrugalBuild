pkgname=xfe
pkgver=0.84
pkgrel=1
pkgdesc="X File Explorer (Xfe) is an MS-Explorer like file manager for X."
url="http://roland65.free.fr/xfe/"
makedepends=('fox')
depends=('fox')
groups=('xapps-extra')
archs=('i686')
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=64835 |grep xfe.*.tar.gz |sed -n -e 's/.*e-\(.*\).tar.*/\1/;1 p'"
source=(http://ovh.dl.sourceforge.net/sourceforge/xfe/$pkgname-$pkgver.tar.gz)
md5sums=('8251a0c1545d3cbbb909beb8027414a6')

build() {
	Fmake || return 1
	Fmakeinstall
}

