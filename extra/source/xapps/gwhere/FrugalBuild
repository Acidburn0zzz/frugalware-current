pkgname=gwhere
pkgver=0.2.1
pkgrel=1
pkgdesc="WhereIsIt clone"
url="http://www.gwhere.org/"
depends=('gtk+' 'zlib')
groups=('xapps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/download/source | grep 'http.*.tar.' | sed -n 's/.*$pkgname-\(.*\).tar.*/\1/;$ p'"
source=($url/download/sources/$pkgname-$pkgver.tar.gz)
sha1sums=('fb298ddcfb74b9d0518b189705cff1fa6d20ae04')

build()
{
	Fbuild
}

