pkgname=tsclient
pkgver=0.140
pkgrel=1
pkgdesc="A frontend for rdesktop and other remote desktop tools."
url="http://www.gnomepro.com/tsclient"
depends=('rdesktop' 'glib2' 'gtk+2')
archs=('i686')
groups=('xapps-extra')
up2date="lynx -dump $url/|grep $pkgname-[0-9\.]*\.tar.gz|sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=($url/$pkgname-$pkgver.tar.gz)
md5sums=('c10a5a151a1ece653f62e07b11228534')
license="GPL"

# optimalization ok

