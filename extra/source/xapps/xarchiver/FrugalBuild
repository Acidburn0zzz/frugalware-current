pkgname=xarchiver
pkgver=0.3.3
pkgrel=1
pkgdesc="A lightweight GTK2 only frontend to zip, bzip2, gzip, rar, 7z, arj and rpm"
url="http://xarchiver.xfce.org"	
depends=('gtk+2' 'gzip' 'bzip2' 'tar')
groups=('xapps-extra')
archs=('i686' 'x86_64')
_F_sourceforge_ext=.tar.bz2
Finclude sourceforge
install=xarchiver.install
sha1sums=('29c754d330f0cb14ccd80ea714157d895fe9a807')

build() 
{
	Fcd Xarchiver-$pkgver
	Fbuild
}

# optimization OK
