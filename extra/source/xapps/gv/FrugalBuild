pkgname=gv
pkgver=3.6.1
pkgrel=3
pkgdesc="gv allows to view and navigate through PostScript and PDF documents on an X display"
url="http://www.gnu.org/software/gv/"
depends=('xaw3d')
rodepends=('xespgs')
up2date="lynx -dump http://ftp.gnu.org/gnu/gv/ | grep 'http.*gv.*\.tar\.gz' | sed -e 's/.*gv-\([0-9]*\.[0-9]*\.[0-9]*\)\.tar\.gz.*/\1/' | tail -n 1"
source=(http://ftp.gnu.org/gnu/$pkgname/$pkgname-$pkgver.tar.gz)
# could not find official md5sum, made it myself
groups=('xapps-extra')
archs=('i686')
sha1sums=('a094d019772e11c6003cc8682c945b7b9bc700b7')

