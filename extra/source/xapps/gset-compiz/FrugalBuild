pkgname=gset-compiz
pkgver=0.3.3
pkgrel=1
pkgdesc="A configuration tool for Compiz"
url="http://www.compiz.net"
depends=('compiz-cvs')
groups=('xapps-extra')
archs=('i686')
up2date="$pkgver"
source=(http://ftp.frugalware.org/pub/other/sources/$pkgname/$pkgname-$pkgver.tar.bz2)
sha1sums=('c2e9621a661ab7393edf98f322b6c37d55fcfd1d')
Finclude gnome-scriptlet
_F_gnome_desktop="y"

