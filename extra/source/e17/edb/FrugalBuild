pkgname=edb
pkgver=20051120
pkgrel=1
pkgdesc="EDB is a database convenience library for E17."
url="http://www.get-e.org"
depends=('gtk+')
groups=('e17-extra' 'e17-core')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('a57542ed2fd919d16a70e97415bd440592342d3f')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

