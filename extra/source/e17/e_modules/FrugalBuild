pkgname=e_modules
pkgver=20051120
pkgrel=1
pkgdesc="Additional modules for E17."
url="http://www.get-e.org"
depends=('ecore' 'ewm')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('f8a44c9c88c3d2545db4564a832f02b63fc13c88')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

