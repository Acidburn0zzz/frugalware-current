pkgname=e_modules
pkgver=20051103
pkgrel=1
pkgdesc="Additional modules for E17."
url="http://enlightenment.freedesktop.org"
depends=('ecore' 'ewm')
groups=('e17-extra' 'e17-apps')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
up2date=20051103
sha1sums=('44684d56bb86aee3c2a64088d127ee8264797dee')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

