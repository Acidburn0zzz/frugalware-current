pkgname=epsilon
pkgver=20051103
pkgrel=1
pkgdesc="Epsilon is a flexable and powerful image thumbnailing library for E17."
url="http://enlightenment.freedesktop.org"
depends=('edje' 'epeg')
groups=('e17-extra' 'e17-core')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
up2date=20051103
sha1sums=('e5f026de17d93d8b5ca1aded7a0984178f3d3090')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

