pkgname=epeg
pkgver=20051103
pkgrel=1
pkgdesc="Epeg is the worlds fastest JPEG thumbnailing library for E17"
url="http://enlightenment.freedesktop.org"
depends=('libjpeg')
groups=('e17-extra' 'e17-core')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
up2date=20051103
sha1sums=('82ff1f12228821ec006194fc29f2b8274f9d418c')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

