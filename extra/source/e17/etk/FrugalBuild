pkgname=etk
pkgver=20051103
pkgrel=1
pkgdesc="EFL toolkit for E17."
url="http://www.get-e.org"
depends=('edje')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
up2date=20051103
sha1sums=('976e33bbb6d2ea82689a877c89412c7ece7d60b1')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

