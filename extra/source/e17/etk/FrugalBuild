pkgname=etk
pkgver=20051120
pkgrel=1
pkgdesc="EFL toolkit for E17."
url="http://www.get-e.org"
depends=('edje')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('a53a6de05a854d9e0f26a199de14ff23645a713b')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

