pkgname=elation
pkgver=20051120
pkgrel=1
pkgdesc="Elation media player for E17."
url="http://www.get-e.org"
depends=('emotion')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('653291ae2fed8909645d5ef4d24c40869eaf79c3')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

