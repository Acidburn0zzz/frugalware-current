pkgname=entrance
pkgver=20051103
pkgrel=1
pkgdesc="Entrance is a login manager for E17."
url="http://enlightenment.freedesktop.org"
depends=('edje' 'esmart')
groups=('e17-extra' 'e17-apps')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
up2date=20051103
sha1sums=('130bbf08e3b39813a35f1bd49de8011bcddec398')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

