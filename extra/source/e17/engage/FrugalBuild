pkgname=engage
pkgver=20051120
pkgrel=1
pkgdesc="Engage is a dockbar and E17 module."
url="http://www.get-e.org"
depends=('ewl' 'ewm' 'esmart' 'imlib2')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz transparent.patch)
Finclude e17
sha1sums=('838e73c3d3e6d06480459faf2367512086b34baf' \
	'6867d9f6d0538ad04cc92a2a4dd8c86de180c608')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

