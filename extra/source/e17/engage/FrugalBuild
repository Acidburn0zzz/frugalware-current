pkgname=engage
pkgver=20051103
pkgrel=1
pkgdesc="Engage is a dockbar and E17 module."
url="http://enlightenment.freedesktop.org"
depends=('ewl' 'ewm' 'esmart')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz transparent.patch)
up2date=20051103
sha1sums=('7c1353e92f6e1056ce9b8c46eba23979ea5969d5' \
	'6867d9f6d0538ad04cc92a2a4dd8c86de180c608')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

