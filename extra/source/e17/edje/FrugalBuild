pkgname=edje
pkgver=20051120
pkgrel=1
pkgdesc="Edje is a complex graphical design & layout library for E17"
url="http://www.get-e.org"
depends=('ecore' 'embryo')
groups=('e17-extra' 'e17-core')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('da3d0abf79c1d406605277c0814071d4358d26c8')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

