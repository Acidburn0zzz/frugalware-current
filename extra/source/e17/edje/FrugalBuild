pkgname=edje
pkgver=20060103
pkgrel=1
pkgdesc="Edje is a complex graphical design & layout library for E17"
url="http://www.get-e.org"
depends=('ecore' 'embryo')
groups=('e17-extra' 'e17-core')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('c5d8f1af5fc44893eeb7471dc4c8f2dcdb99c25b')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

