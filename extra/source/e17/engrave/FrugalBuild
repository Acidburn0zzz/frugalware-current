pkgname=engrave
pkgver=20051120
pkgrel=1
pkgdesc="Engrave is a library for editing edje .eet files for E17"
url="http://www.get-e.org"
depends=('ecore')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('68888bd16efcf0a6e387310ea27fc1df30a691ae')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

