pkgname=esmart
pkgver=20051120
pkgrel=1
pkgdesc="Esmart is a collection of evas smart objects for E17."
url="http://www.get-e.org"
depends=('epsilon')
groups=('e17-extra' 'e17-core')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('d56bbb251806b51404d4c96d6cb9d6e16ff9c8de')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

