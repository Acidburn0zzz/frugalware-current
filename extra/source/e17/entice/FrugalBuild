pkgname=entice
pkgver=20051103
pkgrel=1
pkgdesc="Entice is an image viewer for E17."
url="http://enlightenment.freedesktop.org"
depends=('esmart')
groups=('e17-extra' 'e17-apps')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
up2date=20051103
sha1sums=('acfd4033c870f02506aacb5f7ca354f28171f12f')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

