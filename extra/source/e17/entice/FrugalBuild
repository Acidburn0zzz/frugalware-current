pkgname=entice
pkgver=20051120
pkgrel=1
pkgdesc="Entice is an image viewer for E17."
url="http://www.get-e.org"
depends=('esmart')
groups=('e17-extra' 'e17-apps')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('86aa221bf584b132e31af067cc523bcc1d9a7e1c')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

