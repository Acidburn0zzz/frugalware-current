pkgname=embrace
pkgver=20060418
pkgrel=1
pkgdesc="Mail checker and E17 module."
depends=('ewm' 'esmart' 'libxml2' 'imlib2')
groups=('e17-extra' 'e17-misc')
archs=('i686')
Finclude e17-cvs
sha1sums=('172d5c78d6dc619acbf8bf8613c509258cd804b4')

build() {
	Fcd $pkgname
	./autogen.sh
	automake || Fdie
	Fbuild
}

# optimalization OK
