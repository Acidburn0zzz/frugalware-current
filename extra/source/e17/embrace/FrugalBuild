pkgname=embrace
pkgver=20060103
Finclude e17
pkgrel=1
pkgdesc="Mail checker and E17 module."
url="http://www.get-e.org"
depends=('ewm' 'esmart' 'libxml2' 'imlib2')
groups=('e17-extra' 'e17-misc')
archs=('i686')
sha1sums=('0c4b3decce8451c789e1486193f16f9085db8de1')

build() {
	Fcd $pkgname
	./autogen.sh || Fdie
	automake || Fdie
	Fbuild
}

