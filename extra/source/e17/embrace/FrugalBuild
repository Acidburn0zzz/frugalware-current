pkgname=embrace
pkgver=20060523
pkgrel=1
pkgdesc="Mail checker and E17 module."
depends=('ewm' 'esmart' 'libxml2' 'imlib2')
groups=('e17-extra' 'e17-misc')
archs=('i686')
Finclude e17-cvs
sha1sums=('eeecbe1de7d4b926242898d78e7070619845c30b')

build() {
	Fcd $pkgname
	./autogen.sh
	automake || Fdie
	Fbuild
}

# optimization OK
