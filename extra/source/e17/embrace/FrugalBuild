pkgname=embrace
pkgver=20051120
pkgrel=1
pkgdesc="Mail checker and E17 module."
url="http://www.get-e.org"
depends=('ewm' 'esmart' 'libxml2' 'imlib2')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('51137c92058626de83d6ba912dd58bde064c4576')

build() {
	Fcd $pkgname
	./autogen.sh
	automake || Fdie
	Fbuild
}

