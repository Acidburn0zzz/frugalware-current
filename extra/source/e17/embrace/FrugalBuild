pkgname=embrace
pkgver=20060205
Finclude e17-cvs
pkgrel=1
pkgdesc="Mail checker and E17 module."
depends=('ewm' 'esmart' 'libxml2' 'imlib2')
groups=('e17-extra' 'e17-misc')
archs=('i686')
sha1sums=('ce50e533a33c0a8c510a54ac15ef8fbb0a6351be')

build() {
	Fcd $pkgname
	./autogen.sh
	automake || Fdie
	Fbuild
}

