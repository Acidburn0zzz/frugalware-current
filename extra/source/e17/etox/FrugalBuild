pkgname=etox
pkgver=20060205
pkgrel=1
pkgdesc="Etox is a type setting and text layout library based on Evas for E17."
depends=('ecore' 'edb')
groups=('e17-extra' 'e17-misc')
archs=('i686')
Finclude e17
up2date="lynx -dump http://frugalware.org/~voroskoi/e17.up2date |sed -n '1 p'"
sha1sums=('c45c495887decea9f2d4a1eb15a122c0d5685d7f')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh || Fdie
	Fbuild
}

