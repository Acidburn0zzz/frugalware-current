pkgname=etox
pkgver=20051120
pkgrel=1
pkgdesc="Etox is a type setting and text layout library based on Evas for E17."
url="http://www.get-e.org"
depends=('ecore')
groups=('e17-extra' 'e17-core')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('444ac600d6337443b792842dc794c4f9af205629')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

