pkgname=etox
pkgver=20051103
pkgrel=1
pkgdesc="Etox is a type setting and text layout library based on Evas for E17."
url="http://enlightenment.freedesktop.org"
depends=('ecore')
groups=('e17-extra' 'e17-core')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
up2date=20051103
sha1sums=('7e67b539aa49deb62d23c6da4e23914c117f04e2')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

