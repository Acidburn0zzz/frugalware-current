pkgname=elicit
pkgver=20051103
pkgrel=1
pkgdesc="Elicit is a tool for examining images on your desktop."
url="http://enlightenment.freedesktop.org"
depends=('edje' 'esmart')
groups=('e17-extra' 'e17-apps')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
up2date=20051103
sha1sums=('78c840b71ad464d2562fedda9cbdc87f611712fb')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

