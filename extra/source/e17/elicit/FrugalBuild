pkgname=elicit
pkgver=20051120
pkgrel=1
pkgdesc="Elicit is a tool for examining images on your desktop."
url="http://www.get-e.org"
depends=('edje' 'esmart' 'imlib2')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('6b935d40b4e40765258f93e05a5c9bc5924db55f')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

