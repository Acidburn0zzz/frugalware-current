pkgname=erss
pkgver=20051120
pkgrel=1
pkgdesc="eRSS is a simple tool for watching RSS v1 (RDF) and v2 (XML) feeds for E17."
url="http://www.get-e.org"
depends=('edje' 'esmart' 'etox' 'libxml2' 'imlib2')
groups=('e17-extra' 'e17-misc')
archs=('i686')
source=($pkgname-$pkgver.tar.gz)
Finclude e17
sha1sums=('663137f2e5f5b6a4702b985726565b93570f9f74')

build() {
	Fcd $pkgname
	export NOCONFIGURE="yes"
	./autogen.sh
	Fbuild
}

