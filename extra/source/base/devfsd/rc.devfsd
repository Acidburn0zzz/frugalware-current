# not required
# . /etc/rc.d/rc.functions

if [ "$1" = "start" ]; then
	if [ -r /dev/.devfsd ]; then
	       if [ -x /sbin/devfsd ]; then
			# silent as there is no splash yet
			/sbin/devfsd /dev
		fi
	fi
fi
