pkgname=libnasl
pkgver=2.2.7
pkgrel=1
pkgdesc="Libraries required by Nessus."
url="http://www.nessus.org/"
depends=('nessus-libraries' 'openssl')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=(ftp://ftp.nessus.org/pub/nessus/nessus-$pkgver/src/$pkgname-$pkgver.tar.gz)
sha1sums=('d2d0c5b8ce6e63e08069173b414fa6588837cb46')

build() {
	Fcd $pkgname
	Fbuild
}

# optimalization OK
