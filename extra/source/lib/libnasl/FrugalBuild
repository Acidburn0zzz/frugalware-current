pkgname=libnasl
pkgver=2.2.8
pkgrel=1
pkgdesc="Libraries required by Nessus."
url="http://www.nessus.org/"
depends=('nessus-libraries' 'openssl')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=(ftp://ftp.nessus.org/pub/nessus/nessus-$pkgver/src/$pkgname-$pkgver.tar.gz)
sha1sums=('d01a92380483a105427b73d5e5c33716022e54f8')

build() {
	Fcd $pkgname
	Fbuild
}

# optimization OK
