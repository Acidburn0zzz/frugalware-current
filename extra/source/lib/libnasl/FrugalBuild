pkgname=libnasl
pkgver=2.2.6
pkgrel=1
pkgdesc="Libraries required by Nessus."
url="http://www.nessus.org/"
depends=('nessus-libraries' 'openssl')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.nessus.org/ | grep 'Download Nessus' | sed 's/.*Download Nessus //;s/ now//'"
source=(ftp://ftp.nessus.org/pub/nessus/nessus-$pkgver/src/$pkgname-$pkgver.tar.gz)
sha1sums=('323475d7c11b045abd93f8adad3e7ca7ca05ed0a')

build() {
	Fcd $pkgname
	Fbuild
}

