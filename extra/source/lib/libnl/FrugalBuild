pkgname=libnl
pkgver=1.0_pre5
realver=`echo $pkgver|sed 's/_/-/'`
pkgrel=1
pkgdesc="A library to ease the use of netlink sockets"
url="http://people.suug.ch/~tgr/libnl/"
depends=('glibc')
groups=('network-extra')
archs=('i686')
up2date="lynx -dump $url | grep '${pkgname}-[0-9]' | sed 's/-pre/_pre/g' | Flasttar"
source=($url/files/$pkgname-$realver.tar.gz)
sha1sums=('1b5db325ab2f4095391eca8b1e421601611db5bc')
_F_cd_path="libnl-$realver"

# optimization OK
