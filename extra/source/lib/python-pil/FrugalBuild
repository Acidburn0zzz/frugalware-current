pkgname=python-pil
pkgver=1.1.5
pkgrel=1
pkgdesc="The Python Imaging Library"
url=(http://www.pythonware.com/products/pil/)
depends=('python')
groups=('lib-extra')
archs=('i686')
up2date="lynx -dump $url | grep -m1 current | sed 's/.*L \(.*\),.*/\1/'"
source=(http://effbot.org/downloads/Imaging-1.1.5.tar.gz)
sha1sums=('06705b31b534a15aef827bccc87dceb9ebc51b9f')

build() {
	Fcd Imaging-$pkgver
	Fbuild
}

