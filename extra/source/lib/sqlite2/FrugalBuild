# Maintainer: Laszlo Dvornik <dvornik@gnome.hu>

pkgname=sqlite2
pkgver=2.8.16
pkgrel=1
pkgdesc="A C library that implements an SQL database engine (version 2)"
url="http://www.sqlite.org/"
depends=('readline')
#makedepends=('tcl')
groups=('lib-extra')
archs=('i686')
up2date="lynx -dump http://www.sqlite.org/download.html|grep sqlite-2.*tar.gz|sed -n 's/.*e-\(.*\)\.t.*/\1/;1 p'"
source=(http://www.sqlite.org/sqlite-$pkgver.tar.gz)
sha1sums=('7bef329f78628de252c289bda7cea6f86adcc42e')

build () {
	Fcd sqlite-$pkgver
	Fbuild
}

# vim: ft=sh

# optimalization OK
