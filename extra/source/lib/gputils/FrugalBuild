pkgname=gputils
pkgver=0.13.3
pkgrel=1
pkgdesc="GPUTILS is a collection of tools for the Microchip(TM) PIC microcontrollers."
url="http://gputils.sourceforge.net"
license="GPL"
depends=('glibc')
archs=('i686' 'x86_64')
groups=('lib-extra')
up2date="lynx -dump $url/#Download|grep $pkgname-[0-9\.]|sed -n 's/.*-\(.*\)\.*./\1/;1 p'|cut -d ' ' -f 1"
source=(http://dl.sourceforge.net/sourceforge/gputils/$pkgname-$pkgver.tar.gz)
sha1sums=('50c4750d8c139e5f20438e3bd512ccba74e77b8c')

build()
{
	Fbuild
}

# optimalization OK
