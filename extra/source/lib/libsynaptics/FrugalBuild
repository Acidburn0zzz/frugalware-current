pkgname=libsynaptics
pkgver=0.14.4c
pkgrel=1
pkgdesc="A small C++ library usable by the synaptics driver."
url="http://qsynaptics.sourceforge.net/"
depends=('synaptics' 'libstdc++')
groups=('lib-extra')
archs=('i686')
up2date="lynx -dump $url |grep ^libsynaptics |sed 's/.* \(.*\)/\1/;q'"
source=($url/$pkgname-$pkgver.tar.gz $pkgname-$pkgver-destdir.diff)
sha1sums=('233ac768e1069a5ccaea2a99c2c3b6b632f2c5c0' \
	  '842df49b40106c7017320ed3b9c2e27ef6e6ff49')
