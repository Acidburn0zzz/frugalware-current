pkgver=0.2
pkgrel=1
pkgdesc="PyAlsaAudio is a Python wrapper for the Advanded Linux Sound Architecture."
url="http://www.wilstrup.net/pyalsaaudio/"
license="PSF License"
up2date="lynx -dump $url|grep -o '[0-9\.]\+\.tgz'|sed 's/.tgz//'"
source=($url/$pkgname-$pkgver.tgz)
sha1sums=('a65dc4b21a975fa3b3fd2d296717166182ffd249')
groups=('lib-extra')
archs=('i686')
depends=('python' 'alsa-lib')

