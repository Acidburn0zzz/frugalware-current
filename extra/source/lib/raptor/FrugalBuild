pkgname=raptor
pkgver=1.4.7
pkgrel=1
pkgdesc="Library providing a set of parsers that generate Resource Description Framework (RDF)."
url="http://librdf.org/raptor/"
depends=('curl' 'libxslt')
groups=('lib-extra')
archs=('i686')
up2date="lynx -dump http://librdf.org/raptor/ | grep 'Latest version' | sed 's/.*: //;s/ .*$//'"
source=(http://download.librdf.org/source/$pkgname-$pkgver.tar.gz)
sha1sums=('c5b063c3f844aa1690e7bb0cad8d949e8241302f')

