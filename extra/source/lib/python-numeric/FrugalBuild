pkgname=python-numeric
realname=Numeric
pkgver=24.2
pkgrel=1
pkgdesc="Numerical Python adds a fast array facility to the Python language."
url="http://sourceforge.net/projects/numpy"
depends=('python>=2.4')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=1369 | grep -m5 Numeric | sed 's/.*8]\(.*\) Nov.*/\1/' | tail -n1"
source=(http://heanet.dl.sourceforge.net/sourceforge/numpy/$realname-$pkgver.tar.gz)
sha1sums=('72869ef80c12630a9a125ba10c96093b6ae16aff')

build() {
	Fcd Numeric-$pkgver
	Fbuild
}

