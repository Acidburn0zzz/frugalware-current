pkgname=nessus-libraries
pkgver=2.2.5
pkgrel=1
pkgdesc="Base Nessus libraries."
url="http://www.nessus.org/"
depends=()
groups=('lib-extra')
archs=('i686')
up2date="lynx -dump http://www.nessus.org/ | grep 'Download Nessus' | sed 's/.*Download Nessus //;s/ now//'"
source=(ftp://ftp.nessus.org/pub/nessus/nessus-2.2.5/src/$pkgname-$pkgver.tar.gz)
sha1sums=('301a95ca8c77ac1fd26800765ede183d74fc2e1a')

build() {
	Fcd $pkgname
	Fbuild
	# provided by nessus-core
	Frm usr/include/nessus/includes.h
}

# vim: ft=sh

# optimalization OK
