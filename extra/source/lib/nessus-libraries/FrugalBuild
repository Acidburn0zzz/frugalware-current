pkgname=nessus-libraries
pkgver=2.2.6
pkgrel=1
pkgdesc="Base Nessus libraries."
url="http://www.nessus.org/"
depends=('glibc')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.nessus.org/ | grep 'Download Nessus' | sed 's/.*Download Nessus //;s/ now//'"
source=(ftp://ftp.nessus.org/pub/nessus/nessus-$pkgver/src/$pkgname-$pkgver.tar.gz)
sha1sums=('7552cb5c48ace2e38537d8b58a1c7968172399cc')

build() {
	Fcd $pkgname
	Fbuild
	# provided by nessus-core
	Frm usr/include/nessus/includes.h
}

# vim: ft=sh

# optimalization OK
