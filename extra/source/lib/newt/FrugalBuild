pkgname=newt
pkgver=0.51.6
pkgrel=2
pkgdesc="Semi-graphical interface."
url="http://www.partimage.org"
depends=('slang' 'popt')
groups=('lib-extra')
archs=('i686')
up2date="lynx -dump http://packages.qa.debian.org/n/newt.html |grep 'Last version'|sed 's/-.*//g'|sed 's/.* //g'"
source=(http://ftp.debian.org/debian/pool/main/n/newt/newt_$pkgver.orig.tar.gz)
sha1sums=('d097cc892913421624ace1d0abebb93ac4dd5973')

build()
{
	Fcd
	tar -xzvf newt-0.51.6.tar.gz
	cd newt-0.51.6
	Fbuild
}

