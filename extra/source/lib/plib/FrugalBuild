pkgname=plib
pkgver=1.8.4
pkgrel=1
pkgdesc="A Suite of Portable Game Libraries."
url="http://plib.sourceforge.net/"
depends=()
groups=('lib-extra')
makedepends=('x')
archs=('i686' 'x86_64')
up2date="lynx -dump $url|grep latest|sed 's/.*s \(.*\) - .*/\1/'"
source=($url/dist/$pkgname-$pkgver.tar.gz)
md5sums=('5e3f289a9d1c5de0b1cfdec76bf139e6')

# optimalization ok

