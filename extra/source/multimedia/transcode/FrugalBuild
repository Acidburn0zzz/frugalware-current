pkgver=1.0.2
pkgrel=1
pkgdesc="A suite of command line utilities for transcoding various video, audio, and container formats."
url="http://www.transcoding.org/cgi-bin/transcode"
depends=('mpeg2dec' 'zlib' 'ffmpeg' 'libjpeg' 'libdvdread')
groups=('multimedia-extra')
archs=('i686')
up2date="lynx -dump http://www.jakemsr.com/transcode/|grep $pkgname-[0-9\.]*\.tar.gz|tail -n 1|sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=(http://www.jakemsr.com/transcode/$pkgname-$pkgver.tar.gz)
