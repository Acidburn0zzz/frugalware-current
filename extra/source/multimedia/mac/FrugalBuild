pkgname=mac
pkgver=3.99_u4_b4
pkgrel=1
pkgdesc="Monkey's Audio Codec, a lossless audio codec (amost with the .ap extension)"
url="http://sourceforge.net/projects/mac-port"
makedepends=('nasm')
depends=('libstdc++')
groups=('multimedia-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=123827|grep -m 1 '\W*[^-]mac'|sed 's/.*]\([^ ]* [^ ]* [^ ]*\) .*/\1/;q'| sed 's/\(.*\) update\(.*\) build\(.*\)/\1_u\2_b\3/'"
source=(http://dl.sourceforge.net/sourceforge/mac-port/$pkgname-`echo $pkgver| sed 's/_/-/g'`.tar.gz \
	mac-3.99-u4-b4-x86_64_casting_problem.patch)
sha1sums=('4248340d03ae8cd44a5e37864c197ebff506ffb4' \
	  '212a89cb1f872443ec85ad81bc998d289487f40f')

build()
{
	Fcd $pkgname-`echo $pkgver | sed 's/_/-/g'`
	Fbuild
}

