groups=('apps-extra')
up2date="lynx -dump $url |grep $pkgname-[0-9\.]*\.tar.gz$ |sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=($url/$pkgname-$pkgver.tar.gz)
sha1sums=('2ac933b37487f60a3ce51fa6b0565fc465b87')

