pkgname=fame
pkgver=0.9.0
pkgrel=1
pkgdesc="a MPEG encoder written in C and based on libfame, which is supposed to be fast."
url="http://fame.sourceforge.net/"
depends=('lame' 'libfame')
groups=('apps-extra')
archs=('x86_64' 'i686')
up2date="lynx -dump 'http://prdownloads.sourceforge.net/fame/?sort_by=date&sort=desc'|grep fame-[0-9\.]*.tar.gz | grep -v libfame|sed -n 's/.*-\(.*\).t.*/\1/;1 p'"
source=(http://download.sourceforge.net/sourceforge/fame/$pkgname-$pkgver.tar.gz)
md5sums=('4e70f10250e95d6867160f674b91ca93')
license="GPL2"

