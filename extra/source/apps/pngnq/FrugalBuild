# Last Modified: Thu, 23 Feb 2006 15:49:35 +0100
# Compiling Time: 0.02 SBU
# Maintainer: Christian Hamar alias krix <krics@linuxforum.hu>

pkgname=pngnq
pkgver=0.3
pkgrel=1
pkgdesc="A PNG compression utility"
url="http://www.cybertherial.com/pngnq/pngnq.html"
depends=('libpng')
groups=('apps-extra')
archs=('x86_64')
up2date="lynx -dump http://www.cybertherial.com/pngnq/pngnq.html|grep Version|tr -s ' '|cut -d ' ' -f 5"
source=(http://www.cybertherial.com/pngnq/$pkgname-$pkgver-src.tar.gz \
	pngnq-makefile-corrections.patch)
sha1sums=('e4d18958772009c9d6c4068dfb19a1c130bba48f' \
	  '6dc469f09a476e0af47ea2b711a3ee06b37da8b8')

build() {
	Fcd $pkgname
	Fpatchall
	Fmake
	Fmkdir usr/bin
	Fmakeinstall
}

# optimalization OK
