up2date="lynx -dump ftp://ftp.namesys.com/pub/libreiserfs/|grep $pkgname-[0-9\.]*\.tar.gz|tail -n 1|sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=(ftp://ftp.namesys.com/pub/libreiserfs/$pkgname-$pkgver.tar.gz)
