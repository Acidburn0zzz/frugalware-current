# Last Modified: Fri, 25 Nov 2005 22:51:03 +0100
# Compiling Time: ~1 minute
# Maintainer: VMiklos <vmiklos@frugalware.org>

lang=cy
pkgname=openoffice.org-i18n-$lang
dictname=cy_GB
pkgver=2.0.0.1
pkgrel=1
pkgdesc="Welsh Localization for OpenOffice.org."
url="http://ooo.ximian.com/"
rodepends=('openoffice.org')
groups=('locale-extra')
archs=('i686')
install=$pkgname.install
Finclude openoffice-i18n
sha1sums=('d30e39d39b7ce2640153ae31c30398cecf9278e1' \
	  '7095549a89b21a1952639902fa6dcae0ec6a4e16')

build()
{
        Fresinstall
        Fmkdir $dictdir
        cp -a $dictname.{aff,dic} $Fdestdir$dictdir
}
			
