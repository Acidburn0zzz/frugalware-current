pkgname=kde-i18n-fi
pkgver=3.5.1
pkgrel=1
kdever=3.5.1
pkgdesc="Finnish Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs>=3.5.1')
rodepends=('kdebase>=3.5.1')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('0f0baf89ec2e4a6dfb47eebcb72a1e06d31b36d6')

build() 
{
     
     Fbuild --disable-debug

}

