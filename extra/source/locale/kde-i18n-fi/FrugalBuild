pkgname=kde-i18n-fi
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Finnish Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs=3.5.0')
rodepends=('kdebase=3.5.0')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('dab664b11a4cec9a231c76fe5f4bdfaada1512c0')

build() 
{
     
     Fbuild --disable-debug

}

