pkgname=kde-i18n-fy
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Frisian Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs=3.5.0')
rodepends=('kdebase=3.5.0')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('1c44f8000ac275b2ec65cba9e024dac6acdecf37')

build() 
{
     
     Fbuild --disable-debug

}

