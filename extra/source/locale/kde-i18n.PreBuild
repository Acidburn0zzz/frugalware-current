#!/bin/bash

# run this script before building

LC_ALL=
LANG=

old=3.3.0
oldshort=3.3
new=3.3.1
newshort=3.3.1

for i in kde-i18n-*
do
	cd $i
		rm *tar.bz2
		cat FrugalBuild |sed -e "s/$old/$new/" -e "s/\/$oldshort/\/$newshort/" >.new
		echo -n '# Last Modified: ' >FrugalBuild
		date >>FrugalBuild
		cat .new |grep -v 'Last Modified:' >>FrugalBuild
		rm -f .new
	cd ..
done
