# Last Modified: Fri, 28 Oct 2005 00:34:26 +0200
# Compiling Time: ~1 minute
# Maintainer: VMiklos <vmiklos@frugalware.org>

lang=fr
pkgname=openoffice.org-i18n-$lang
dictname=fr_FR
pkgver=2.0.0.1
pkgrel=1
pkgdesc="French Localization for OpenOffice.org."
url="http://ooo.ximian.com/"
rodepends=('openoffice.org')
groups=('locale-extra')
archs=('i686')
install=$pkgname.install
Finclude openoffice-i18n
sha1sums=('2fa6a5b6f4042fec53fa4d6363aba707c6d02500' \
	  '18275a59855abb81098d31d74aaff51a7ffc14e0')

build()
{
	Fresinstall
	Fmkdir $dictdir
	cp -a $dictname.{aff,dic} $Fdestdir$dictdir
}

# vim: ft=sh
