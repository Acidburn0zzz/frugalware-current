# Last Modified: Fri, 25 Nov 2005 23:48:11 +0100
# Compiling Time: ~1 minute
# Maintainer: VMiklos <vmiklos@frugalware.org>

lang=nb
pkgname=openoffice.org-i18n-$lang
dictname=nb_NO
pkgver=2.0.0.1
pkgrel=1
pkgdesc="Norwegian Bookmal Localization for OpenOffice.org."
url="http://ooo.ximian.com/"
rodepends=('openoffice.org')
groups=('locale-extra')
archs=('i686')
install=$pkgname.install
Finclude openoffice-i18n
sha1sums=('0ae492d8962f4628df1b45aa14222439b0ccb020' \
	  'c736289723351e4e8d07660b00abf15046f587a6')

build()
{
        Fresinstall
        Fmkdir $dictdir
        cp -a $dictname.{aff,dic} $Fdestdir$dictdir
}
			
