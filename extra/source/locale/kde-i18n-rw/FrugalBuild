pkgname=kde-i18n-rw
pkgver=3.5.1
pkgrel=1
kdever=3.5.1
pkgdesc="Kinyarwanda Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs>=3.5.1')
rodepends=('kdebase>=3.5.1')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('1abf2b0be8d7d691fadf6492764f0521ace5112f')

build() 
{
     
     Fbuild --disable-debug

}

