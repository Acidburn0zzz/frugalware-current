#!/bin/bash

# run this script before building

LC_ALL=
LANG=

old=1.3.4
new=1.3.5
maint="Kapolnasi Tamas <ktamas@tdc.hu>"
for i in koffice-i18n-*
do
	cd $i
		#rm *tar.bz2 *.fpm
		cat FrugalBuild |sed -e "s/$old/$new/" >.new
		echo -n '# Last Modified: ' >FrugalBuild
		date >>FrugalBuild
		cat .new |grep -v 'Last Modified:' >>FrugalBuild
		rm -f .new
	cd ..
done
