pkgname=kde-i18n-el
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="EL Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs=3.5.0')
rodepends=('kdebase=3.5.0')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('91e5594ab05fc87f87aaff67a48930dd14ef0f70')

build() 
{
     
     Fbuild --disable-debug

}

