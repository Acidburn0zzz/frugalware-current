pkgname=kde-i18n-az
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Azerbaijani Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs=3.5.0')
rodepends=('kdebase=3.5.0')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('72eee584032fe60803df7cab91312e4a8165a394')

build() 
{
     
     Fbuild --disable-debug

}

