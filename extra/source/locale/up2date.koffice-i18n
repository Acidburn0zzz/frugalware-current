#!/bin/bash

pkgurl="ftp://ftp.kde.org/pub/kde/stable/koffice-$pkgver/src/koffice-l10n"
up2date="lynx -dump $url |grep current|sed 's/.*e \(.*\) i.*/\1/'"
md5sums=(`lynx -dump ${url}releases/$pkgver-release.php|grep -1 $pkgname|sed -n 's/^ *\(.*\)/\1/;3 p'`)

build()
{
	Fcd "$pkgname-$realver"
	Fbuild || return 1
	Frm /usr/share/locale/*/LC_MESSAGES/kdgantt.mo
}
