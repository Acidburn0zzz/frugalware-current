lang=sk
pkgname=openoffice.org-i18n-$lang
dictname=sk_SK
pkgver=2.0.0.1
pkgrel=2
pkgdesc="Slovak Localization for OpenOffice.org."
url="http://ooo.ximian.com/"
rodepends=('openoffice.org')
groups=('locale-extra')
archs=('i686')
Finclude openoffice-i18n
sha1sums=('b48ca6f3994339ca486da01085e2fa66a77b8f9f' \
	  'a5a703b2da40a16db166718372a2a6842f6671e2')

build()
{
	Fresinstall
	Fmkdir $dictdir
	cp -a $dictname.{aff,dic} $Fdestdir$dictdir
}

