#!/bin/bash

oldver=3.4.0
kdever=3.4.1

for i in kde-i18n-*
do
	cd $i
	repoman del extra/source/locale/$i/$i-$oldver.tar.bz2
	repoman del extra/frugalware/$i-$oldver-1-i686.fpm
	cd ..
done

darcs push

for i in kde-i18n-*
do
	cd $i
	repoman up $i-$kdever.tar.bz2 extra/source/locale/$i
	repoman up $i-$kdever-1-i686.fpm extra/frugalware/
	cd ..
done
