# Last Modified: Fri, 25 Nov 2005 22:47:09 +0100
# Compiling Time: ~1 minute
# Maintainer: VMiklos <vmiklos@frugalware.org>

lang=cs
pkgname=openoffice.org-i18n-$lang
dictname=cs_CZ
pkgver=2.0.0.1
pkgrel=1
pkgdesc="Czech Localization for OpenOffice.org."
url="http://ooo.ximian.com/"
rodepends=('openoffice.org')
groups=('locale-extra')
archs=('i686')
install=$pkgname.install
Finclude openoffice-i18n
sha1sums=('8e34bd8b4439ff7e6e35c01e324c80757a198903' \
	  '9bee527b20a2ed1ee4f9eae3f01b67d324f9af02')

build()
{
        Fresinstall
        Fmkdir $dictdir
        cp -a $dictname.{aff,dic} $Fdestdir$dictdir
}
			
