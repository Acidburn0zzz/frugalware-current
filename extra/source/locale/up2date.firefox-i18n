# up2date firefox i18n packages

if [ "$dotversion" ]; then
	up2date="wget -O - -q ftp://ftp.mozilla.org/pub/mozilla.org/firefox/releases/|grep '>[0-9\.]*/'|sed -n 's|.*>\([0-9\.]*\)/.*|\1|;$ p'"
else
	up2date="wget -O - -q ftp://ftp.mozilla.org/pub/mozilla.org/firefox/releases/|grep 1.0.4|sed -n 's|.*>\([0-9\.]*\)/.*|\1|;$ p'"
fi
