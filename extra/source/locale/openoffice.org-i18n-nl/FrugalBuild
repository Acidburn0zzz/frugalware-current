# Last Modified: Fri, 25 Nov 2005 23:52:24 +0100
# Compiling Time: ~1 minute
# Maintainer: VMiklos <vmiklos@frugalware.org>

lang=nl
pkgname=openoffice.org-i18n-$lang
dictname=nl_NL
pkgver=2.0.0.1
pkgrel=1
pkgdesc="Dutch Localization for OpenOffice.org."
url="http://ooo.ximian.com/"
rodepends=('openoffice.org')
groups=('locale-extra')
archs=('i686')
install=$pkgname.install
Finclude openoffice-i18n
sha1sums=('9c32f132bafb90b543976300cae64ff4600d8f9e' \
	  '9f5432aeb20ab7e9a3c72877b71056a33b2bd9ba')

build()
{
        Fresinstall
        Fmkdir $dictdir
        cp -a $dictname.{aff,dic} $Fdestdir$dictdir
}
			
