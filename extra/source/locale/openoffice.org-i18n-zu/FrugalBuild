lang=zu
pkgname=openoffice.org-i18n-$lang
dictname=zu_ZA
pkgver=2.0.0.1
pkgrel=1
pkgdesc="Zulu Localization for OpenOffice.org."
url="http://ooo.ximian.com/"
rodepends=('openoffice.org')
groups=('locale-extra')
archs=('i686')
install=$pkgname.install
Finclude openoffice-i18n
sha1sums=('c66daa446c2557a7b651c406d7885b7b6341a176' \
	  'cecd9eed143c6fa4e91c9098a142e0f933a4b08a')

build()
{
        Fresinstall
        Fmkdir $dictdir
        cp -a $dictname.{aff,dic} $Fdestdir$dictdir
}
			
