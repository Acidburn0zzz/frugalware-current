pkgname=kde-i18n-fr
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="French Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs=3.5.0')
rodepends=('kdebase=3.5.0')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('7dee1f093db80a4a57bb97d09001e19af6049611')

build() 
{

      Fbuild --disable-debug 	
}

