pkgname=kde-i18n-es
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Spanish Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs=3.5.0')
rodepends=('kdebase=3.5.0')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('1602c3c42199237a734751ea8c7cca8dff600cc6')

build() 
{
     
     Fbuild --disable-debug

}

