pkgname=kde-i18n-eu
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Basque Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs=3.5.0')
rodepends=('kdebase=3.5.0')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('77126749d5b3ddf77ed09d5af99d948d1e486008')

build() 
{
     
     Fbuild --disable-debug

}

