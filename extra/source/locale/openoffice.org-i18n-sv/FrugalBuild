# Maintainer: VMiklos <vmiklos@frugalware.org>

lang=sv
pkgname=openoffice.org-i18n-$lang
dictname=sv_SE
pkgver=2.0.0.1
pkgrel=1
pkgdesc="Swedish Localization for OpenOffice.org."
url="http://ooo.ximian.com/"
rodepends=('openoffice.org')
groups=('locale-extra')
archs=('i686')
install=$pkgname.install
Finclude openoffice-i18n
sha1sums=('199c64022377c72bb67d7f54f6f9354a7b86afc3' \
	  'f5e138e456a39c8cb177826ba467f3ecdfbc03cb')

build()
{
        Fresinstall
        Fmkdir $dictdir
        cp -a $dictname.{aff,dic} $Fdestdir$dictdir
}
			
