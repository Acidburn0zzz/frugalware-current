pkgname=kde-i18n-ca
pkgver=3.5.1
pkgrel=1
kdever=3.5.1
pkgdesc="Catalan Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs>=3.5.1')
rodepends=('kdebase>=3.5.1')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('b6465906f833d447fd79ae26e8b8b151e7fa18b5')

build() 
{
     
     Fbuild --disable-debug

}

