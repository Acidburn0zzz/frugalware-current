pkgname=kde-i18n-de
pkgver=3.5.1
pkgrel=1
kdever=3.5.1
pkgdesc="German Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs>=3.5.1')
rodepends=('kdebase>=3.5.1')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('5c95005fcc1ce43b35f3997be62793facd48b11e')


build() 
{

	 Fbuild --disable-debug 
}

