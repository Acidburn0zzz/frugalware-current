lang=nn
pkgname=openoffice.org-i18n-$lang
dictname=nn_NO
pkgver=2.0.0.1
pkgrel=1
pkgdesc="Norwegian Nynorsk Localization for OpenOffice.org."
url="http://ooo.ximian.com/"
rodepends=('openoffice.org')
groups=('locale-extra')
archs=('i686')
install=$pkgname.install
Finclude openoffice-i18n
sha1sums=('233b5a2c468021a7fa8a049ba957968471247559' \
	  'c5b088c1e8dcb54d66450b77252a1b3631e38cf1')

build()
{
        Fresinstall
        Fmkdir $dictdir
        cp -a $dictname.{aff,dic} $Fdestdir$dictdir
}
			
