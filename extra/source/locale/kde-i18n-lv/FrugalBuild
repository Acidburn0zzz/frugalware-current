pkgname=kde-i18n-lv
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Latvian Localization for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src/kde-i18n"
depends=('kdelibs=3.5.0')
rodepends=('kdebase=3.5.0')
groups=('locale-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('db193b6b39b0264114d516f065141c6fac75b4e5')

build() 
{
     
     Fbuild --disable-debug

}

