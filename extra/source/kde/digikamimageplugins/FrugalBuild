pkgname=digikamimageplugins
pkgver=0.7.2
pkgrel=1
pkgdesc="Image plugins for digikam"
url="http://digikam.sourceforge.net"
depends=('digikam')
groups=('kde-extra' 'kde-apps')
source=(http://download.sourceforge.net/digikam/$pkgname-$pkgver.tar.bz2)
up2date=`lynx -dump 'http://prdownloads.sourceforge.net/digikam/?sort_by=date&sort=desc'|grep 'digikamimageplugins-[0-9\.]*tar.bz2$'|sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'`
md5sums=('ff3d2da71168b7e2f166b450fe8053e2')

build()
{
	Fprefix=/opt/kde
	Fbuild
}

