# Compiling Time: 0.28 SBU
# Maintainer: crazy <malagarax@gmx.net>

pkgname=kdepim-docs
origname=kdepim
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Documentation for the KDE PIM Utilities."
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
url="http://www.kde.org"
groups=('kde-extra' 'kde-docs')
archs=('i686')
rodepends=('kdepim=3.5.0')
up2date=$pkgver
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('fb98ea719990bf1169289ca5012ad75ac261f488')

build ()
{
        Fcd $origname-$pkgver
        rm -rf AUTHORS COPYING INSTALL README*
        Fbuild DO_NOT_COMPILE=" \
              indexlib kabc kdgantt kgantt kmailcvt kmobile kode ktnef libemailfunctions libkdenetwork \
              libkholidays libkmime libkpgp libksieve mimelib networkstatus libkcal certmanager korn \
              kfile-plugins libkdepim kpilot ksync libkpimidentities libkpimexchange konsolekalendar \
              knotes knode kitchensync kioslaves kandy kalarm akregator kmail korganizer kaddressbook \
              plugins kresources wizards karm kontact"

}
# vim: ft=sh

# optimalization OK
