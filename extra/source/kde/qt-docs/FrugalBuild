pkgname=qt-docs
pkgver=3.3.5
pkgrel=1
realpkg=qt-x11-free-$pkgver
pkgdesc="Documentation for the QT GUI toolkit."
url="http://www.trolltech.com/products/qt"
rodepends=('qt=3.3.5') 
groups=('kde-extra' 'kde-docs')
archs=('i686')
up2date=$pkgver
source=(ftp://ftp.trolltech.com/qt/source/$realpkg.tar.bz2)
sha1sums=('8d7c7ba0b39848ef60b3cd93fc1d33e9527694ec')

build()
{
	Fcd $realpkg
	Fmkdir /usr/share/doc/qt-$pkgver
	rm -f FAQ INSTALL MANIFEST README README-QT.TXT
	cp -r doc/html $Fdestdir/usr/share/doc/qt-$pkgver
}

# vim: ft=sh

# optimalization OK
