pkgname=kdeaccessibility-docs
origname=kdeaccessibility
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Documentation for the KDE accessibility programs."
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
url="http://www.kde.org"
groups=('kde-extra' 'kde-docs')
archs=('i686')
rodepends=('kdeaccessibility=3.5.0')
up2date=$pkgver
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('40f7b4493bc4e07dcd74d58df41d775ad0c2ddee')

build() 
{

               Fcd $origname-$pkgver
               rm -rf AUTHORS COPYING INSTALL README*
               Fbuild DO_NOT_COMPILE="IconThemes  kbstateapplet kmag kmousetool kmouth ksayit kttsd"

                   
}

