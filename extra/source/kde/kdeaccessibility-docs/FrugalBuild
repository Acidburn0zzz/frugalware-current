pkgname=kdeaccessibility-docs
origname=kdeaccessibility
pkgver=3.5.3
pkgrel=1
pkgdesc="Documentation for the KDE accessibility programs."
groups=('kde-extra' 'kde-docs')
archs=('i686' 'x86_64')
rodepends=('kdeaccessibility>=3.5.3')
makedepends=('kdelibs>=3.5.3')
Finclude kde
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('267c280558780c198d6c429be7e157cb246f2709')

build() 
{

    Fcd $origname-$pkgver
    Fbuild DO_NOT_COMPILE="IconThemes  kbstateapplet kmag kmousetool kmouth ksayit kttsd"
}

