pkgname=knetload
pkgver=3.0_alpha1
pkgrel=1
pkgdesc="A Network Load Monitor applet for Kicker"
url="http://wwwstud.dsi.unive.it/~dpetteno/kdeapps.xhtml#knetload"
depends=('kdelibs')
groups=('kde-extra')
up2date=`lynx -dump http://wwwstud.dsi.unive.it/~dpetteno/downloads.xhtml#knetload|grep ' KNetLoad' |sed -n 's/.*d \(.*\) -.*/\1/;1 p'`
source=(http://digilander.libero.it/dgp85/files/$pkgname-$pkgver.tar.bz2)
md5sums=('6ae466b269b472d25f59a0ac91b78a28')

build()
{
	Fprefix=/opt/kde
	Fbuild || return 1
}

