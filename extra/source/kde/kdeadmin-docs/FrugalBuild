pkgname=kdeadmin-docs
origname=kdeadmin
pkgver=3.5.1
pkgrel=1
pkgdesc="Documentation for the KDE Administration Programs."
groups=('kde-extra' 'kde-docs')
archs=('i686')
rodepends=('kdeadmin>=3.5.1')
Finclude kde
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('2197e764a806e93a8f593717694d999483a92bfd')

build() 
{
        Fcd $origname-$pkgver
	Fbuild DO_NOT_COMPILE="ksysv lilo-config kcron kfile-plugins kdat knetworkconf kpackage kuser secpolicy" 
}

# vim: ft=sh

# optimalization OK
