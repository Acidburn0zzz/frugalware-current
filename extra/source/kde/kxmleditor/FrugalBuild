pkgname=kxmleditor
pkgver=1.1.4
pkgrel=1
pkgdesc="KXML Editor is application for KDE, that display and edit contents of XML file."
url="http://kxmleditor.sourceforge.net/"
license="GPL2"
install=$pkgname.install
up2date="lynx -dump $url/index.orig.html | grep 'Current version' | cut -d ' ' -f 6"
source=(http://dl.sourceforge.net/sourceforge/$pkgname/$pkgname-$pkgver.tar.gz)
md5sums=('d2329e06393c78f22531b8ba42e67d9b')
groups=('kde-extra')
archs=('i686' 'x86_64')
depends=('qt' 'kdelibs')

# optimalization OK

