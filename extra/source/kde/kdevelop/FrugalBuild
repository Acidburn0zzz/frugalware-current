pkgname=kdevelop
pkgver=3.3.0
pkgrel=1
kdever=3.5
pkgdesc="The KDevelop integrated C/C++ application development environment"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
url="http://www.kde.org"
groups=('kde-extra' 'kde-core')
archs=('i686')
depends=('kdebase=3.5.0' 'kdesdk=3.5.0' 'gettext' 'make' 'flex' 'automake' 'autoconf')
makedepends=('cvs' 'graphviz')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('ca3b8c2985ea6c45c48dfc3e7e21d68fcd5f95b9')

build()
{
  
	Fbuild  CXXFLAGS="$CXXFLAGS -Wno-deprecated" \
                --enable-new-ldflags \
                --with-gnu-ld \
                --enable-vba \
                --disable-dependency-tracking \
               DO_NOT_COMPILE="doc" 
                 
	
}

