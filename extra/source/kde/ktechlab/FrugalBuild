pkgname=ktechlab
pkgver=0.3
pkgrel=1
pkgdesc="KTechlab provides a powerful environment for electronic circuits and microcontrollers."
url="http://ktechlab.org"
license="GPL"
depends=('gpsim' 'qt' 'kdelibs' 'gputils')
archs=('i686')
groups=('kde-extra')
up2date="lynx -dump $url/download|grep $pkgname-[0-9\.]*\.tar.gz|sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=($url/download/$pkgname-$pkgver.tar.gz)
sha1sums=('945cc5b985d227e9b7bf8bca4fe86e03900ce465')

build()
{
	Fbuild
}

