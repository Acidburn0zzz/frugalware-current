# Last Modified: Wed, 30 Nov 2005 13:22:12 +0100
# Compiling Time: 0.20 SBU
# Maintainer: crazy <malagarax@gmx.net>

pkgname=kdegraphics-docs
origname=kdegraphics
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Documentation for the KDE Graphics Programs"
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
groups=('kde-extra' 'kde-docs')
archs=('i686')
depends=('kdegraphics=3.5.0')
up2date=$pkgver
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('ca693ae50a9392f7a8cddcc7d16ff0931d537e9a')

build()
{
	
        Fcd $origname-$pkgver
        rm -f AUTHORS COPYING INSTALL README*
	Fbuild DO_NOT_COMPILE="kamera kcoloredit kfax kgamma kghostview kiconedit kmrml kolourpaint \
               kpdf kpovmodeler kruler ksnapshot ksvg kuickshow kview kviewshell libkscan kfile-plugins \
               kfaxview kdvi kooka"
                 
	
}

# vim: ft=sh

# optimalization OK
