# Maintainer: crazy <malagarax@gmx.net>

pkgname=kdesdk-docs
origname=kdesdk
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Documentation for the KDE Software Development Kit."
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
url="http://www.kde.org"
groups=('kde-extra' 'kde-docs')
archs=('i686')
rodepends=('kdesdk=3.5.0')
up2date=$pkgver
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('aac8b23f75786f17229d6eee8d329cef38c8d6b5')


build() 
{

        Fcd $origname-$pkgver
        rm -f AUTHORS COPYING INSTALL README*
	Fbuild DO_NOT_COMPILE=" \
                  cervisia kapptemplate kbabel kbugbuster kcachegrind kdeaccounts-plugin \
                  kfile-plugins kioslave kmtrace kompare kprofilemethod kspy kstartperf kuiviewer \
                  kunittest poxml scheck scripts umbrello"
           
}

# vim: ft=sh

# optimalization OK
