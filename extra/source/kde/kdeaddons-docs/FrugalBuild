# Last Modified: Wed, 30 Nov 2005 12:59:07 +0100
# Compiling Time: 0.16 SBU
# Maintainer: crazy <malagarax@gmx.net>

pkgname=kdeaddons-docs
origname=kdeaddons
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Documentation for the Kdeaddons package."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
groups=('kde-extra' 'kde-docs')
archs=('i686')
rodepends=('kdeaddons=3.5.0')
up2date=$pkgver
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('c3ccd6d54f46c8164480a17956e11033908d03ff')

build() 
{

        Fcd $origname-$pkgver
        rm -rf AUTHORS COPYING INSTALL README*
	Fbuild DO_NOT_COMPILE=" \
                 atlantikdesigner kaddressbook-plugins kate kfile-plugins kicker-applets \
                 knewsticker-scripts konq-plugins ksig noatun-plugins renamedlgplugins"
                
}

# vim: ft=sh

# optimalization OK
