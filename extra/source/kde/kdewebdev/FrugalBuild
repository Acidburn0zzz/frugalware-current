# Compiling Time: 2.38 SBU
# Maintainer: crazy <malagarax@gmx.net>

pkgname=kdewebdev
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="A web development tool for the K Desktop Environment."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
groups=('kde-extra' 'kde-core')
archs=('i686')
depends=('kdebase=3.5.0' 'kdesdk=3.5.0' 'libxslt' 'libxml2')
replaces=('quanta')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('d04355047631741a8b068df8893d70d4d5cd3668')

build() 
{
	Fbuild CXXFLAGS="$CXXFLAGS -Wno-deprecated" \
               --disable-debug \
               --disable-dependency-tracking \
               --enable-new-ldflags \
               --with-gnu-ld \
              DO_NOT_COMPILE="doc" 
               
}

# vim: ft=sh

# optimalization OK
