pkgname=kdbg
pkgver=2.0.1
pkgrel=1
pkgdesc="A Graphical Debugger Interface to gdb"
url="http://members.nextra.at/johsixt/kdbg.html"
depends=('gdb' 'kdelibs')
groups=('kde-extra')
archs=('i686')
Finclude sourceforge
sha1sums=('2865ed99eb54f3100712277fa1c55b598ac83f2b')

build()
{
	Fsed "head -1" "head -n 1" configure*
	make -f admin/Makefile.common || return 1
	Fbuild
}

