pkgname=koffice
pkgver=1.4.1
realver=1.4.1
pkgrel=2
pkgdesc="An integrated Office suite for KDE"
url="http://www.koffice.org"
depends=('kdebase' 'aspell')
makedepends=('esd')
groups=('kde-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url |grep current|sed 's/.*e \(.*\) i.*/\1/'"
source=(ftp://ftp.kde.org/pub/kde/stable/$pkgname-$pkgver/src/$pkgname-$realver.tar.bz2)
# http://www.koffice.org/releases/$pkgver-release.php
md5sums=('91a7b68757addc5934c5a8209a4926d5')

