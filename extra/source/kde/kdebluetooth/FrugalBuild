pkgname=kdebluetooth
pkgver=1.0_beta1
pkgrel=1
pkgdesc="KDE Bluetooth Framework"
url="http://kde-bluetooth.sf.net/"
depends=('glibc' 'dbus>=0.23.3' 'bluez-libs=2.21' 'openobex>=1.0.0')
groups=('kde-extra')
archs=('i686')
up2date=`lynx -dump http://sourceforge.net/project/showfiles.php?group_id=89888|grep "kdebluetooth-"|sed -n 's/.*-\(.*\).t.*/\1/;1 p'`
source=(http://switch.dl.sourceforge.net/sourceforge/kde-bluetooth/$pkgname-$pkgver.tar.bz2)
sha1sums=('58787b1d11ec98b1de52ebbe1f236218a78f5c13')

build()
{
	Fbuild || return 1
}

