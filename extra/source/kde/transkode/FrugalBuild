pkgname=transkode
pkgver=0.5
pkgrel=1
pkgdesc="KDE/Qt based frontend for varios audio transcoding tools."
url="http://transkode.sourceforge.net/"
depends=('kdelibs>=3.5.2' 'taglib')
rodepends=('mplayer' 'wavpack' 'speex' 'libmpcdec' 'faad2' 'flac' 'gstreamer')
options=('scriptlet')
groups=('kde-extra')
archs=('i686')
_F_sourceforge_prefix='transkode-'
_F_sourceforge_ext=".tar.bz2"
Finclude sourceforge
sha1sums=('e3678980d73fcf106aeed1568011a68c665fdb5f')

build()
{
	Fcd $pkgname
	Fbuild --disable-debug --without-debug --with-gnu-ld
}

# optimization OK
