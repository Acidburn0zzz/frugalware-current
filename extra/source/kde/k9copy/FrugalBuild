pkgname=k9copy
pkgver=1.0.4_2
pkgrel=1
pkgdesc="K9Copy is a small utility which allows the copy of DVD under Linux."
url="http://k9copy.sourceforge.net/index.php"
depends=('kdelibs' 'dvdauthor' 'mjpegtools' 'libdvdread' 'dvd+rw-tools' 'toolame' 'vamps')
groups=('kde-extra')
archs=('i686' 'x86_64')
options=('scriptlet')
Finclude sourceforge

build()
{
	Fcd $pkgname-${pkgver//_/-}
	Fbuild
}

# optimization OK

sha1sums=('89b201b9fca17f1d4c9c171416d1ccbd92c610a4')
