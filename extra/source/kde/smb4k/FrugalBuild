pkgname=smb4k
pkgver=0.7.1
pkgrel=1
pkgdesc="Smb4K is a SMB/CIFS share browser for KDE."
url="http://smb4k.berlios.de/"
depends=('kdebase')
rodepends=('samba')
groups=('kde-extra' 'kde-apps')
archs=('i686' 'x86_64')
up2date="lynx -dump http://smb4k.berlios.de/download.html |grep smb4k.*tar |sed -n -e 's/.*k-\(.*\).t.*/\1/;1 p'"
source=(http://download.berlios.de/smb4k/$pkgname-$pkgver.tar.bz2)
sha1sums=('778e714367edc4b71931336c922de735adeb6235')
Finclude kdeapps

