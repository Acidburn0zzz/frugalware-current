pkgname=mtaskbar
pkgver=0.7
pkgrel=1
pkgdesc="A modified TaskBar for KDE"
url="http://www.uni-weimar.de/~wolff3/"
depends=('kdebase')
groups=('kde-extra')
archs=('i686')
up2date="lynx -dump http://www.uni-weimar.de/~wolff3/software/taskbar.html | grep 'latest' | grep -o '\([0-9]\+\.[0-9\.]*\)'"
source=(http://www.uni-weimar.de/~wolff3/kdelook/mtaskbar-0.7.tar.bz2)
sha1sums=('b94b75ba9b1108db7f496303395d9aa0b9145f90')

build () 
{

    Fcd $pkgname-$pkgver/$pkgname
    Fbuild
}         
