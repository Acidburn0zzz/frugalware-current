# Last Modified: Wed, 30 Nov 2005 13:16:22 +0100
# Compiling Time: 0.23 SBU
# Maintainer: crazy <malagarax@gmx.net>

pkgname=kdeedu-docs
origname=kdeedu
pkgver=3.5.0
pkgrel=1
kdever=3.5
url="http://www.kde.org"
pkgdesc="Documentation for the KDE Educational programs."
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
groups=('kde-extra' 'kde-docs')
archs=('i686')
rodepends=('kdeedu=3.5.0')
up2date=$pkgver
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('8708afafae3d482ced966a6a2ea7fa6dcfede3b0')

build()
{
	Fcd $origname-$pkgver
        rm -rf AUTHORS COPYING INSTALL README*
	Fbuild DO_NOT_COMPILE=" \
                 libkdeedu applnk blinken kalzium kanagram kbruch \
                 keduca kgeography khangman kig kiten klatin klettres kmplot \
                 kpercentage kstars ktouch kturtle kverbos kvoctrain kwordquiz"
 
}

# vim: ft=sh

# optimalization OK
