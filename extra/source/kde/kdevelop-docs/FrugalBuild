# Last Modified: Wed, 30 Nov 2005 13:42:24 +0100
# Compiling Time: 0.34 SBU
# Maintainer: crazy <malagarax@gmx.net>

pkgname=kdevelop-docs
origname=kdevelop
pkgver=3.3.0
pkgrel=1
kdever=3.5
pkgdesc="Documentation for the KDevelop package."
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
url="http://www.kde.org"
groups=('kde-extra' 'kde-docs')
archs=('i686')
rodepends=('kdevelop=3.3.0')
up2date=$pkgver
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('ca3b8c2985ea6c45c48dfc3e7e21d68fcd5f95b9')

build()
{
        Fcd $origname-$pkgver
        rm -rf AUTHORS COPYING INSTALL README*
	Fbuild DO_NOT_COMPILE="lib parts buildtools editors embedded kdevdesigner languages mimetypes pics src vcs"         
        Frm /usr/bin
        Frm /usr/share/{applications,desktop-directories,mimelnk}        	
}

# vim: ft=sh

# optimalization OK

