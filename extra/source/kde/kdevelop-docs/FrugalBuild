# Last Modified: Mon, 06 Mar 2006 02:39:41 +0100
# Compiling Time: 0.34 SBU
# Maintainer: crazy <crazy@frugalware.org>

pkgname=kdevelop-docs
origname=kdevelop
pkgver=3.3.1
pkgrel=1
pkgdesc="Documentation for the KDevelop package."
groups=('kde-extra' 'kde-docs')
archs=('i686' 'x86_64')
rodepends=('kdevelop>=3.3.1')
makedepends=('kdelibs>=3.5.1')
Finclude kde
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('1551c818ada9f0d362cc72f626f698b8636d947b')

build()
{
        Fcd $origname-$pkgver
	Fbuild DO_NOT_COMPILE="lib parts buildtools editors embedded kdevdesigner languages mimetypes pics src vcs"         
        Frm /usr/bin
        Frm /usr/share/{applications,desktop-directories,mimelnk}        	
}

# vim: ft=sh

# optimalization OK

