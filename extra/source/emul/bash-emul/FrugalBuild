pkgname=bash-emul
pkgver=3.0
pkgrel=6
pkgdesc="The GNU Bourne Again shell for emulation of i686 on x86_64"
url="http://cnswww.cns.cwru.edu/~chet/bash/bashtop.html"
backup=(etc/{profile,shells})
depends=('glibc-emul' 'readline-emul')
groups=('emul-extra')
up2date="lynx -dump $url|grep current|sed -n -e 's/.*bash-\(.*\)\.$/\1/' -e '1 p'"
Finclude emul
archs=('x86_64')
sha1sums=('15cc0ca6c3f5dce155f8a354deeee050cb441347')

