# Last modified: Wed, 25 Jan 2006 20:54:15 +0100
# Compiling time: 0.15 SBU
# Maintainer: CSÉCSY László <boobaa@ajrg.hu>

pkgname=gnome-mag
pkgver=0.12.4
pkgrel=1
pkgdesc="GNOME screen magnifier"
url="http://www.gnome.org/"
depends=('at-spi' 'libxtst')
makedepends=('intltool')
groups=('gnome-extra')
archs=('i686' 'x86_64')
nondevel=1
Finclude gnome
sha1sums=('bddbda064a483942bd842f495a53cbf64f80f516')

build() {
	[ "$GNOMEDIR" = "" ] && source /etc/profile.d/gnome.sh
	Fcd
	Fmake
	Fmakeinstall DESTDIR=$startdir/pkg
}


# optimalization OK
