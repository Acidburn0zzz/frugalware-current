pkgname=ubuntu-artwork
pkgver=26
pkgrel=1
url="https://launchpad.net/distros/ubuntu/dapper/+source/ubuntu-artwork"
pkgdesc="Ubuntu themes and artwork"
depends=()
rodepends=('gnome-icon-theme' 'gtk+2-engines-clearlooks')
makedepends=('intltool' 'icon-naming-utils' 'gnome-common' 'glib2' 'imagemagick')
groups=('gnome-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump 'http://ftp.esat.net/mirrors/archive.ubuntu.com/pool/main/u/ubuntu-artwork/?C=M;O=D'|grep tar.gz|sed -n 's/.*work_\([0-9\.]*\)\..*/\1/;1p'"
source=(http://ftp.esat.net/mirrors/archive.ubuntu.com/pool/main/u/$pkgname/ubuntu-artwork_$pkgver.tar.gz)
sha1sums=('00702e43ae94ce353bc06506d1dc8dfdc5fc6b9b')

build() {
	Fcd
	./autogen.sh || Fdie
	Fbuild
}

# optimization OK - noarch
