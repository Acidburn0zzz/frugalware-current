url="http://cssed.sourceforge.net"
depends=('gtk+2')
groups=('gnome-extra')
archs=('i686')
up2date="lynx -dump $url | grep 'Download now' | sed 's/Download now (//g' | sed 's/)//g' | sed 's/ //g'"
source=(http://heanet.dl.sourceforge.net/$pkgname/$pkgname-$pkgver.tar.gz)
