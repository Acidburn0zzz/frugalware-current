pkgname=zapping
pkgver=0.10cvs4
pkgrel=1
pkgdesc="Zapping is a TV viewer"
url="http://zapping.sourceforge.net"
license="GPL"
depends=('libgnomeui' 'libxxf86vm' 'libxmu' 'rte' 'libxv' 'atk' 'pango' 'zvbi' 'arts' 'libxxf86dga' 'perl-xml')
install=$pkgname.install
archs=('i686')
groups=('gnome-extra')
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=2599|grep $pkgname|grep Source|sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=(http://dl.sourceforge.net/sourceforge/zapping/$pkgname-$pkgver.tar.bz2)
sha1sums=('f05d88fb552465dc0bb4b5835ef2056731a8bf48')

build() 
{
	Fbuild
}

