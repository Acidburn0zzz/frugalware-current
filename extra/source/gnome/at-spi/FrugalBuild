pkgname=at-spi
pkgver=1.6.6
pkgrel=1
pkgdesc="The Gnome Accessibility Toolkit"
url="http://developer.gnome.org/projects/gap/"
depends=('gtk+2' 'gail' 'libbonobo' 'perl-xml')
makedepends=('gtk-doc')
groups=('gnome-extra')
archs=('i686' 'x86_64')
[ -e ../../../../source/gnome/up2date.gnome ] && . ../../../../source/gnome/up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2)
sha1sums=('32b853c9a4460e13c5eca048e5aaad9119d75bc2')

build() {
	Fbuild --enable-gtk-doc
}

