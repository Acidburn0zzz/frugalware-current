# Maintainer: Miklos Nemeth <desco@frugalware.org>

pkgname=rhythmbox
pkgver=0.9.4
pkgrel=1
pkgdesc="A music management application for GNOME"
url="http://www.rhythmbox.org/"
up2date="lynx -dump http://www.rhythmbox.org/news.html|grep 'Rhythmbox [0-9]'|sed -n 's/.*Rhythmbox \([0-9\.]*\).*/\1/;1p'"
depends=('libgnomeui' 'gst-plugins' 'gst-plugins-base' 'nautilus-cd-burner' 'scrollkeeper' 'desktop-file-utils' 'gstreamer' 'totem')
makedepends=('intltool' 'gnome-doc-utils')
groups=('gnome-extra')
archs=('i686' 'x86_64')
install=$pkgname.install
#Finclude gnome
source=(http://ftp.gnome.org/pub/GNOME/sources/rhythmbox/0.9/$pkgname-$pkgver.tar.bz2)

build()
{
	unset MAKEFLAGS
	Fbuild --disable-schemas-install GSTREAMER_0_10=1
}


# optimalization OK
sha1sums=('59654d8f77c28d6f24b3e2f22b13015a4b535555')

# optimization OK
