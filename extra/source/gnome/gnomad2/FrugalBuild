pkgname=gnomad2
pkgver=2.6.3
pkgrel=1
pkgdesc="Gnomad 2 is a GUI built on top of GTK/GNOME 2, id3lib and libnjb that makes it possible to transfer tracks and files from/to a Creative Nomad Jukebox (all brands)."
url="http://gnomad2.sourceforge.net/"
depends=('libgnomeui' 'libid3tag' 'libnjb')
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=65573 | grep -m 1 'Gnomad [0-9]\.[0-9]\.[0-9]' | cut -d ' ' -f 12"
source=(http://dl.sourceforge.net/sourceforge/$pkgname/$pkgname-$pkgver.tar.gz)
md5sums=('a969cb089c63c72ab242d94fc0413e62')
license="GPL2"
groups=(gnome-extra)
archs=('i686')
install=$pkgname.install

