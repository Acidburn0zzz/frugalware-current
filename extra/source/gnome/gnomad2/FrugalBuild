pkgname=gnomad2
pkgver=2.8.2
pkgrel=1
pkgdesc="Gnomad2 transfer tracks/files from/to a Creative Nomad Jukebox (all brands)."
url="http://gnomad2.sourceforge.net/"
depends=('libgnomeui' 'libid3tag' 'libnjb')
makedepends=('perl-xml')
up2date="lynx -dump 'http://sourceforge.net/project/showfiles.php?group_id=65573'|grep -m 1 -o 'Gnomad [0-9\.]\+[0-9]'|grep -o '[0-9\.]\+[0-9]'"
source=(http://dl.sourceforge.net/sourceforge/$pkgname/$pkgname-$pkgver.tar.gz)
sha1sums=('ab5208ab7406b1efdc37f41d0cc5f6bf31f5b5bd')
license="GPL2"
groups=('gnome-extra')
archs=('i686' 'x86_64')
install=$pkgname.install

