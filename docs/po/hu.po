# type: Bullet: '* '
#: quickref.txt:21
msgid "Supported architectures: i686, x86_64"
msgstr "Támogatott architektúrák: i686, x86_64"

# type: Bullet: '* '
#: quickref.txt:21
msgid "Stable releases every 6 months"
msgstr "Stabil kiadások 6 havonta"

# type: Bullet: '* '
#: quickref.txt:21
msgid "Security support for stable releases"
msgstr "Biztonsági frissítések a stabil kiadásokhoz"

# type: Bullet: '* '
#: quickref.txt:21
msgid "Text mode installation"
