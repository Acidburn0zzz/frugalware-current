pkgname=m4
pkgver=1.4.5
pkgrel=1
pkgdesc="An implementation of the traditional Unix macro processor"
url="http://www.gnu.org/software/m4"
depends=('glibc' 'bash')
groups=('devel' 'devel-core')
archs=('i686' 'x86_64' 'ppc')
up2date="lynx -dump 'http://ftp.gnu.org/pub/gnu/m4/?M=D'|grep tar.gz$|sed -n -e 's/.*4-\([0-9\.]*\).tar.gz$/\1/' -e '1 p'"
source=(ftp://ftp.gnu.org/gnu/$pkgname/$pkgname-$pkgver.tar.gz)

# optimization ok

sha1sums=('3af7394e8e464dd851319417d16096d1832a7bfa')
