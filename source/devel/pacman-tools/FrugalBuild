groups=('devel')
up2date=`lynx -dump http://ftp.frugalware.org/pub/other/pacman-tools/|grep tar.gz$|sed -n 's/.*-\(.*\)\.t.*/\1/;$ p'`
source=(http://ftp.frugalware.org/pub/other/$pkgname/$pkgname-$pkgver.tar.gz)
