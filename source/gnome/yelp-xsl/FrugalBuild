pkgname=yelp-xsl
pkgver=3.19.90
pkgrel=1
pkgdesc="TXSL stylesheets for the yelp help browser"
depends=('libxml2>=2.9.3-3' 'libxslt>=1.1.28-3')
makedepends=('itstool>=2.0.2-3' 'intltool>=0.51.0-2')
groups=('gnome')
archs=('i686' 'x86_64' 'arm')
_F_gnome_ext=".tar.xz"
_F_gnome_devel="y"
Finclude gnome
sha1sums=('d2c7443aae7c8f878d7f634055c0c8e6ae6be428')

build() {
	Fcd
	Fsed '$(datadir)/pkgconfig' '$(libdir)/pkgconfig' Makefile.{am,in}
	Fbuild
}
