# Last modified: Fri, 24 Jun 2005 14:33:11 +0200
# Compiling time: ~1 minute
# Maintainer: Laszlo Dvornik <dvornik@gnome.hu>

pkgname=libgsf
pkgver=1.12.1
pkgrel=1
pkgdesc="GNOME Structured File Library"
url="http://www.gnome.org/"
depends=('gnome-vfs')
makedepends=('gtk-doc')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
nondevel=1
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2)
md5sums=('76473db3245f2f384475f2e40c4ca432')

build() {
	Fcd
	Fmake --enable-gtk-doc --with-bz2 --with-gnome --with-zlib || return 1
	Fmakeinstall || return 1
}

# vim: ft=sh
