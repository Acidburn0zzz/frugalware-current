pkgname=libgsf
pkgver=1.14.0
pkgrel=2
pkgdesc="GNOME Structured File Library"
url="http://www.gnome.org/"
depends=('gnome-vfs')
makedepends=('gtk-doc' 'perl-xml')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
nondevel=1
Finclude gnome
options=('scriptlet')
sha1sums=('89ab34725cf00f1380f204415ec547b19225c674')

build()
{
	Fbuild --enable-gtk-doc --with-bz2 --with-gnome --with-zlib
}


