pkgname=gthumb
pkgver=2.7.8
pkgrel=1
pkgdesc="Image browser and viewer for GNOME"
url="http://gthumb.sourceforge.net/"
depends=('libgnomeui' 'libgnomeprintui' 'libgphoto2' 'scrollkeeper' \
	'desktop-file-utils')
makedepends=('intltool')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
_F_gnome_schemas=('/etc/gconf/schemas/gthumb.schemas')
_F_gnome_scrollkeeper="y"
_F_gnome_desktop="y"
Finclude gnome gnome-scriptlet

