# Last modified: Sat, 11 Jun 2005 11:07:25 +0000
# Compiling time: ~1 minute
# Maintainer: Laszlo Dvornik <dvornik@gnome.hu>

pkgname=libgnomeprintui
pkgver=2.10.2
pkgrel=2
pkgdesc="User Interface library for printing with GNOME"
url="http://www.gnome.org/"
depends=('libgnomeprint' 'libgnomecanvas' 'gnome-icon-theme')
makedepends=('gtk-doc')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2)
md5sums=('01fce7918f4e106e00ee8b5447783e4c')

build() {
	Fcd
	Fmake --enable-gtk-doc || return 1
	Fmakeinstall || return 1
}

# vim: ft=sh
