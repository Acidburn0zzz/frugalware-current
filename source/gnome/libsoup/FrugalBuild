pkgname=libsoup
pkgver=2.2.95.1
pkgrel=1
pkgdesc="An HTTP library implementation in C"
url="http://www.gnome.org/"
depends=('glib2' 'gnutls' 'libxml2' 'libgcrypt')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
options=('scriptlet')
sha1sums=('92efa4d9969af992e1f0775891fe8a353e529edb')

build() {
	Fbuild --enable-ssl --enable-libgpg-error
}

