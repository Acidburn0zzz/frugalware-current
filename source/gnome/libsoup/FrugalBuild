pkgname=libsoup
pkgver=2.2.92
pkgrel=1
pkgdesc="An HTTP library implementation in C"
url="http://www.gnome.org/"
depends=('glib2' 'gnutls' 'libxml2' 'libgcrypt')
makedepends=('gtk-doc')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
options=('scriptlet')
sha1sums=('4b1f392c5b8dcd357ce0688495becdbaa76879a0')

build() {
	Fbuild --enable-ssl --enable-gtk-doc --enable-libgpg-error
}

# optimalization OK
