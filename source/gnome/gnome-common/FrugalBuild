pkgname=gnome-common
pkgver=2.12.0
pkgrel=2
pkgdesc="Common macros for GNOME"
url="http://www.gnome.org/"
depends=('scrollkeeper')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
source=(${source[@]} gnome.sh use-automake-110.patch)
sha1sums=('5483bad8bcded040946f776c655507caf05946af' \
          'fb0a0d87d507a20c32ec2af9d0f2bda57f28ffaa' \
	  'fa7f9941ac915b255ab0748bdae40f99b7f4dea0')

build() {
	Fbuild
	Fexe /etc/profile.d/gnome.sh
}
