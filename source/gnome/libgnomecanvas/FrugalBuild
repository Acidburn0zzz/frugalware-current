pkgname=libgnomecanvas
pkgver=2.10.2
pkgrel=2
pkgdesc="GNOME canvas"
url="http://www.gnome.org/"
depends=('libglade' 'libart_lgpl')
makedepends=('gtk-doc')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2)
md5sums=('976eada7b499ae6e74966f8888c015d1')

build() {
	Fbuild --enable-gtk-doc || return 1
}

