pkgrel=2
pkgdesc="GNOME session manager"
depends=('libxml2>=2.7.8' 'gnome-desktop>=2.91.0' 'polkit' 'upower' 'gconf-gtk'  'usermode' \
	'dbus-glib>=0.76' 'gnome-control-center>=2.91.0' 'libnotify' 'gnome-keyring>=2.91.0' \
	'metacity')
makedepends=('intltool' 'gnome-common')
#TODO remove gnome-shell rodepends only help for update gnome2 to gnome3
rodepends=('consolekit-x11' 'gnome-shell')
groups=('gnome')
archs=('i686' 'x86_64' 'ppc')
