pkgname=gtk+2-engines
realname=gtk-engines
pkgver=2.6.3
pkgrel=4
pkgdesc="Theme engines for GTK+2"
url="http://www.gnome.org/"
depends=('gtk+2>=2.6.0')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/gtk-engines/$preup2date/gtk-engines-$pkgver.tar.bz2)
md5sums=('ba5975f8ab390fa43fc0bf94f4a3b023')

build()
{
	Fcd gtk-engines-$pkgver

	Fbuild --disable-clearlooks --disable-industrial || return 1
}

# vim: ft=sh

# optimalization OK
