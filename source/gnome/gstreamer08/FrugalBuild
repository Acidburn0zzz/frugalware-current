pkgname=gstreamer08
pkgver=0.8.11
pkgrel=1
pkgdesc="GNOME Multimedia Framework"
url="http://gstreamer.freedesktop.org/"
depends=('libgnomeui')
makedepends=('python' 'gtk-doc' 'pyxml')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
options=('scriptlet')
sha1sums=('945b08e68f31f99621ce7c35d7ec3b7ac3e49b1e')

build() {
	Fbuild --disable-tests --disable-debug --disable-docbook \
		--disable-examples --enable-fast-stack-trash \
		--disable-docs-build --with-package-name='Frugalware gstreamer package'
}

