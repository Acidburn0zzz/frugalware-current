pkgname=file-roller
pkgver=2.28.2
pkgrel=1
pkgdesc="Archive manager utility for GNOME"
depends=('nautilus>=2.28.2' 'rarian' 'desktop-file-utils')
makedepends=('intltool' 'gnome-doc-utils>=0.16.0')
groups=('gnome' 'gnome-minimal')
archs=('i686' 'x86_64' 'ppc')
_F_gnome_schemas=('/etc/gconf/schemas/file-roller.schemas')
_F_gnome_scrollkeeper="y"
_F_gnome_desktop="y"
_F_gnome_iconcache="y"
Finclude gnome gnome-scriptlet
sha1sums=('e2a1535b1f8d76db95521b95d8efc9b596be5999')

build(){
	Fcd
	Fautoreconf
	Fbuild
	Fbuild_gnome_scriptlet
}

# optimization OK
