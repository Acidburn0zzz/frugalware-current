pkgname=file-roller
pkgver=2.30.1.1
pkgrel=1
pkgdesc="Archive manager utility for GNOME"
depends=('nautilus>=2.30.0-2' 'rarian' 'desktop-file-utils')
makedepends=('intltool' 'gnome-doc-utils>=0.16.0')
groups=('gnome' 'gnome-minimal')
archs=('i686' 'x86_64' 'ppc')
_F_gnome_schemas=('/etc/gconf/schemas/file-roller.schemas')
_F_gnome_scrollkeeper="y"
_F_gnome_desktop="y"
_F_gnome_iconcache="y"
Finclude gnome gnome-scriptlet
sha1sums=('256d86b66fcb671d7bd23b797bd16ba013558698')
Fconfopts="$Fconfopts --disable-packagekit"

build(){
	Fcd
	Fautoreconf
	Fbuild
	Fbuild_gnome_scriptlet
}

# optimization OK
