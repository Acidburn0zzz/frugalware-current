pkgname=nautilus
pkgver=2.14.1
pkgrel=2
pkgdesc="A file manager for GNOME"
url="http://www.gnome.org/"
depends=('eel' 'librsvg' 'libexif' 'eject' 'desktop-file-utils' 'libbeagle>=0.2.6-3')
makedepends=('intltool')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
install=$pkgname.install
Finclude gnome
sha1sums=('5267c76782190796743b0647025ef9079eee7f78')

build() {
	Fcd
	Fmake --disable-schemas-install --disable-update-mimedb --enable-beagle
	Fmakeinstall GCONF_DISABLE_SCHEMA_INSTALL=1
}

# optimization OK
