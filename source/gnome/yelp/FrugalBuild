pkgname=yelp
pkgver=2.15.5
pkgrel=1
pkgdesc="The default help browser for GNOME"
url="http://www.gnome.org/"
depends=('libgnomeui' 'libxslt' 'firefox>=1.5.0.4' 'libxml2' 'gtk+2' 'startup-notification' 'libbonobo')
makedepends=('intltool' 'gnome-doc-utils' 'libgnomeprintui' 'libglade' 'gnome-vfs>=2.15.90')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
_F_gnome_schemas=('/etc/gconf/schemas/yelp.schemas')
Finclude gnome gnome-scriptlet
Fconfopts="$Fconfopts --enable-man --enable-info --with-mozilla=firefox"
unset MAKEFLAGS
sha1sums=('46d9deae62401c325e655bdab08cd32bf98eca40')

