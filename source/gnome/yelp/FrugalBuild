pkgname=yelp
pkgver=2.20.0
pkgrel=1
pkgdesc="The default help browser for GNOME"
url="http://www.gnome.org/"
depends=('libgnomeui>=2.20.0' 'libxslt' 'xulrunner=1.8.1.3' \
	'libxml2' 'gtk+2>=2.12.0' 'startup-notification' 'libbonobo>=2.20.0')
makedepends=('intltool' 'gnome-doc-utils' \
	'libgnomeprintui>=2.20.0' 'libglade' 'gnome-vfs>=2.20.0')
groups=('gnome' 'gnome-minimal')
archs=('i686' 'x86_64')
_F_gnome_schemas=('/etc/gconf/schemas/yelp.schemas')
Finclude gnome gnome-scriptlet
Fconfopts="$Fconfopts --enable-man --enable-info --with-mozilla=xulrunner"
unset MAKEFLAGS
sha1sums=('29c99793174c24f86ad34373ed5798b534aaac9e')

