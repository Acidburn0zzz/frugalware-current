pkgname=yelp
pkgver=2.18.0
pkgrel=2
pkgdesc="The default help browser for GNOME"
url="http://www.gnome.org/"
depends=('libgnomeui>=2.18.0' 'libxslt' 'firefox=2.0.0.3' \
	'libxml2' 'gtk+2>=2.10.10' 'startup-notification' 'libbonobo>=2.18.0')
makedepends=('intltool' 'gnome-doc-utils' \
	'libgnomeprintui>=2.18.0' 'libglade' 'gnome-vfs>=2.18.0.1')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
_F_gnome_schemas=('/etc/gconf/schemas/yelp.schemas')
Finclude gnome gnome-scriptlet
Fconfopts="$Fconfopts --enable-man --enable-info --with-mozilla=firefox"
unset MAKEFLAGS

sha1sums=('2324c5313605edca4a7adf0709483ebb0c1434a0')
# optimization OK
