pkgname=yelp
pkgver=2.16.0
pkgrel=2
pkgdesc="The default help browser for GNOME"
url="http://www.gnome.org/"
depends=('libgnomeui>=2.16.0-2' 'libxslt' 'firefox>=1.5.0.6' 'libxml2' 'gtk+2' 'startup-notification' 'libbonobo')
makedepends=('intltool' 'gnome-doc-utils' 'libgnomeprintui>=2.12.1-3' 'libglade' 'gnome-vfs>=2.16.0')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
_F_gnome_schemas=('/etc/gconf/schemas/yelp.schemas')
Finclude gnome gnome-scriptlet
Fconfopts="$Fconfopts --enable-man --enable-info --with-mozilla=firefox"
unset MAKEFLAGS
sha1sums=('ef0bd0f1c45c15fc4c31dd66ba770779d294c069')

