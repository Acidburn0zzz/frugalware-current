pkgname=yelp
pkgver=2.14.2
pkgrel=1
pkgdesc="The default help browser for GNOME"
url="http://www.gnome.org/"
depends=('libgnomeui' 'libxslt' 'firefox>=1.5.0.4' 'libxml2' 'gtk+2' 'startup-notification')
makedepends=('intltool' 'gnome-doc-utils' 'libgnomeprintui' 'libglade' 'gnome-vfs')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
_F_gnome_schemas=('/etc/gconf/schemas/yelp.schemas')
Finclude gnome gnome-scriptlet
Fconfopts="$Fconfopts --enable-man --enable-info --with-mozilla=firefox"
unset MAKEFLAGS
# optimization OK
sha1sums=('7cc81e80825425d8344892db2beda82ccd28293e')
