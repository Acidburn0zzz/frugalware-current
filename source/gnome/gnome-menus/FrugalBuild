pkgname=gnome-menus
pkgver=2.13.5
pkgrel=1
pkgdesc="GNOME menus"
url="http://www.gnome.org/"
depends=('gnome-vfs' 'gamin')
makedepends=('intltool' 'python')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
nondevel=1
Finclude gnome
sha1sums=('962b5963eff7fbfcd8de843ef4016dda52d23066')

build() {
	Fbuild --sysconfdir=/etc/gnome --enable-python
}

