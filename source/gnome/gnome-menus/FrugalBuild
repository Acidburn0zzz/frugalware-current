pkgname=gnome-menus
pkgver=2.15.4.1
pkgrel=1
pkgdesc="GNOME menus"
url="http://www.gnome.org/"
depends=('gnome-vfs>=2.15.3' 'gamin')
makedepends=('intltool' 'python')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
sha1sums=('6ddbf8d283ab6563d6c14da32a1c9af11088ef77')

build() {
	Fbuild --sysconfdir=/etc/gnome --enable-python
}

# optimization OK
