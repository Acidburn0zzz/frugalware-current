pkgname=gnome-menus
pkgver=2.20.0
pkgrel=1
pkgdesc="GNOME menus and menu editor"
url="http://www.gnome.org/"
depends=('gamin' 'pygtk>=2.12.0')
makedepends=('intltool')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
Fsysconfdir="/etc/gnome"
Fconfopts="$Fconfopts --enable-python --enable-debug=no --enable-inotify"
source=(${source[@]} $pkgname-2.18.0-categorize-prefs-menu.patch)
sha1sums=('ba78e3cc9dd5ae13bc89858362cd5c831339235d' \
          '16d2d01801f13b0aa827371cf89a2e9fb7030c08')

# optimization OK
