pkgname=gnome-menus
pkgver=2.14.0
pkgrel=1
pkgdesc="GNOME menus"
url="http://www.gnome.org/"
depends=('gnome-vfs' 'gamin')
makedepends=('intltool' 'python')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
sha1sums=('faef5d654df39cc2174c8f38e20ad165bb65d286')

build() {
	Fbuild --sysconfdir=/etc/gnome --enable-python
}

# optimalization OK
