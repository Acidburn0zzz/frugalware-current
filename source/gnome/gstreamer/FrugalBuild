pkgname=gstreamer
pkgver=0.10.4
pkgrel=1
pkgdesc="GNOME Multimedia Framework"
url="http://gstreamer.freedesktop.org/"
depends=('libgnomeui')
makedepends=('python' 'gtk-doc' 'pyxml')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
options=('scriptlet')
sha1sums=('334d5ed48eec5ef83bbd738c0e1d79ab750902db')

build() {
	Fbuild --disable-tests --disable-debug --enable-gtk-doc --disable-docbook \
		--disable-examples --enable-fast-stack-trash \
		--with-package-name='Frugalware gstreamer package'
}

# optimalization OK
