pkgname=evolution-exchange
pkgver=2.4.0
pkgrel=1
pkgdesc="Ximian Connector for Microsoft Exchange, which adds support for Microsoft Exchange 2000 and 2003 to Evolution."
url="http:////www.gnome.org/projects/evolution/"
depends=('gtkhtml' 'mono' 'evolution-data-server' 'evolution')
makedepends=('gtk-doc')
groups=('gnome')
archs=('x86_64')
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2)
sha1sums=('d9841fae4899842edf3009d26e6a6ef7d818b52e')

build() {
	Fbuild --enable-gtk-doc
}

