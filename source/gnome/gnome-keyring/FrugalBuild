pkgname=gnome-keyring
pkgver=3.1.92
pkgrel=3
pkgdesc="Password and keyring managing daemon for GNOME"
url="http://www.gnome.org/"
depends=('libxml2>=2.7.8' 'gtk+3' 'dbus-glib>=0.76-1' 'libtasn1>=1.4' 'gconf' 'pam' 'libgnome-keyring>=3.1.92' 'p11-kit' 'libcap')
makedepends=('intltool' 'gtk-doc>=1.9')
groups=('gnome')
archs=('i686' 'x86_64' 'ppc')
_F_gnome_iconcache="y"
_F_gnome_mime="y"
_F_gnome_glib="y"
Finclude gnome gnome-scriptlet
Fconfopts="$Fconfopts --disable-gtk-doc --with-pam-dir=/lib/security --with-gtk=3.0 --disable-update-mime --enable-pam"
sha1sums=('072cfb1eeef2205e404f8dbb9f1d0fdc12cb425e')

build() {
	Fcd
	Fautoreconf
	Fbuild
	Fbuild_gnome_scriptlet
}

# optimization OK
