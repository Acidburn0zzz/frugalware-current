pkgname=libwnck
pkgver=2.14.2
pkgrel=1
pkgdesc="Window Navigator Construction Kit"
url="http://www.gnome.org/"
depends=('gtk+2' 'atk' 'libsm' 'pango' 'libxres' 'startup-notification')
groups=('gnome' 'gnome-core')
makedepends=('gtk-doc' 'perl-xml')
archs=('i686' 'x86_64')
Finclude gnome
options=('scriptlet')
sha1sums=('8600141d8829357808780e1bbd888b5c227ae501')

build()
{
	if [ "$CARCH" == "x86_64" ]; then
	    Fbuild
	else
	    Fbuild --enable-gtk-doc
	fi
}

# optimization OK
