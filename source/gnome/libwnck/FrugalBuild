pkgname=libwnck
pkgver=2.10.0
pkgrel=2
pkgdesc="Window Navigator Construction Kit"
url="http://www.gnome.org/"
depends=('gtk+2' 'startup-notification')
groups=('gnome' 'gnome-core')
makedepends=('gtk-doc')
archs=('i686' 'x86_64')
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2)
md5sums=('c04c79f1e8576bc930c79d2e7bb8190a')

build() {
	Fbuild --enable-gtk-doc || return 1
}

