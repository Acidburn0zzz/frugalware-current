pkgver=2.15.91
pkgrel=1
pkgdesc="An image viewing and cataloging program for GNOME"
url="http://www.gnome.org/"
depends=('libexif' 'libgnomeui' 'scrollkeeper' 'desktop-file-utils' \
	 'libgnomeprintui' 'gnome-desktop')
