pkgname=nautilus-sendto
pkgver=2.28.4
pkgrel=4
pkgdesc="A plugin to send files to other apps from Nautilus."
url="http://www.gnome.org/"
depends=('nautilus>=2.30.0-2' 'evolution-data-server>=2.30.2')
makedepends=('intltool' 'perl-xml-parser' 'pidgin>=2.5.6-2' 'claws-mail>=3.7.1-3' 'krb5' 'evolution')
groups=('gnome')
archs=('i686' 'x86_64' 'ppc')
_F_gnome_iconcache="y"
_F_gnome_schemas=('/etc/gconf/schemas/nst.schemas')
Finclude gnome gnome-scriptlet
sha1sums=('da97f886a1e400c9e55ac57d01bddfae0362cb4f')

build() {
	Fbuild
	Frm usr/lib/nautilus-sendto/plugins/libnstbluetooth.*
	Fbuild_gnome_scriptlet
}

# optimization OK
