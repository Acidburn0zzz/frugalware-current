pkgname=evolution-sharp
pkgver=0.12.2
pkgrel=1
pkgdesc="Mono bindings for evolution."
url="http://www.gnome.org"
depends=('gtk2-sharp>=2.10.0' 'atk' 'libsm' 'evolution>=2.7.92' 'gnome-sharp>=2.16.0')
makedepends=('pkgconfig')
Finclude gnome mono
groups=('gnome')
archs=('i686' 'x86_64')

build() {
	Fcd
	Fmonoexport
	Fpatchall
	Fautoreconf
	Fmake
	Fmakeinstall
	Fmonocleanup
}

sha1sums=('a630a07e8cb8275ca1f5d18d235857a47bef0863')
