pkgname=gnome-screenshot
pkgver=3.3.1
pkgrel=1
pkgdesc="GNOME screenshot utilities"
url="http://www.gnome.org/"
depends=('libxext' 'gtk+3' 'libcanberra-gtk')
makedepends=('intltool')
rodepends=('gnome-utils')
groups=('gnome')
archs=('i686' 'x86_64' 'ppc')
_F_gnome_desktop="y"
_F_gnome_glib="y"
Finclude gnome gnome-scriptlet
sha1sums=('fad4d07d93ff1e5dc9a99311c882d3a56ef00daf')

build() {
        Fbuild
        Fbuild_gnome_scriptlet
}

# optimization OK
