# Last modified: Mon, 13 Jun 2005 19:05:47 +0000
# Compiling time: ~1 minute
# Maintainer: Laszlo Dvornik <dvornik@gnome.hu>

pkgname=evolution-webcal
pkgver=2.2.1
pkgrel=1
pkgdesc="Evolution web calendar"
url="http://www.gnome.org/projects/evolution/"
depends=('evolution-data-server')
makedepends=('intltool')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
install=$pkgname.install
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2 \
	$pkgname.install)
md5sums=('36c667bb50a2ae89be457780081dfd39' \
	 '67458f2e305ed489f071a41fb27cc725')

build() {
	Fcd
	Fmake --sysconfdir=/etc --disable-schemas-install || return 1
	Fmakeinstall GCONF_DISABLE_SCHEMA_INSTALL=1 || return 1
}

# vim: ft=sh
