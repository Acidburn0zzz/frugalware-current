depends=('gconf' 'gnome-vfs')
makedepends=('perl-xml-parser')
groups=('gnome' 'gnome-minimal')
archs=('i686' 'x86_64' 'ppc')
_F_gnome_schemas=('/etc/gconf/schemas/gweather.schemas')
Finclude gnome gnome-scriptlet
