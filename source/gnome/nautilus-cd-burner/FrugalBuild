pkgname=nautilus-cd-burner
pkgver=2.14.2
pkgrel=1
pkgdesc="An easy to use CD/DVD burning package for Nautilus"
url="http://www.gnome.org/"
depends=('nautilus' 'cdrtools')
makedepends=('intltool')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
install=$pkgname.install
Finclude gnome
sha1sums=('75d2e43f8d3df2f2ea8eb30b1c694e5bea84b094')

build()
{
	Fmake --disable-schemas-install --enable-hal
	Fmakeinstall GCONF_DISABLE_SCHEMA_INSTALL=1
}

# optimization OK
