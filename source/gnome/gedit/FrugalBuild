pkgname=gedit
pkgver=2.15.5
pkgrel=1
pkgdesc="A text editor for GNOME"
url="http://www.gnome.org/"
depends=('aspell' 'eel' 'libgnomeprintui' 'scrollkeeper' 'desktop-file-utils' \
	'gnome-python-desktop>=2.15.90')
makedepends=('intltool' 'gnome-doc-utils')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
_F_gnome_schemas=('/etc/gconf/schemas/gedit.schemas')
_F_gnome_scrollkeeper="y"
_F_gnome_desktop="y"
Finclude gnome gnome-scriptlet
sha1sums=('859b7a99e2fc326a91a982eac7bd90e378ee90cd')

unset MAKEFLAGS

