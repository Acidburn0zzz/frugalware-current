pkgname=atk
pkgver=1.10.1
pkgrel=2
pkgdesc="A library of accessibility functions used by GNOME"
url='http://www.gtk.org/'
depends=('glib2')
nondevel=1
groups=('gnome' 'gnome-core')
makedepends=('gtk-doc')
archs=('i686' 'x86_64')
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2)
md5sums=('29df8fe9016083e7eaf129bdd65d8402')

build() {
	Fbuild --enable-gtk-doc || return 1
}

