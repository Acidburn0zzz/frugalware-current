pkgname=atk
pkgver=1.10.3
pkgrel=1
pkgdesc="A library of accessibility functions used by GNOME"
url='http://www.gtk.org/'
depends=('glib2')
groups=('gnome' 'gnome-core')
makedepends=('gtk-doc')
archs=('i686' 'x86_64')
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2)
sha1sums=('74d83882dec2e9bd9233d12ae8db97064e7c3fbe')

build() {
	Fbuild --enable-gtk-doc
}

# optimalization OK

