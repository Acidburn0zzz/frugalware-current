pkgname=libgnomeprint
pkgver=2.18.2
pkgrel=1
pkgdesc="GNOME printing library"
url="http://www.gnome.org/"
depends=('pango>=1.18.2' 'libxml2' 'libgnomecups>=0.2.2-3' 'fontconfig' 'popt' 'libart_lgpl')
makedepends=('intltool' 'gtk-doc')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
source=(${source[@]} cups-transport.patch)
Fconfopts="$Fconfopts --enable-gtk-doc --with-cups"
sha1sums=('25c468b78fedfc708b26e95941d51dddbbf25562' \
          '6e76a279547af2631d3003e9dece6cf4f7fd81dd')

