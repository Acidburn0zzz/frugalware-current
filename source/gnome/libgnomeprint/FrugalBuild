depends=('pango' 'libxml2' 'libgnomecups>=0.2.2-2' 'fontconfig' 'popt' 'libart_lgpl')
makedepends=('intltool' 'gtk-doc')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
Finclude gnome
source=(${source[@]} cups-transport.patch)
sha1sums=('7deaa96c34b3e7d7af18d12432ba6839c3d2e957' \
	  '6e76a279547af2631d3003e9dece6cf4f7fd81dd')
Fconfopts="$Fconfopts --enable-gtk-doc --with-cups"

