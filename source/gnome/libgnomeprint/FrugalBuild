pkgname=libgnomeprint
pkgver=2.12.0
pkgrel=1
pkgdesc="GNOME printing library"
url="http://www.gnome.org/"
depends=('pango' 'libxml2' 'libgnomecups' 'fontconfig' 'popt' 'libart_lgpl')
makedepends=('intltool' 'gtk-doc')
groups=('gnome' 'gnome-core')
archs=('i686' 'x86_64')
[ -e ../up2date.gnome ] && . ../up2date.gnome
source=(http://ftp.gnome.org/pub/gnome/sources/$pkgname/$preup2date/$pkgname-$pkgver.tar.bz2)
sha1sums=('86bda382f48f5dfb341b26520f7519a98f31b8f3')

build() {
	Fbuild --enable-gtk-doc --with-cups
}

