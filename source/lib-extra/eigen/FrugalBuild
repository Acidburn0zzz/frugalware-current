pkgname=eigen
pkgver=2.0.6
pkgrel=1
pkgdesc="A lightweight C++ template library for vector and matrix math."
url="http://eigen.tuxfamily.org/"
depends=()
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_cd_path=eigen2
Finclude cmake
up2date="lynx -dump http://eigen.tuxfamily.org | grep released | sed 's/\(.*\) Eigen \(.*\) released!/\2/'"
source=(http://bitbucket.org/eigen/eigen2/get/$pkgver.tar.bz2)
sha1sums=('610f9a984681585b8c001aa4adb4f054971f07f9')
