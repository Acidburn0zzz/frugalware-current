pkgname=libassuan1
_F_archive_name="libassuan"
pkgver=1.0.5
pkgrel=1
pkgdesc="Libassuan is the IPC library used by some GnuPG related software"
url="ftp://ftp.gnupg.org/gcrypt/libassuan"
depends=('libgpg-error')
makedepends=('pth') # it must be compiled with pth for gpg-agent
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=($url/$_F_archive_name-$pkgver.tar.bz2)
sha1sums=('f6c36e9d99736883a8ce5b80b0a51d854cbbbc70')

