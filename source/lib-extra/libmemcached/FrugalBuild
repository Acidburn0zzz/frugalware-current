pkgname=libmemcached
pkgver=1.0.5
pkgrel=1
pkgdesc="A C and C++ client library for memcached"
depends=('memcached')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_launchpad_branch="1.0"
Finclude launchpad
sha1sums=('900b58ec83fcdb7d305ece8b6e8bc31652e07a36')

build() {
	Fcd
	Fconf --prefix=/usr
	Fmake
	Fmakeinstall
}

# optimization OK
