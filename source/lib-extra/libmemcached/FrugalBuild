pkgname=libmemcached
pkgver=1.0.13
pkgrel=1
pkgdesc="A C and C++ client library for memcached"
depends=('memcached')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_launchpad_branch="1.0"
Finclude launchpad
sha1sums=('4bc14e47ce3719757acd50ce861a5c9efcfa13ec')

build() {
	Fcd
	Fconf --prefix=/usr
	Fmake
	Fmakeinstall
}
