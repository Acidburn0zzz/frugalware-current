pkgname=python-requests
_F_archive_name="requests"
pkgver=2.9.0
pkgrel=1
pkgdesc="Python HTTP for Humans."
depends=('python')
makedepends=()
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_pypi_name="requests"
Finclude pypi
sha1sums=('5da11303dcb6eca601c79cf8c90345cd997d66c0')

build() {
	Fcd
	python setup.py install --root="$Fdestdir" -O1 || Fdie
}
