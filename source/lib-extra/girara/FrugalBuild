archs=('i686' 'x86_64')
groups=('lib-extra')
depends=('gtk+3')
up2date="Flasttar $url/download"
source="$url/download/$pkgname-$pkgver.tar.gz"
sha1sums=('92e2781d8dac7a0cdb13c144d3b2cb208f28e7f8')

# optimization OK
