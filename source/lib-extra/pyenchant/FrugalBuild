pkgname=pyenchant
pkgver=1.1.5
pkgrel=2
pkgdesc="PyEnchant is a spellchecking library for Python."
url="http://pyenchant.sourceforge.net/"
depends=('enchant')
makedepends=('setuptools')
groups=('lib-extra')
archs=('i686' 'x86_64')
Finclude sourceforge
sha1sums=('69f5c9ed9eb306e513a81d1a6d3afcc76fe03144')

build() {
	Fmake
	Fmakeinstall --single-version-externally-managed
}

# optimization OK
