pkgdesc="PyEnchant is a spellchecking library for Python."
url="http://pyenchant.sourceforge.net/"
depends=('enchant')
makedepends=('setuptools')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_sourceforge_broken_up2date=1
Finclude sourceforge
Fconfopts="$Fconfopts --single-version-externally-managed"
up2date="lynx -dump http://pyenchant.sourceforge.net/ |grep stable |sed 's/.*t-\(.*\)./\1/'"
sha1sums=('97cfc31ee5e2bb438f7c8b06ba6f2d16daabc047')

