pkgname=paramiko
pkgver=1.6.1
pkgrel=1
pkgdesc="SSH2 protocol for python"
url="http://www.lag.net/paramiko/"
depends=('pycrypto' 'python')
groups=('lib-extra')
archs=('i686')
up2date="lynx -dump http://www.lag.net/paramiko/ | grep -m2 download | sed -n 's/.*-\(.*\)\.z.*/\1/;$ p'"
source=(http://www.lag.net/$pkgname/download/$pkgname-$pkgver.zip)
sha1sums=('faf4dbca6762c970e7a7da03b4bd7391d14d0d3a')

