pkgname=paramiko
pkgver=1.6.4
pkgrel=1
pkgdesc="SSH2 protocol for python."
url="http://www.lag.net/paramiko/"
depends=('pycrypto' 'python>=2.5')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.lag.net/paramiko/ | grep -m2 download | sed -n 's/.*-\(.*\)\.z.*/\1/;$ p'"
source=($url/download/$pkgname-$pkgver.zip)
sha1sums=('4fe7bef40685afaa3b7e7fa6c54a0ade5f6df21c')

