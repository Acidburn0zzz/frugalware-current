pkgname=python3-requests
_F_archive_name="requests"
pkgver=2.8.0
pkgrel=1
pkgdesc="Python HTTP for Humans."
depends=('python-3.0')
makedepends=()
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_pypi_name="requests"
Finclude pypi
sha1sums=('6d2f798cfb764129dcb1c2e7bf2dc0b80608c039')

build() {
	Fcd
	python3 setup.py install --root="$Fdestdir" -O1 || Fdie
}
