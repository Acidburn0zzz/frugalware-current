pkgname=python3-requests
_F_archive_name="requests"
pkgver=2.9.1
pkgrel=1
pkgdesc="Python HTTP for Humans."
depends=('python-3.0')
makedepends=()
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_pypi_name="requests"
Finclude pypi
sha1sums=('17f01c47a0d7c676f6291608ef2f43db3fa74095')

build() {
	Fcd
	python3 setup.py install --root="$Fdestdir" -O1 || Fdie
}
