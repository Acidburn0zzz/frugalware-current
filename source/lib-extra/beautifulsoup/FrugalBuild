pkgname=beautifulsoup
realname=BeautifulSoup
pkgver=3.0.3
pkgrel=1
pkgdesc="Beautiful Soup is a Python HTML/XML parser designed for quick turnaround projects like screen-scraping"
url="http://www.crummy.com/software/BeautifulSoup/"
depends=('python>=2.5')
groups=('lib-extra')
archs=('i686')
up2date="lynx -dump http://www.crummy.com/software/BeautifulSoup/ | grep -i 'latest version' | sed 's/.*Soup version //' | sed 's/\,.*//'"
source=(http://www.crummy.com/software/BeautifulSoup/download/BeautifulSoup.tar.gz)
sha1sums=('327bdd8e269fab18dfb01efa0e18a93a4ea482f9')
_F_cd_path="$realname-$pkgver"
