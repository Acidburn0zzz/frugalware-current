pkgname=libssh
pkgver=0.4.0
pkgrel=1
pkgdesc="The SSH library"
url="http://www.libssh.org/"
depends=('openssl' 'zlib')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_archive_grepv="\.9[0-9]"
up2date="Flasttar $url/files/"
source=($url/files/libssh-$pkgver.tar.gz)
sha1sums=('88f2021d2dfdb5c9521d99840b5ed8035a0ee5c3')
Finclude cmake
# optimization OK
