pkgname=libssh
pkgver=0.3.2
pkgrel=1
pkgdesc="The SSH library"
url="http://www.libssh.org/"
depends=('openssl' 'zlib')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url/files/"
source=($url/files/libssh-$pkgver.tar.gz)
sha1sums=('a0dee607a75b1fa86883a0f28731c3b1b6aac58e')
Finclude cmake
# optimization OK
