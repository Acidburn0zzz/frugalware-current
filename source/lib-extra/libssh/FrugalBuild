pkgname=libssh
pkgver=0.3.1
pkgrel=1
pkgdesc="The SSH library"
url="http://www.libssh.org/"
depends=('openssl' 'zlib')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url/files/"
source=($url/files/libssh-$pkgver.tar.gz)
sha1sums=('e80a272ec5aa173a659c0a22d3dc6f12563300c0')
Finclude cmake
# optimization OK
