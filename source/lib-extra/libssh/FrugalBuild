pkgname=libssh
pkgver=0.3.0
pkgrel=1
pkgdesc="The SSH library"
url="http://www.libssh.org/"
depends=('openssl' 'zlib')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url/files/"
source=($url/files/libssh-$pkgver.tar.gz)
sha1sums=('446601b488dd11f3255b667e030eb4979fb9575f')
Finclude cmake
# optimization OK
