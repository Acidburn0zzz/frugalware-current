pkgname=libssh
pkgver=0.3.91
pkgrel=1
pkgdesc="The SSH library"
url="http://www.libssh.org/"
depends=('openssl' 'zlib')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url/files/"
source=($url/files/libssh-$pkgver.tar.gz)
sha1sums=('67bb22b97ddb50dae1c1fe4abec419eac305d805')
Finclude cmake
# optimization OK
