pkgname=libssh
pkgver=0.3.4
pkgrel=1
pkgdesc="The SSH library"
url="http://www.libssh.org/"
depends=('openssl' 'zlib')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url/files/"
source=($url/files/libssh-$pkgver.tar.gz)
sha1sums=('abded99e80706ae8d50f5f25e1a3eed3c909513d')
Finclude cmake
# optimization OK
