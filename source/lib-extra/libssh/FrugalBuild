pkgrel=1
pkgdesc="The SSH library"
url="http://www.libssh.org/"
depends=('openssl>=1.0.0' 'zlib')
groups=('lib-extra')
archs=('i686' 'x86_64' 'ppc')
_F_archive_grepv="\.9[0-9]"
up2date="Flasttar $url/files/"
source=($url/files/libssh-$pkgver.tar.gz)
