pkgname=libpri
pkgver=1.4.1
pkgrel=1
pkgdesc="Primary Rate ISDN specification library"
url="http://www.asterisk.org/"
depends=('glibc')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/downloads | grep -m1 'libpri-.*.tar.gz' | sed 's/.*pri-\(.*\).t.*/\1/'"
source=(http://ftp.digium.com/pub/$pkgname/$pkgname-$pkgver.tar.gz \
        $pkgname-makefile.patch)
sha1sums=('76284bcb68b57d972396115ba29d1e9d3aa88507' \
          'af2c0d53ca4f219f2e8ac99fa7f65c276169c04b')

# optimization ok
