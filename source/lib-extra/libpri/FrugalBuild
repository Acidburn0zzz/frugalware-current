pkgname=libpri
pkgver=1.4.3
pkgrel=1
pkgdesc="Primary Rate ISDN specification library"
url="http://www.asterisk.org/"
depends=('glibc')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/downloads | grep -m1 'libpri-.*.tar.gz' | sed 's/.*pri-\(.*\).t.*/\1/'"
source=(http://ftp.digium.com/pub/$pkgname/$pkgname-$pkgver.tar.gz \
	libpri-cflags.diff)
sha1sums=('e56426b94dd66c5f8cd30be165196080b7275808' \
          'bd6fe6ad4ff35c18e470a0a98c7673b921890743')

