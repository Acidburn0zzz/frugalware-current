pkgname=mm-common
pkgver=0.9.10
pkgrel=1
pkgdesc="Common build files of the C++ bindings"
depends=('libsigc++2>=2.6.1-3')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_gnome_devel="y"
_F_gnome_ext=".tar.xz"
Finclude gnome
sha1sums=('72625160de9249cf7df86a1f860b5d4db94f0d6e')

build() {
	Fcd
	Fsed '$(datadir)/pkgconfig' '$(libdir)/pkgconfig' Makefile.{am,in}
	Fbuild
}

# optimization OK
