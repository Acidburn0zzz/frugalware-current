pkgname=stormlib
pkgver=8.04
pkgrel=1
pkgdesc="A pack of modules, written in C++, which are able to read and also to write files from/to the MPQ archives"
url="http://www.zezula.net/en/mpq/stormlib.html"
depends=('zlib' 'libstdc++' 'bzip2')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=(http://www.zezula.net/download/$pkgname.zip)
Finclude cmake
_F_cd_path="StormLib"
sha1sums=('c0e0df5302ddc150f7c481a0ff4fd275b89108a0')

# optimization OK
