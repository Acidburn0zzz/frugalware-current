pkgname=pymetar
pkgver=0.15
pkgrel=1
pkgdesc="This library downloads the weather report for a given station ID."
url="http://www.schwarzvogel.de/software-pymetar.shtml"
depends=('python>=2.6.1')
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url | Flasttar"
source=(http://www.schwarzvogel.de/pkgs/$pkgname-$pkgver.tar.gz setup.diff)
sha1sums=('b59d5da5286add38433d4172369d72e8361d3b2b' \
          '5cbd22532f018d1c96a206228abfe2299b730d3f')
