# Compiling Time: 0.13 SBU
# Maintainer: Melko <lorddarthfener@gmail.com>

pkgname=quazip
pkgver=0.3
pkgrel=2
pkgdesc="wrapper for ZIP/UNZIP package"
depends=('libqtcore')
archs=('i686' 'x86_64')
groups=(lib-extra)
Finclude sourceforge
sha1sums=('dbb08bf3edf0d81771b373e2154d4f258070277e')
unset MAKEFLAGS

build() {
	Fcd
	qmake PREFIX=/usr || Fdie
	make || Fdie
	make INSTALL_ROOT=$Fdestdir install
}
