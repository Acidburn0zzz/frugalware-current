pkgname=libgcal
pkgver=0.9.3.g2d8a207
pkgrel=2
pkgdesc="ANSI C library that does allow communication with google calendar and contacts"
url="http://code.google.com/p/libgcal/"
archs=('i686' 'x86_64')
groups=('lib-extra')
depends=('libxml2' 'curl>=7.19.6-2' 'libopensync-plugin-file' 'check')
license=('GPL')
_F_scm_type="git"
_F_scm_url="git://repo.or.cz/libgcal.git"
Fconfopts="--enable-curldebug"
Finclude cmake scm
build(){
	Funpack_scm
	CMake_build
}
# optimization OK
