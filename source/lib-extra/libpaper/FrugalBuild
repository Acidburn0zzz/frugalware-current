pkgname=libpaper
pkgver=1.1.22
pkgrel=1
pkgdesc="The paper library provides a simple way for applications to take actions based on a user-defined paper size"
pkgurl="http://packages.debian.org/unstable/source/libpaper"
depends=()
groups=('lib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url | grep tar.gz -m1 | sed 's/.*r_\(.*\).t.*/\1/'"
source=(http://ftp.debian.org/debian/pool/main/libp/$pkgname/${pkgname}_${pkgver}.tar.gz)

