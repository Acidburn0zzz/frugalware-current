_F_drupal_module=jquery_ui
_F_drupal_ver=6.x
pkgver=6.x_1.2
pkgrel=1
pkgdesc="Drupal wrapper module around the jQuery UI effects library."
rodepends=('drupal6-jquery_update')
Finclude drupal
jqueryui_ver=1.5.3
source=(${source[@]} http://jquery-ui.googlecode.com/files/jquery.ui-$jqueryui_ver.zip)
sha1sums=('634e4fc50812480ed646591fe09d0e7eb676ffb6' \
	  '83dc9b7a003a6b0a3e3adedf3c72bab24c15e3b2')

build()
{
	Fbuild_drupal
	Fcpr jquery.ui-$jqueryui_ver /var/www/drupal6/sites/all/modules/$_F_drupal_module/jquery.ui
}
