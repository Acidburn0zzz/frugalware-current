pkgname=zopeinterface
pkgver=3.0.1
pkgrel=2
pkgdesc="Standalone Zope interface library"
url="http://zope.org/Products/ZopeInterface"
depends=('python')
groups=('network-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url|grep Stable|sed -n 's/.*]\(.*\)f.*/\1/;1 p'"
source=($url/${pkgver}final/ZopeInterface-$pkgver.tgz)
sha1sums=('3707f7129acf085d5a0b6b616893ab647cbc7c84')

build()
{
	Fcd ZopeInterface-$pkgver
	Fbuild
}

# optimalization OK
