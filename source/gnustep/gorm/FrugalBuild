# Compiling Time: 0.17 SBU
# Maintainer: Priyank Gosalia <priyankmg@gmail.com>

pkgname=gorm
pkgver=1.2.6
pkgrel=1
pkgdesc="An interface builder application for GNUstep"
makedepends=('gnustep-make>=2.0.6' 'gcc-objc')
depends=('libobjc' 'gnustep-gui')
_F_gnustep_category="dev-apps"
Finclude gnustep
groups=(${groups[@]} 'gnustep-apps')
archs=('i686')
sha1sums=('d96448e89a894dc40a67f4fff7ff2d89e7bd7363')

build()
{
	Fgnustepinit
	Fconf
	make || Fdie
	make DESTDIR=$Fdestdir install || Fdie
}


# optimization OK
