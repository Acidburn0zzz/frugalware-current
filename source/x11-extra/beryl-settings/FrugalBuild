pkgname=beryl-settings
pkgrel=1
makedepends=('intltool' 'perl-xml')
groups=('x11-extra')
archs=('i686' 'x86_64')
_F_gnome_desktop=y
Finclude gnome-scriptlet beryl
depends=("beryl=$pkgver" 'gtk+2' 'beryl-dbus')

build() {
	Fbuild_beryl
	Fbuild_gnome_scriptlet
}

# optimization OK
