pkgname=cgwd-themes
pkgver=20060806
pkgrel=1
pkgdesc="Themes for cgwd, a window decorator for compiz"
url="http://en.opensuse.org/Compiz"
depends=('xgl' 'compiz-cvs' 'cgwd')
groups=('x11-extra')
archs=('i686')
up2date="$pkgver"
source=(http://frugalware.org/~priyank/cvs/$pkgname-$pkgver.tar.bz2)
sha1sums=('1e0b01384c93171d3eff4d12c22a7b123f3f41c6')

build() {
	Fcd
	./autogen.sh --prefix=/usr
	make || return 1
	make DESTDIR=$Fdestdir install || return 1
}
