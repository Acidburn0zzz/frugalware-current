pkgrel=1
pkgdesc="A tool for adjusting the volume of WAV files to a standard level"
url="http://www.cs.columbia.edu/~cvaill/normalize/"
depends=('libmad' 'glibc')
groups=('multimedia')
archs=('i686' 'x86_64')
