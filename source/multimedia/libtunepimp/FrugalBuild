# Last Modified: Thu, 30 Mar 2006 02:16:58 +0200
# Compiling Time: 0.66 SBU
# Maintainer: crazy <crazy@frugalware.org>

pkgname=libtunepimp
pkgver=0.4.2
pkgrel=1
pkgdesc="The TunePimp library is a development library to create MusicBrainz enabled tagging applications."
url="http://musicbrainz.org/products/tunepimp/"
depends=('libmusicbrainz' 'libmad' 'flac' 'libvorbis' 'libid3tag')
conflicts=('trm')
replaces=('trm')
groups=('multimedia')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/download.html|grep 'Source D'|sed 's/.*) \([^ ]*\) .*/\1/;q'"
source=(http://ftp.musicbrainz.org/pub/musicbrainz/$pkgname-$pkgver.tar.gz)
sha1sums=('deb7ac22e451b7512006e6f090333e14d4cbc78f')

build() 
{
	Fmkdir /usr/lib/pkgconfig
	Fbuild --disable-dependency-tracking --with-gnu-ld
}

# optimalization OK

