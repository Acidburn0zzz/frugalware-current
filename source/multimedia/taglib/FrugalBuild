pkgver=1.11
pkgrel=2
pkgdesc="A library for reading and editing the meta-data of several popular audio formats."
groups=('multimedia')
archs=('i686' 'x86_64')
depends=('libstdc++>=5.3.0-2' 'zlib>=1.2.8-4')
makedepends+=('boost')
