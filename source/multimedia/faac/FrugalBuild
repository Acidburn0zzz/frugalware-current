# Compiling Time: 0.14 SBU
# Maintainer: crazy <crazy@frugalware.org>

pkgname=faac
pkgver=1.25
pkgrel=1
pkgdesc="FAAC is an AAC audio encoder."
depends=('libmp4v2')
makedepends=('hd2u')
groups=('multimedia')
archs=('i686' 'x86_64')
_F_sourceforge_broken_up2date=1
Finclude sourceforge
url="http://www.audiocoding.com"
sha1sums=('482a0217362c50ce523ce5e165c5566e1a4a2a87')


build()
{
	Fcd $pkgname
	dos2unix * || Fdie
	chmod +x bootstrap || Fdie
	./bootstrap || Fdie
	Fbuild
}

# optimization OK
