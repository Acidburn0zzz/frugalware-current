pkgname=libsndfile
pkgver=1.0.27
pkgrel=3
pkgdesc="Libsndfile is a C library for reading and writing files containing sampled sound"
url="http://www.mega-nerd.com/libsndfile"
groups=('multimedia')
archs=('x86_64')
depends=('flac>=1.3.1-3' 'sqlite3>=3.9.2-4' 'alsa-lib>=1.1.2-2' 'libogg>=1.3.2-5' 'libvorbis>=1.3.5-5')
up2date="Flasttar $url/files"
source="$url/files/$pkgname-$pkgver.tar.gz"
sha1sums=('e112d4937352d1722b06911b00c79e9bce15095c')
_F_cross32_simple_auto="yes"
Finclude cross32

# optimization OK
