pkgname=alsa-lib
pkgver=1.0.10
pkgrel=1
pkgdesc="An alternative implementation of Linux sound support"
url="http://www.alsa-project.org"
depends=('glibc')
groups=('multimedia')
archs=('i686' 'x86_64')
up2date="lynx -dump $url |grep 'Stable release' -3|sed -n 's/.*]\(.*\)/\1/;$ p'"
source=(ftp://ftp.alsa-project.org/pub/lib/$pkgname-$pkgver.tar.bz2)
sha1sums=('020b334bee891e58b17fc768e8cbd1e5ce058aa9')

