pkgname=alsa-lib
pkgver=1.0.14
pkgextraver=a
pkgrel=2
pkgdesc="An alternative implementation of Linux sound support"
url="http://www.alsa-project.org"
depends=('glibc')
groups=('multimedia')
archs=('i686' 'x86_64')
up2date="lynx -dump $url |grep -o 'alsa-lib-\(.*\)tar.bz2'|grep -v rc|sed 's/.*lib-\(.*\).t.*/\1/;s/$pkgextraver//;q'"
source=(ftp://ftp.alsa-project.org/pub/lib/$pkgname-$pkgver$pkgextraver.tar.bz2)
sha1sums=('0d0e217b778c411220471cd941ae48c4af675e61')
# optimization ok
