pkgname=gphoto2
pkgver=2.1.6
pkgrel=1
pkgdesc="A set of digital camera applications for Unix-like systems."
url="http://www.gphoto.org"
depends=('glibc' 'libgphoto2' 'libexif' 'libjpeg' 'ncurses' 'popt' 'readline' 'aalib')
groups=('multimedia')
archs=('i686' 'x86_64')
up2date="lynx -dump $url|grep '[0-9].[0-9] has' |sed -n 's/.*2 \(.*\) h.*/\1/;1 p'"
source=(http://dl.sourceforge.net/gphoto/$pkgname-$pkgver.tar.gz)
md5sums=('2de2bcc62599b8a7337b54b0a067c50b')

