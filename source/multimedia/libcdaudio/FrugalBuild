pkgname=libcdaudio
pkgver=0.99.12
pkgrel=1
pkgdesc="Library of cd audio related routines."
url="http://libcdaudio.sourceforge.net/"
depends=('glibc')
groups=('multimedia')
archs=('x86_64')
up2date="lynx -dump 'http://sourceforge.net/project/showfiles.php?group_id=27134' | grep libcdaudio- | grep -v p2 |sed -n -e 's/.*libcdaudio-\([0-9\.].*\)\.t.*/\1/' -e '1 p'"
source=(http://dl.sourceforge.net/$pkgname/$pkgname-$pkgver.tar.gz \
	libcdaudio-0.99-CAN-2005-0706.patch)
sha1sums=('1862d3f387634a216faa867164d840b6f5552294' \
	  '00045e3cd2be07254665029205182eec87288d83')

build() {
	Fbuild --enable-threads
}

