pkgname=libdvdnav
pkgver=5.0.3
pkgrel=4
pkgdesc="The library for xine-dvdnav plugin."
url="http://www.mplayerhq.hu/"
dlurl="http://download.videolan.org/pub/videolan/$pkgname/"
groups=('multimedia')
archs=('i686' 'x86_64')
depends=('glibc>=2.24-4' 'libdvdread>=5.0.3-4')
makedepends=('libdvdcss>=1.4.0-2')
up2date="Flastverdir $dlurl"
source=($dlurl/$pkgver/$pkgname-$pkgver.tar.bz2)
sha1sums=('e3ec3277e37dd1714e7dc7d205b8fd084125b51a')

build()
{
	Fcd
	Fautoreconf
	Fmake
	Fmakeinstall
}

# optimization OK
