pkgname=lsdvd
pkgver=0.10
pkgrel=1
pkgdesc="This program is designed to print information about the contents and \
structure of a DVD."
url="http://untrepid.com/acidrip/"
depends=('glibc' 'libdvdread')
groups=('multimedia')
up2date=`lynx -dump http://sourceforge.net/projects/acidrip/|grep lsdvd|sed 's/.*]\(.*\) -.*/\1/'`
source=(http://mesh.dl.sourceforge.net/sourceforge/acidrip/$pkgname-$pkgver.tar.gz)
md5sums=('7fd659f8e26fd790ecc4903d7961985a')
license="GPL"

