# Compiling Time: 0.01 SBU
# Maintainer: James Buren <ryuo@frugalware.org>

pkgname=unifont
pkgver=9.0.02
pkgrel=1
pkgdesc="Unicode font from GNU in PCF format."
url="http://unifoundry.com"
groups=('fonts-extra')
archs=('i686' 'x86_64')
up2date="Flastarchive $url/unifont.html .pcf.gz"
_F_fonts_subdir='misc'
Finclude fonts
source=($url/pub/$pkgname-$pkgver/font-builds/$pkgname-$pkgver.pcf.gz)
sha1sums=('92c320c57e9d70b75922265f7dd6327c1db0667d')

build()
{
	mv $pkgname-$pkgver.pcf $pkgname.pcf || Fdie
	Fbuild_fonts
}

# optimization OK
