groups=('kde5')
_F_scm_type="git"
_F_scm_tag="master"
_F_scm_url="git://anongit.kde.org/libkcddb"
_F_kde_project="applications"
Finclude kf5 scm
depends=("kio>=$_F_kf5_full" 'libmusicbrainz>=5.1.0-3')
makedepends+=('kdoctools')

unset source

build()
{
        Funpack_scm
        KDE_build
}


