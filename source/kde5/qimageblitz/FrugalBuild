pkgname=qimageblitz
pkgver=0.0.7.g8760f7d
pkgrel=2
pkgdesc="Blitz image effect and filter library for KDE 4.0"
groups=('kde5')
archs=('i686' 'x86_64')
_F_github_author="mylxiaoyi"
_F_github_devel="yes"
Finclude kf5 github
depends=("qt5-base>=$_F_kdever_qt5")
unset source

build()
{
        Funpack_scm
        KDE_build
	Fmkdir usr
	Fmv var/tmp/fst/src/qimageblitz/frugalware_cmake_build/lib/ usr/
	Frm var/
}

# optimization OK
