pkgname=libmediawiki
pkgver=17.08.1
pkgrel=1
pkgdesc="A KDE C++ interface for MediaWiki based web service."
archs=("x86_64")
_F_scm_type="git"
_F_scm_url="git://anongit.kde.org/$pkgname"
_F_kde_project="applications"
Finclude kf5 scm
up2date="$pkgver"
depends=("kcoreaddons>=$_F_kf5_full")
unset source

build()
{
        Funpack_scm
        KDE_build
}

# optimization OK
