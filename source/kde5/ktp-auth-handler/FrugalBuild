pkgname=ktp-auth-handler
pkgrel=1
pkgdesc="Background application for handling passwords and ssl certificates."
_F_kde_project="applications"
Finclude kf5
archs=('i686' 'x86_64')
depends=("ktp-common-internals>=$_F_kdever_apps" "kdewebkit>=$_F_kf5_full" 'qca>=2.1.1-4' 'qca-ossl>=2.1.1-4')
sha1sums=('ddfbd4a00a41c58a0942e55a9087986b27a8fa05')

replaces=('telepathy-kde-auth-handler')
provides=("${replaces[@]}")
conflicts=("${replaces[@]}")

build()
{
	Fcd
	Fsed 'Qca-qt5-ossl' 'Qca-qt5' CMakeLists.txt
	KDE_build
}
# optimization OK
