pkgver=17.08.3
pkgrel=1
pkgdesc="DNS Service Discovery for KDE."
groups=('kde5')
archs=("x86_64")
_F_scm_type="git"
_F_scm_url="git://anongit.kde.org/zeroconf-ioslave"
_F_kde_project="applications"
Finclude kf5 scm
up2date="$pkgver"
depends=("kio>=$_F_kf5_full" "kdnssd>=$_F_kf5_full")
unset source

build()
{
