pkgname=emotion
pkgver=1.7.1
pkgrel=1
pkgdesc="Video playback library for E17"
depends=('edje>=1.7.1' 'xine-lib>=1.2.2-1' 'gstreamer')
groups=('e17-extra' 'e17-core')
archs=('i686' 'x86_64')
Finclude e17
sha1sums=('a0877e96f041322cdfc3597a1cb72c5d49c346de')

build()
{
	Fcd
	Fautoreconf
        Fbuild DESTDIR=$Fdestdir
}

# optimization OK
