pkgname=emotion
pkgver=1.7.0
pkgrel=2
pkgdesc="Video playback library for E17"
depends=('edje>=1.7.0' 'xine-lib' 'gstreamer')
makedepends=('subversion')
groups=('e17-extra' 'e17-core')
archs=('i686' 'x86_64')
Finclude e17
sha1sums=('93d5d1ffd010827eeff3da5e2a817d95ea14f41e')

build()
{
	Fcd
	Fautoreconf
        Fbuild DESTDIR=$Fdestdir
}

# optimization OK
