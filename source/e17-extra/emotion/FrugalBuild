pkgname=emotion
pkgver=1.7.6
pkgrel=1
pkgdesc="Video playback library for E17"
depends=('edje>=1.7.6' 'xine-lib>=1.2.2-2' 'gstreamer')
makedepends=('subversion')
groups=('e17-extra' 'e17-core')
archs=('i686' 'x86_64')
Finclude e17
sha1sums=('88f99c62446206d486e7eb0f03516995b19e4e6c')

build()
{
	Fcd
	Fconf
        Fbuild DESTDIR=$Fdestdir
}

# optimization OK
