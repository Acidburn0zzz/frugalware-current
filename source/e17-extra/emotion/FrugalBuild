pkgname=emotion
pkgver=1.7.9
pkgrel=1
pkgdesc="Video playback library for E17"
depends=('edje>=1.7.9' 'xine-lib>=1.2.2-2' 'gstreamer')
makedepends=('subversion')
groups=('e17-extra' 'e17-core')
archs=('i686' 'x86_64')
Finclude e17
sha1sums=('9c82e6e725e802480f2beec693cfc801689364a1')

build()
{
	Fcd
	Fconf
        Fbuild DESTDIR=$Fdestdir
}

# optimization OK
