pkgname=emotion
pkgver=1.7.4
pkgrel=1
pkgdesc="Video playback library for E17"
depends=('edje>=1.7.4' 'xine-lib>=1.2.2-2' 'gstreamer')
groups=('e17-extra' 'e17-core')
archs=('i686' 'x86_64')
Finclude e17
sha1sums=('5b9b8cd524c7f058af401ffa997da26d087b24ee')

build()
{
	Fcd
	Fautoreconf
        Fbuild DESTDIR=$Fdestdir
}

# optimization OK
