pkgname=kcdemu
_F_kde_name=KCDemu
pkgver=0.4.0
pkgrel=2
pkgdesc="A GUI for the CDemu kernel module."
depends=('kdelibs' 'libstdc++')
rodepends=('cdemu')
groups=('kde-extra')
Finclude kde
url="http://wiki.suselinuxsupport.de"
archs=('i686' 'x86_64')
up2date="lynx -dump $url/wikka.php?wakka=Downloads|grep KCDemu|grep -m1 '.bz2'|sed 's/.*_\(.*\).tar.bz2.*/\1/'"
source=($url/uploads/files/${_F_kde_name}_${pkgver}.tar.bz2 install-fixes.patch)
_F_cd_path="$_F_kde_name"
sha1sums=('9dcf0a96169571efa7ff82f047db9d33e346590f' \
          '56737099e65602769832aa5b43337df89a42d921')

