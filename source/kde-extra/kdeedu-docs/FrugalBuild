# Last Modified: Thu, 01 Jun 2006 03:15:41 +0200
# Compiling Time: 0.23 SBU
# Maintainer: crazy <crazy@frugalware.org>

pkgname=kdeedu-docs
origname=kdeedu
pkgver=3.5.3
pkgrel=1
pkgdesc="Documentation for the KDE Educational programs."
groups=('kde-extra' 'kde-docs')
archs=('i686' 'x86_64')
rodepends=('kdeedu>=3.5.3')
makedepends=('kdelibs>=3.5.3')
Finclude kde
source=($pkgurl/$origname-$pkgver.tar.bz2)
sha1sums=('99ccf43ac130cbf2fbcdf07ca87d20fccf017123')

build()
{
	Fcd $origname-$pkgver
	Fbuild DO_NOT_COMPILE=" \
                 libkdeedu applnk blinken kalzium kanagram kbruch \
                 keduca kgeography khangman kig kiten klatin klettres kmplot \
                 kpercentage kstars ktouch kturtle kverbos kvoctrain kwordquiz"
}
