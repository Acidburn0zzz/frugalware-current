# Compiling Time: 2.75 SBU
# Maintainer: DeX77 <dex@dragonslave.de>

pkgname=kdewebdev
pkgrel=1
pkgdesc="KDE Webdev"
depends=('kdepimlibs>=4.3.3' 'kdebase-runtime>=4.3.3' 'tidy')
makedepends=('pkgconfig' 'automoc4' 'ruby' 'boost')
groups=('kde-extra')
archs=('i686' 'x86_64')

subpkgs=("$pkgname-docs")
subdescs=("Kdewebdev Documentation")
subdepends=('')
subgroups=('docs-extra kde-docs')
subarchs=('i686 x86_64')
_F_cmake_confopts="-DBUILD_doc=OFF"

_F_cmakekde_final=OFF
Finclude kde
sha1sums=('697f0557c616497a6f80bbcac472ad6ddd687ae8')

build() {
	
	CMakeKDE_build
	Fsplit $pkgname-docs /usr
}
# optimization OK
