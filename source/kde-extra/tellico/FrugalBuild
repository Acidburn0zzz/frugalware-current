pkgname=tellico
pkgver=1.1.6
pkgrel=2
pkgdesc="A collection manager for the KDE environment."
url="http://www.periapsis.org/tellico"
depends=('libxml2' 'libxslt' 'kdemultimedia' 'kdepim' 'taglib')
groups=('kde-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url |Flasttar"
source=($url/download/$pkgname-$pkgver.tar.gz \
	$pkgname-1.0.3-readlink.diff hu.po)
Finclude kdeapps
sha1sums=('42fdbe15b74217ce74028b380fddf3b9a7de90f4' \
	  '1566569d13f30587b21d918bd6c3de4cfd4c4360' \
	  '7d59246735085c42d0d8c6df63f099c2e5ccdf08')

build()
{
	Fcp ../src/hu.po ../src/$pkgname-$pkgver/po/
	Fbuild
}

# optimization OK
