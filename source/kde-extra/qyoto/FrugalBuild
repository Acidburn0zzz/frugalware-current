# Compiling Time: 0.34 SBU
# Maintainer: centuri0 <achevaux at gmail dot com>

pkgname=qyoto
pkgrel=1
pkgdesc=".NET/Mono bindings for the Qt libraries."
groups=('kde-extra')
archs=('i686' 'x86_64')
Finclude mono kde
depends=("smokeqt>=$_F_kde_ver")
makedepends=("${makedepends[@]}" 'mono')
source=("${source[@]}" cmake2810.patch)
sha1sums=("${sha1sums[@]}" '88115856a2bd52c9b66c322785363592216b0cf7')

build() {
        unset MAKEFLAGS
        unset _F_KDE_LD_FLAGS
	Fmonoexport
        KDE_build
	Fmonocleanup
}

# optimization OK
