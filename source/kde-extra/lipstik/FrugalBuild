pkgname=lipstik
pkgver=2.2.2
pkgrel=1
pkgdesc="Lipstik theme for KDE"
depends=('kdebase>=3.5.6')
groups=('kde-extra')
archs=('i686' 'x86_64')
Finclude kde
url="http://poplix.homelinux.com/lipstik/"
source=($url/$pkgname-$pkgver.tar.bz2)
up2date="lynx -dump $url |grep -om1 'lipstik-\(.*\).tar.bz2'|sed 's/.*-\(.*\).t.*/\1/'"

sha1sums=('6643230e4b5a646191c444b439a6658470a1812a')
# optimization OK
