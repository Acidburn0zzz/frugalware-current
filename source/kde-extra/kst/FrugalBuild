pkgname=kst
pkgver=2.0.2
pkgrel=1
pkgdesc="Fast real-time large-dataset viewing and plotting tool for KDE"
depends=('libqtopengl' 'libuuid' 'libqtxml' 'gsl')
makedepends=('libqtdesigner' 'libqtnetwork')
groups=('kde-extra')
archs=('i686' 'x86_64')
_F_archive_grepv="rc"
Finclude sourceforge
sha1sums=('80c7dca0fe36c64ff7a39678bb95233c3329a748')

build() {
	Fcd
	./install-kst release "$Fdestdir/usr" || Fdie
}
