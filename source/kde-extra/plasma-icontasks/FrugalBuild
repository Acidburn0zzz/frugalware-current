source=(http://kde-apps.org/CONTENT/content-files/$_F_kde_id-$pkgname-$pkgver.tar.bz2)
up2date="lynx -dump "$url"|grep -v http|grep  -m1 '      [0-9.0-9.0-9]'|sed 's/      \([0-9.]*\).*/\1/'"
