_F_lib32_name=wine
pkgver=1.6.2_2
rodepends=('lib32-libglu' 'lib32-alsa-lib' 'lib32-libmpg123' 'lib32-libxml2' 'lib32-openal')
sha1sums=('68b3bf608fadf3e01758a8db5e0f4796f413f8c4')

Finclude lib32

build() {
	Fbuildlib32
	Fexerel usr/bin/wine /usr/bin/wine
	Fexerel usr/bin/wine-preloader /usr/bin/wine-preloader
}
