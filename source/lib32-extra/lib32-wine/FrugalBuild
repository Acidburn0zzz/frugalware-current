_F_lib32_name=wine
pkgver=1.6.2_3
rodepends=('lib32-libglu' 'lib32-alsa-lib' 'lib32-libmpg123' 'lib32-libxml2' 'lib32-openal' 'lib32-lcms2' 'lib32-fontconfig')
sha1sums=('5c801d5cfd69861d07cd29135e6fb94cd95af619')

Finclude lib32

build() {
	Fbuildlib32
	Fexerel usr/bin/wine /usr/bin/wine
	Fexerel usr/bin/wine-preloader /usr/bin/wine-preloader
}
