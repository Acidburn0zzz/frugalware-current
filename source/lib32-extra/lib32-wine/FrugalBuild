_F_lib32_name=wine
pkgver=1.8.1_2
rodepends=('lib32-lcms2' 'lib32-openal' 'lib32-libglu' 'lib32-libldap' 'lib32-libpcap' \
	'lib32-libpulse' 'lib32-libmpg123' 'lib32-libgphoto2' 'lib32-gettext' \
        'lib32-libxcursor' 'lib32-libxi' 'lib32-libxrandr' 'lib32-libxinerama' \
	'lib32-libxcomposite' 'lib32-sane-backends' 'lib32-v4l-utils' \
	'lib32-libxrender' 'lib32-libxslt')
sha1sums=('ab9cffc2337fdbc3f685e8a3f2b5a27953cb7789')

Finclude lib32

build() {
	Fbuildlib32
	Fexerel usr/bin/wine /usr/bin/wine
	Fexerel usr/bin/wine-preloader /usr/bin/wine-preloader
}
