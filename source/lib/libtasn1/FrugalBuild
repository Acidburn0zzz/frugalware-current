pkgname=libtasn1
pkgver=0.3.2
pkgrel=1
pkgdesc="The ASN.1 library used in GNUTLS"
url="http://www.gnu.org/software/gnutls/"
groups=('lib')
archs=('i686' 'x86_64')
depends=('glibc' 'texinfo')
makedepends=('gtk-doc')
up2date="wget --passive-ftp -O - -q ftp://ftp.gnutls.org/pub/gnutls/$pkgname/|sed -n 's/.*-\([0-9.]\+\).tar.gz.*/\1/;T;p'|sort -r|sed -n '1p'"
source=(ftp://ftp.gnutls.org/pub/gnutls/$pkgname/$pkgname-$pkgver.tar.gz)
sha1sums=('25c228d9a3675b5d763a671d97438009e9a548d3')

