# Compiling Time: 0.36 SBU
# Maintainer: Melko <melko@frugalware.org>

pkgname=libraw
pkgver=0.17.2
pkgrel=1
pkgdesc="A library for reading RAW files obtained from digital photo cameras."
url="http://www.libraw.org/"
depends=('lcms2>=2.7-3' 'libjpeg>=9a')
groups=('lib')
archs=('i686' 'x86_64')
_F_archive_name="LibRaw"
_F_archive_grepv="Beta"
up2date="Flasttar $url/download"
source=(http://www.libraw.org/data/$_F_archive_name-${pkgver}.tar.gz)
sha1sums=('c6721f78bd7437334c33c26354d9f2e072d240e6')
unset MAKEFLAGS
options+=('force')
_F_cd_path="$_F_archive_name-${pkgver}"

# optimization OK
