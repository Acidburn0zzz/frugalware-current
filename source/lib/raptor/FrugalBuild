pkgname=raptor
pkgver=1.4.21
pkgrel=1
pkgdesc="Library providing a set of parsers that generate Resource Description Framework (RDF)."
url="http://librdf.org/raptor/"
depends=('curl>=7.19.0' 'libxslt' 'openssl' 'libxml2')
groups=('lib')
archs=('i686' 'x86_64' 'ppc')
up2date="lynx -dump http://librdf.org/raptor/ | grep 'Latest version' | sed 's/.*: \(.*\) .*/\1/'"
source=(http://download.librdf.org/source/$pkgname-$pkgver.tar.gz)
