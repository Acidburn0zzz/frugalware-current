pkgname=gnutls
pkgver=1.5.4
pkgrel=1
pkgdesc="A library which provides a secure layer over a reliable transport layer"
url="http://www.gnu.org/software/gnutls/"
groups=('lib')
archs=('i686' 'x86_64')
depends=('libtasn1>=0.3.6' 'opencdk' 'libgcrypt>=1.2.3' 'lzo' 'readline')
makedepends=('gtk-doc')
options=('scriptlet')
up2date="lynx -dump http://josefsson.org/gnutls/releases/|grep tar.bz2$ | grep -v rc | sed -n 's/.*-\(.*\)\.t.*/\1/;$ p'"
source=(http://josefsson.org/gnutls/releases/$pkgname-$pkgver.tar.bz2)
Finclude texinfo
Fconfopts="$Fconfopts --enable-gtk-doc"
sha1sums=('8d9895023a3939f45de95e84e6e9aa9103713e65')

# optimization OK
