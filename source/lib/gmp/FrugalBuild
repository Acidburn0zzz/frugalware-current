pkgname=gmp
pkgver=4.1.4
pkgrel=4
pkgdesc="GNU multiple precision arithmetic library"
url="http://swox.com/gmp/"
groups=('lib')
archs=('i686' 'x86_64')
depends=('glibc' 'texinfo')
up2date="lynx -dump http://swox.com/gmp/index.orig.html|grep Current|sed 's/.* \(.*\)$/\1/;q'"
groups=('lib')
source=(ftp://ftp.gnu.org/pub/gnu/$pkgname/$pkgname-$pkgver.tar.bz2)
md5sums=('0aa7d3b3f5b5ec5951e7dddd6f65e891')
install=$pkgname.install

build()
{
	Fbuild --enable-mpfr || return 1
}

