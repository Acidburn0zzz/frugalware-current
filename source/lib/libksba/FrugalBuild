pkgname=libksba
pkgver=0.9.13
pkgrel=1
pkgdesc="Libksba  is a CMS and X.509 access library."
url="http://www.gnupg.org"
depends=('glibc' 'libgpg-error')
groups=('lib')
archs=('i686' 'x86_64')
install=$pkgname.install
up2date="lynx -dump http://www.gnupg.org/\(en\)/download/index.html |grep libksba.*tar |sed -n -e 's/.*a-\(.*\)\.t.*/\1/;s/-/_/;1 p'"
source=(ftp://gd.tuwien.ac.at/privacy/gnupg/alpha/libksba/$pkgname-$pkgver.tar.bz2)
sha1sums=('32809d450ae38a06fe7b5bb6c057ddbb23f75682')

