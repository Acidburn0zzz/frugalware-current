groups=('lib')
archs=('i686' 'x86_64')
depends=('libgcc')
_F_cd_path="gc$pkgver"
up2date="lynx -dump '$url/gc_source/?M=D'|grep -o 'gc\(.*\).tar.gz$'|Fsort|sed 's/.*source//'|Flasttar|sed 's/.*gc\(.*\).t.*/\1/'"
source=($url/gc_source/gc$pkgver.tar.gz)
