# Last Modified: Sat, 10 Sep 2005 22:07:43 +0200
# Compiling Time: 0.18 SBU
# Maintainer: Shrift <shrift@frugalware.org>

pkgname=libgc
pkgver=6.6
pkgrel=1
pkgdesc="Boehm conservative garbage collector"
url="http://www.hpl.hp.com/personal/Hans_Boehm/gc/"
groups=('lib')
archs=('i686' 'x86_64')
depends=('gcc-lib')
up2date="lynx -dump '$url/gc_source/?M=D'|grep [0-9].tar.gz$|sed -n 's/.*gc\(.*\)\.t.*/\1/;1 p'"
source=($url/gc_source/gc$pkgver.tar.gz)
sha1sums=('524ffba866c54234d1fdb98927039262cc14877e')

build()
{
	Fcd gc$pkgver
	Fbuild
}
# optimalization ok
