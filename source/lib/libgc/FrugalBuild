groups=('lib')
archs=('i686' 'x86_64')
depends=('gcc-lib')
_F_cd_path="gc$pkgver"
up2date="lynx -dump '$url/gc_source/?M=D'|grep [0-9].[0-9].tar.gz$|sed -n 's/.*gc\(.*\)\.t.*/\1/;1 p'"
source=($url/gc_source/gc$pkgver.tar.gz)
