pkgrel=1
pkgdesc="jack audio connection kit is a low-latency audio server"
_F_sourceforge_dirname="jackit"
_F_sourceforge_name="jack-audio-connection-kit"
Finclude sourceforge
url="http://jackit.sourceforge.net/"
depends=('alsa-lib')
makedepends=('doxygen')
