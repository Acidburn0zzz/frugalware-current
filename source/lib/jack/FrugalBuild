archs=('i686' 'x86_64')
_F_cd_path="jack-audio-connection-kit-$pkgver"
source=($source no-multilib-stuff-k-thx.patch)

build()
{
	Fpatchall
	Fautoreconf
	Fmake
	Fmakeinstall
}

