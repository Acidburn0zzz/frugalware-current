pkgname=libxslt
pkgver=1.1.31
pkgrel=1
pkgdesc="XML stylesheet transformation library"
url="http://xmlsoft.org/XSLT/"
_F_archive_grepv="-rc"
up2date="Flastarchive http://xmlsoft.org/sources/ .tar.gz"
groups=('lib')
archs=("x86_64")
depends=('libgcrypt>=1.7.3-2' 'libxml2>=2.9.6')
makedepends=('python>=2.7.12-2')
source=(http://xmlsoft.org/sources/$pkgname-$pkgver.tar.gz)
sha1sums=('850be1f3b38a1dba1629f90f9543c0177f106692')

build() {
	Fbuild --without-debug --with-plugins --with-debugger
	Fdocrel Copyright
}

# optimization OK
