pkgname=gd
pkgver=2.0.33
pkgrel=1
pkgdesc="an ansi C library to generate fany images from programs"
url="http://www.boutell.com/gd/http"
depends=('glibc') 
groups=('lib')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.boutell.com/gd/| grep Stable|sed  -e 's|.*Version \(.*\) (.*|\1|g'"
source=($url/$pkgname-$pkgver.tar.gz)
md5sums=('be0a6d326cd8567e736fbc75df0a5c45')

