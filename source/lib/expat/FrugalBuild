pkgname=expat
pkgver=1.95.8
pkgrel=1
pkgdesc="An XML Parser library written in C"
depends=('glibc')
url="http://expat.sf.net"
up2date=`lynx -dump http://expat.sourceforge.net/|grep ', Expat' |tr -s ' '|cut -d ' ' -f 6|sed -n '1 p'`
source=(http://unc.dl.sourceforge.net/sourceforge/expat/$pkgname-$pkgver.tar.gz)
md5sums=('aff487543845a82fe262e6e2922b4c8e')

build() {
  cd $startdir/src/$pkgname-$pkgver
  ./configure --prefix=/usr
  make || return 1
  make prefix=$startdir/pkg/usr install
}
