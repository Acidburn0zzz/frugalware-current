pkgname=libwebp
pkgver=0.5.0
pkgrel=2
pkgdesc="A library for a new image format comparable to JPEG."
depends=('libpng>=1.6.20' 'libjpeg>=9a' 'libtiff>=4.0.6')
groups=('lib')
archs=('i686' 'x86_64')
_F_github_name="$pkgname"
_F_github_author="webmproject"
_F_github_tag_v=y
Finclude github
up2date="lynx -dump https://github.com/webmproject/libwebp/releases| grep -a -v "rc[0-9]" | Flasttar | sed 's/v//g'"
sha1sums=('d820b4284ff12fee7d48f197c4c4fe9f01f4cbe2')
Fconfopts=" --enable-libwebpdemux "

# optimization OK
