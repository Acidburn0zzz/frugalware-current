# Compiling Time: 0.2 SBU
# Maintainer: James Buren <ryuo@frugalware.org>

options+=('asneeded')

pkgname=libwebp
pkgver=0.3.1
pkgrel=1
pkgdesc="A library for a new image format comparable to JPEG."
depends=('libpng' 'libjpeg')
groups=('lib')
archs=('i686' 'x86_64')
_F_github_name="$pkgname"
_F_github_author="webmproject"
_F_github_tag_v=y
Finclude github
up2date="lynx -dump https://github.com/webmproject/libwebp/releases| grep -a -v "rc[0-9]" | Flasttar | sed 's/v//g'"
sha1sums=('eb0accbd3bead6f46acc67314caf338a917aab53')

# optimization OK
