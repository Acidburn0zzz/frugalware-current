pkgname=libraw1394
pkgver=1.2.0
pkgrel=2
pkgdesc="Provides an API to the Linux IEEE1394 (FireWire) driver"
url="http://www.linux1394.org/"
depends=('glibc')
groups=('lib')
up2date=`lynx -dump 'http://linux1394.org/index.php'|grep "Latest libraw"|sed 's/.*libraw1394 release is.*v\(.*\)/\1/'`
source=(http://linux1394.org/dl/$pkgname-$pkgver.tar.gz)
md5sums=('eea394f74afecd6a22f74110128c9fb6')

