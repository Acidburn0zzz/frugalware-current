pkgname=libcddb
pkgver=1.2.1
pkgrel=1
pkgdesc="Libcddb is a library that implements the different protocols (CDDBP, HTTP, SMTP) to access data on a CDDB server (http://freedb.org)."
url="http://libcddb.sourceforge.net/"
depends=('glibc')
up2date="lynx -dump '$url'|grep Version |sed -n 's/.*n \(.*\) of.*/\1/;1 p'"
source=(http://heanet.dl.sourceforge.net/libcddb/$pkgname-$pkgver.tar.bz2)
sha1sums=('35e510af0bee12bd8aefe2c483978668728d6cd6')
license="GPL2"
groups=('lib')
archs=('i686' 'x86_64')

