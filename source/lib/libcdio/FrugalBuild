makedepends=('cdparanoia' 'cdrdao')
up2date="lynx -dump http://ftp.gnu.org/gnu/libcdio/|grep -o 'libcdio-[0-9\.]\+[0-9]'|tail -n 1|sed 's/libcdio-//'"
source=(ftp://ftp.cs.tu-berlin.de/pub/gnu/$pkgname/$pkgname-$pkgver.tar.gz)
