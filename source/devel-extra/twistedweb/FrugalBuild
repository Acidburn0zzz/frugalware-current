pkgrel=1
pkgdesc="Twisted web is a web server, and also provides basic HTTP client support."
url="http://twistedmatrix.com/projects/web/"
depends=('twisted')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://twistedmatrix.com/trac/wiki/TwistedWeb | grep 'Release notes' | sed 's/.*Twisted Web //'"
