pkgname=python-userwebkit
pkgver=13.12.0
pkgrel=1
pkgdesc="A library that allows WebKitGtk apps to talk to a local per-user CouchDB"
depends=('python-3.0>=3.3.0' 'python-microfiber>=13.12.0' 'pygobject' 'webkit-gtk3')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_launchpad_dirname="userwebkit"
_F_launchpad_name="userwebkit"
_F_launchpad_subdir="13.12"
Finclude launchpad
sha1sums=('ea1271803781ba40ff65920a4c308e1496906cdd')

build() {
	Fcd
	python3 setup.py install --prefix=/usr --root="$Fdestdir" -O1 || Fdie
}

