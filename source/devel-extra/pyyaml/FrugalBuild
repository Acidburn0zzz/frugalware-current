pkgname=pyyaml
pkgver=3.04
pkgrel=1
pkgdesc="a complete python YAML 1.1 parser"
url="http://pyyaml.org/wiki/PyYAML"
depends=('python')
groups=('devel-extra')
archs=('i686')
up2date="lynx -dump $url | grep -m1 'current stable' | sed 's/.*: \(.*\)./\1/'"
source=(http://pyyaml.org/download/pyyaml/PyYAML-$pkgver.tar.gz)
sha1sums=('c25830beca63212000487136cc2a968f90f80968')
_F_cd_path="PyYAML-$pkgver"

build() {
	Fmake
	Fmakeinstall
}

# optimization OK
