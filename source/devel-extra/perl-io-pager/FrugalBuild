pkgname=perl-io-pager
_F_perl_name=IO-Pager
_F_perl_author=J/JP/JPIERCE
pkgver=0.20
pkgrel=1
pkgdesc="Select a pager, optionally pipe it output if destination is a TTY"
depends=('perl')
groups=('devel-extra')
_F_perl_ext=".tgz"
Finclude perl
archs=('i686' 'x86_64')
sha1sums=('1270fe05f50efc85fd1613e5ea4d8a894fbf4564')
build(){
	Fcd ${_F_perl_name}-0.2
	Fbuild
}
