pkgname=nasm
pkgver=2.07
pkgrel=1
pkgdesc="NASM is the Netwide Assembler"
depends=('glibc')
groups=('devel-extra')
archs=('i686' 'x86_64' 'ppc')
url="http://www.nasm.us/"
up2date="lynx -dump $url|grep releasebuilds/[0-9]|sed 's|.*/\(.*\)/$|\1|'"
source=(http://www.nasm.us/pub/nasm/releasebuilds/$pkgver/nasm-$pkgver.tar.bz2)
sha1sums=('eb4cc984dbc936d02eb11cba5ff98066d7c01fa1')
Fmandir="$Fdestdir/usr/share/man"
# optimization OK
