pkgname=certifi
pkgver=2017.4.17
pkgrel=1
pkgdesc="Python package for providing Mozilla's CA Bundle"
groups=('devel-extra')
archs=('x86_64')
depends=('python')
Finclude pypi
_F_cd_path="$_F_pypi_name-$pkgver"
sha1sums=('d29f70056fb0eb69509658dc409c2eddaf59e573')

build() {
        Fcd
        python setup.py install --root="$Fdestdir" || Fdie
}
