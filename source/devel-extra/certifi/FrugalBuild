pkgname=certifi
pkgver=2017.7.27.1
pkgrel=1
pkgdesc="Python package for providing Mozilla's CA Bundle"
groups=('devel-extra')
archs=('x86_64')
depends=('python')
Finclude pypi
_F_cd_path="$_F_pypi_name-$pkgver"
sha1sums=('de9f9a06ef094850383c2b8b025ab66cb39eccb9')

build() {
        Fcd
        python setup.py install --root="$Fdestdir" || Fdie
}
