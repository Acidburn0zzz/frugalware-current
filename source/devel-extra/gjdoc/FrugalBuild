pkgname=gjdoc
pkgver=0.7.8
pkgrel=2
pkgdesc="A documentation generation system for 'javadoc'-style comments."
url="http://savannah.gnu.org/projects/classpath/"
depends=('libgcj' 'antlr')
makedepends=('gcc-gcj>=4.2.0')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump ftp://ftp.gnu.org/gnu/classpath/|grep gjdoc-.*gz$|sed -n 's/.*-\(.*\)\.t.*/\1/;$ p'"
source=(ftp://ftp.gnu.org/gnu/classpath/gjdoc-$pkgver.tar.gz)
signatures=($source.sig)
