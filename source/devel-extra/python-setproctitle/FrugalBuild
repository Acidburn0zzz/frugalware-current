pkgname=python-setproctitle
pkgver=1.1
pkgrel=1
pkgdesc="Allows a python process to change its process title"
url="http://pypi.python.org/pypi/setproctitle/"
depends=('python>=2.7.1')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_archive_name="setproctitle"
up2date="Flasttar $url"
source=(http://pypi.python.org/packages/source/s/$_F_archive_name/$_F_archive_name-$pkgver.tar.gz)
sha1sums=('fa497c7cf4b0a0f2c29f1b9c403aba1ff40539b5')

build() {
  Fcd
  python setup.py install --root=$Fdestdir || Fdie
}

# optimization OK
