pkgname=pure-docs
pkgver=0.54
pkgrel=1
pkgdesc="Pure Language and Library Documentation"
depends=()
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_googlecode_dirname="pure-lang"
Finclude googlecode
