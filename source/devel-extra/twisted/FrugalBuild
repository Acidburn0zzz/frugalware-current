pkgname=twisted
pkgver=2.2.0
pkgrel=2
pkgdesc="An event-driven networking framework written in Python."
url="http://twistedmatrix.com/"
depends=('pyopenssl>=0.5.1' 'pyserial' 'pycrypto>=1.9a6' 'zopeinterface')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://twistedmatrix.com/projects/core/|grep Minimal|sed 's/.*!) \([0-9\.]*\).*/\1/'"
source=(http://tmrc.mit.edu/mirror/twisted/Twisted/2.2/Twisted-$pkgver.tar.bz2)
sha1sums=('fcce114624dfde91bc1c2358f905d3170e72ad6a')

build()
{
	Fcd Twisted-$pkgver
	Fbuild
}

