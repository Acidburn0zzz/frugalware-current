pkgname=python-cffi
_F_archive_name="cffi"
pkgver=1.4.2
pkgrel=1
pkgdesc="cryptography expose cryptographic recipes and primitives in a python module"
depends=('pycparser>=2.14-2' 'python>=2.7.11-3' 'libffi>=3.2.1')
makedepends=('python-distribute>=0.6.49-2')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_pypi_name="cffi"
Finclude pypi
sha1sums=('76cff6f1ff5bfb2b9c6c8e2cfa8bf90b5c944394')

build() {
	Fcd
	python setup.py install --root=$Fdestdir || Fdie
}

