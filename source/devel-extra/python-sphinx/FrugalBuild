pkgname=python-sphinx
pkgver=1.3.4
pkgrel=1
pkgdesc="Python documentation generator"
depends=('pygments>=2.1' 'docutils>=0.12' 'python-jinja>=2.8')
makedepends=('python-distribute>=0.6.49-2')
groups=('devel-extra')
archs=('i686' 'x86_64' 'arm')
_F_archive_name="Sphinx"
_F_pypi_name="Sphinx"
Finclude pypi
_F_cd_path="$_F_pypi_name-$pkgver"
sha1sums=('3d54fbb8e188ee2435995d936dea9f7c17f6c9c0')

build()	{
	Fcd
	python setup.py install --root="$Fdestdir" || Fdie
}
