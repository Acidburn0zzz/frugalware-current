_F_pecl_name="memcache"
pkgver=3.0.3
pkgdesc="allows you to work with memcached through handy OO and procedural interfaces"
depends=('php>=5.2.6')
Finclude pecl
pkgrel=1
sha1sums=('b9ad5d59f63b220d04d6a443db33242da917191a')
up2date="lynx -dump http://pecl.php.net/package/$_F_pecl_name |grep -m1 '[0-9.]\{3,\}\.tgz$'|sed 's/.*-\(.*\)\.t.*/\1/'"

build() {
	Fbuildpecl
}

# optimization OK
