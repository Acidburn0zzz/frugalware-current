pkgname=python-degu
pkgver=0.4.0
pkgrel=1
pkgdesc="An embedded HTTP server and client library for Python3"
archs=('i686' 'x86_64')
groups=('devel-extra')
depends=('python-3.0>=3.4.1')
_F_launchpad_name=degu
_F_launchpad_subdir="0.4"
Finclude launchpad
sha1sums=('30b45dc4ca82aa5c709d1c5a91e9dcbdbf156653')

build()
{
	Fcd
	python3 setup.py install --prefix=/usr --root=$Fdestdir || Fdie
}
