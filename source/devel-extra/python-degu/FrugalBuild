pkgname=python-degu
pkgver=0.5.0
pkgrel=1
pkgdesc="An embedded HTTP server and client library for Python3"
archs=('i686' 'x86_64')
groups=('devel-extra')
depends=('python-3.0>=3.4.1')
_F_launchpad_name=degu
_F_launchpad_subdir="0.5"
Finclude launchpad
sha1sums=('3787bf2da5649272493188d521457b751e32d3e5')

build()
{
	Fcd
	python3 setup.py install --prefix=/usr --root=$Fdestdir || Fdie
}
