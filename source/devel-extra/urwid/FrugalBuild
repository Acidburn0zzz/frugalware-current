pkgname=urwid
pkgver=1.1.2
pkgrel=1
pkgdesc="Urwid is a curses-based user interface library for Python."
depends=('python>=2.7')
makedepends=('distribute')
groups=('devel-extra')
archs=('i686' 'x86_64')
options=('nostrip')
Finclude pypi
sha1sums=('288f61b444b7f21964fdee33e656da4abeb76c53')

build() {
	Fcd
	python setup.py install --prefix=/usr --root=$Fdestdir || Fdie
}

# optimization OK
