pkgname=urwid
pkgver=2.0.0
pkgrel=1
pkgdesc="Urwid is a curses-based user interface library for Python."
depends=('python>=2.7')
makedepends=('distribute')
groups=('devel-extra')
archs=('i686' 'x86_64')
options=('nostrip')
Finclude pypi
sha1sums=('41a1e2ce5c1bfe0d0059ba76f2b77d413491db7e')

build() {
	Fcd
	python setup.py install --prefix=/usr --root=$Fdestdir || Fdie
}

# optimization OK
