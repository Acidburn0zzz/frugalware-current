pkgname=urwid
pkgver=1.3.1
pkgrel=1
pkgdesc="Urwid is a curses-based user interface library for Python."
depends=('python>=2.7')
makedepends=('distribute')
groups=('devel-extra')
archs=('i686' 'x86_64')
options=('nostrip')
Finclude pypi
sha1sums=('5c562de41e150b87c8e2660f70c699fb6e951127')

build() {
	Fcd
	python setup.py install --prefix=/usr --root=$Fdestdir || Fdie
}

# optimization OK
