pkgname=python-distutils-extra
pkgver=2.10
pkgrel=1
pkgdesc="provides additional functions to Python's distutils and setuptools"
url="http://www.glatzor.de/projects/$pkgname"
depends=('python' 'setuptools')
groups=('devel-extra')
archs=('i686' 'x86_64' 'ppc')
up2date="lynx -dump http://archive.ubuntu.com/ubuntu/pool/main/p/$pkgname | grep tar.gz | grep -v ubuntu | sed 's/_/-/' | Flasttar"
source=(http://archive.ubuntu.com/ubuntu/pool/main/p/$pkgname/${pkgname}_$pkgver.tar.gz)
sha1sums=('eb3096cdeca8a01d9493e8e1b87998e0a4a3e42f')
_F_cd_path="debian"

