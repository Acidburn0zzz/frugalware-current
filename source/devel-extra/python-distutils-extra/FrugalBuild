groups=('devel-extra')
archs=('i686' 'x86_64' 'ppc')
up2date="lynx -dump http://archive.ubuntu.com/ubuntu/pool/main/p/$pkgname | grep tar.gz | grep -v ubuntu | sed 's/_/-/' | Flasttar"
