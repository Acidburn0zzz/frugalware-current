pkgname=python-alabaster
pkgver=0.7.9
pkgrel=1
pkgdesc="Python Sphinx Read The Docs Theme"
depends=('python>=2.7.12-2')
makedepends=('python-distribute>=0.6.49-3')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_archive_name="alabaster"
_F_pypi_name="alabaster"
Finclude pypi
_F_cd_path="$_F_pypi_name-$pkgver"
sha1sums=('5228e07468aec8bb09ce8adcd248e54ca18589b7')

build()	{
	Fcd
	python setup.py install --root="$Fdestdir" || Fdie
}

# optimization OK
