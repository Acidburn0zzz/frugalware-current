pkgname=python3-requests
_F_archive_name="requests"
pkgver=2.18.3
pkgrel=1
pkgdesc="Python HTTP for Humans."
depends=('python3>=3.5')
groups=('devel-extra')
archs=('x86_64')
_F_pypi_name="requests"
Finclude pypi
sha1sums=('877777f790b9e18a5d808ea3906da72aef1830fd')

build() {
	Fcd
	Fexec python3 setup.py install --root="$Fdestdir" -O1 || Fdie
}
