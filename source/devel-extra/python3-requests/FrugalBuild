pkgname=python3-requests
_F_archive_name="requests"
pkgver=2.13.0
pkgrel=1
pkgdesc="Python HTTP for Humans."
depends=('python3>=3.5')
groups=('devel-extra')
archs=('x86_64')
_F_pypi_name="requests"
Finclude pypi
sha1sums=('827f7c8abb5a57d980d9870908724613b8b120c1')

build() {
	Fcd
	python3 setup.py install --root="$Fdestdir" -O1 || Fdie
}
