pkgname=sqlite3-ruby
pkgver=1.2.1
pkgrel=1
pkgdesc="A Ruby interface for the SQLite database engine"
url="http://rubyforge.org/projects/sqlite-ruby/"
depends=('sqlite3')
groups=('devel-extra')
archs=('i686' 'x86_64')

Finclude gem

build() {
	Finstallgem
	Frm "$archdir"
}

sha1sums=('b749ca7ceff3f568965918d68259081bdd650802')
