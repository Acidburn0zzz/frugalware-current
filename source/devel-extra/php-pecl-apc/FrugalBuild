_F_pecl_name="apc"
pkgver=3.1.3p1
pkgrel=1
pkgdesc="Framework for caching and optimizing PHP intermediate code"
depends=('php>=5.2.9')
Finclude pecl
archs=('i686' 'x86_64')
pkgrel=1
sha1sums=('aed228105250f7d7f1deb57abb2770f1e19c2e49')

build() {
	Fcd APC-$pkgver
	Fbuildpecl
}

# optimization OK
