# Compiling Time: ~0.00 SBU
# Maintainer: Cedrick Hannier alias Cedynamix <cedynamix@gmail.com>

pkgname=gmpy
pkgver=1.01
tarver=`echo $pkgver | tr -d .`
pkgrel=5
pkgdesc="Python-usable modules providing multiprecision arithmetic functionality"
license="LGPL"
_F_sourceforge_ext=".zip"
_F_sourceforge_name="gmpy-sources"
Finclude sourceforge
url="http://gmpy.sourceforge.net/"
up2date="$pkgver"
source=(${source/$pkgver/$tarver})
groups=('devel-extra')
archs=('i686' 'x86_64')
depends=('gmp>=5.0.1' 'python')
sha1sums=('ce3f408ef12ec193ff7bf8b2ec987db438cfbea5')

build() {
	Fcd $pkgname
	Fbuild
	Fdocrel doc/*
}

# optimization OK
