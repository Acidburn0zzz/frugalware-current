# Compiling Time: ~0.00 SBU
# Maintainer: BMH1980 <bmh1980@frugalware.org>

pkgname=gmpy
pkgver=1.0
tarver=`echo $pkgver | tr -d .`
pkgrel=1
pkgdesc="Python-usable modules providing multiprecision arithmetic functionality"
url="http://gmpy.sourceforge.net/"
license="LGPL"
up2date="lynx -dump $url | grep 'release:' | cut -d ' ' -f 13"
source=(http://heanet.dl.sourceforge.net/sourceforge/$pkgname/$pkgname-sources-$tarver.zip)
groups=('devel-extra')
archs=('i686' 'x86_64')
depends=('gmp' 'python')
sha1sums=('4842fe979d20ef417d633d86fa6be0b94ac172ed')

build()
{
	Fcd $pkgname
	Fbuild
	Fdocrel doc/*
}

# optimalization OK
