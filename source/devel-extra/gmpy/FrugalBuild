pkgname=gmpy
pkgver=1.01
tarver=`echo $pkgver | tr -d .`
pkgrel=1
pkgdesc="Python-usable modules providing multiprecision arithmetic functionality"
url="http://gmpy.sourceforge.net/"
license="LGPL"
Finclude sourceforge
source=(http://heanet.dl.sourceforge.net/sourceforge/$pkgname/$pkgname-sources-$tarver.zip)
groups=('devel-extra')
archs=('i686' 'x86_64')
depends=('gmp' 'python')
sha1sums=('ce3f408ef12ec193ff7bf8b2ec987db438cfbea5')

build() {
	Fcd $pkgname
	Fbuild
	Fdocrel doc/*
}

# optimalization OK

# optimization OK
