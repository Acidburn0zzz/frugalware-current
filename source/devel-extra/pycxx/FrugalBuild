pkgname=pycxx
pkgver=5.3.5
pkgrel=2
pkgdesc="A set of classes to help create extensions of Python in the C++ language."
_F_sourceforge_dirname="cxx"
Finclude sourceforge
url="http://cxx.sourceforge.net/"
depends=('python' 'libstdc++')
groups=('devel-extra')
archs=('i686')
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=3180|grep ']PyCXX V'|sed 's/.*V\([^ ]*\) .*/\1/;q'"
_F_cd_path="${pkgname}_${pkgver//./_}"
source=(${source/pycxx*/$_F_cd_path.tar.gz})
sha1sums=('d7940fcd127d6603cc863b08cbae67ea3d669f59')
