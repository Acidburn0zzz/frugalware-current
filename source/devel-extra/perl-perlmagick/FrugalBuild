_F_perl_name=PerlMagick
_F_perl_author=J/JC/JCRISTY
pkgver=6.66
pkgrel=1
pkgdesc="Module to use the ImageMagick's libMagick methods"
archs=('i686')
depends=('imagemagick')
_F_perl_no_up2date="yes"
up2date="lynx -dump -nolist http://search.cpan.org/~jcristy/PerlMagick-6.66-1/Magick.pm.in | grep -m1 'tar.gz' | sed -e 's/.*"$_F_perl_name"-\(.*\).*/\1/' -e 's/-1.tar.gz//g'"
Finclude perl
sha1sums=('a133c30d96400baa8efd55aa2742c6b7027d87e7')

# optimization OK
