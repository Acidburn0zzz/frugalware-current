pkgname=reportlab
pkgver=2.1
realver=`echo $pkgver|sed 's/\./_/'`
pkgrel=2
pkgdesc="PDF Generation library"
url="http://www.reportlab.org"
depends=('python' 'imaging')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.reportlab.org/downloads.html | grep -m1 "ReportLab_[0-9].*\.tgz" | sed 's/.*ReportLab_//' | sed 's/\.tgz//' | sed 's/_/\./'"
source=(http://www.reportlab.org/ftp/ReportLab_$realver.tgz)
sha1sums=('67c6fba57b6372987dd3280be257a1f6bb7fff80')
_F_cd_path="${pkgname}_${realver}/${pkgname}"
