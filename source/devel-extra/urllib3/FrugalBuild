pkgname=urllib3
pkgver=1.22
pkgrel=1
pkgdesc="HTTP library with thread-safe connection pooling, file post, and more."
groups=('devel-extra')
archs=('x86_64')
depends=('python')
Finclude pypi
_F_cd_path="$_F_pypi_name-$pkgver"
sha1sums=('02c623c3bdceda4ef5bffdafdda472d2d9e34ef1')

build() {
        Fcd
        python setup.py install --root="$Fdestdir" || Fdie
}
