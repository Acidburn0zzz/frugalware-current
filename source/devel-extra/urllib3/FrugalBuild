pkgname=urllib3
pkgver=1.21.1
pkgrel=1
pkgdesc="HTTP library with thread-safe connection pooling, file post, and more."
groups=('devel-extra')
archs=('x86_64')
depends=('python')
Finclude pypi
_F_cd_path="$_F_pypi_name-$pkgver"
sha1sums=('f0112a21bede6876e502071a531f630d01be4ed5')

build() {
        Fcd
        python setup.py install --root="$Fdestdir" || Fdie
}
