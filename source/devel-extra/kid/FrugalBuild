pkgname=kid
pkgver=0.9.5
pkgrel=1
pkgdesc="A simple template language for XML based vocabularies written in Python."
url="http://kid.lesscode.org/"
depends=('setuptools' 'elementtree')
groups=('devel-extra')
archs=('i686')
up2date="lynx -dump $url |Flasttar"
source=(http://kid.lesscode.org/dist/$pkgver/kid-$pkgver.tar.gz)

build()
{
	Fcd
	rm -f makefile
	Fbuild
}
sha1sums=('3e7e32fcaabb1c1f8cf7a06ba72ac339310adc0a')
