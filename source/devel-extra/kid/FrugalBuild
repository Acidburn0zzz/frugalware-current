pkgname=kid
pkgver=0.9.6
pkgrel=4
pkgdesc="A simple template language for XML based vocabularies written in Python."
url="http://www.kid-templating.org/"
depends=('setuptools' 'python>=2.7')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url"
source=($url/dist/$pkgver/kid-$pkgver.tar.gz)
sha1sums=('8a6b7a2323d266dacea22898465cb4e6929af9e7')

build()
{
	Fcd
	rm -f makefile
	Fbuild
}
