# Last Modified: Sat, 16 Sep 2006 15:15:12 +0200
# Compiling Time: 0.10 SBU
# Maintainer: VMiklos <vmiklos@frugalware.org>

pkgname=javacup
pkgver=10k
pkgrel=1
pkgdesc="CUP Parser Generator for Java"
url="http://www.cs.princeton.edu/~appel/modern/java/CUP/"
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.cs.princeton.edu/~appel/modern/java/CUP/|grep gz$|sed 's/.*_v\(.*\)\.t.*/\1/;q'"
source=($url/java_cup_v$pkgver.tar.gz)
sha1sums=('79fda6316d3bbb9fee7dbd28301ca973abf140ff')
Finclude java

build()
{
	Fjavacleanup
	ecj java_cup/*.java java_cup/runtime/*.java || return 1
	find java_cup -name "*.class" |xargs jar -cvf javacup.jar
	Fjar javacup.jar
}

# optimization OK
