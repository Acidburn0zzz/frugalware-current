pkgname=gtk-doc
pkgver=1.27
pkgrel=1
pkgdesc="Used to document the public API of libraries"
url="http://www.gnome.org/"
depends=('openjade' 'docbook-xsl' 'yelp-tools' 'source-highlight' 'perl' 'python-six')
makedepends=('pkgconfig' 'itstool')
groups=('devel-extra')
archs=('x86_64')
_F_gnome_ext=".tar.xz"
_F_gnome_devel="y"
Finclude gnome
sha1sums=('4f8ae8921f474601f2f2023fc836fe7ca23cccfd')

build(){
	Fcd
	Fsed '$(datadir)/pkgconfig' '$(libdir)/pkgconfig' Makefile.{am,in}
	Fbuild
}


# optimization OK
