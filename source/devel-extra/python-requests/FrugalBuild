pkgname=python-requests
_F_archive_name="requests"
pkgver=2.11.1
pkgrel=1
pkgdesc="Python HTTP for Humans."
depends=('python>=2.7')
makedepends=('python-distribute')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_pypi_name="requests"
Finclude pypi
source=(https://pypi.python.org/packages/2e/ad/e627446492cc374c284e82381215dcd9a0a87c4f6e90e9789afefe6da0ad/requests-2.11.1.tar.gz)
sha1sums=('7083e52275a9bcc02757e8b53c0be07cb73149a1')

build() {
	Fcd
	python setup.py install --root="$Fdestdir" -O1 || Fdie
}
