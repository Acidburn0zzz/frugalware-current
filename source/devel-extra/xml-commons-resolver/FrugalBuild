# Last Modified: Sat, 16 Sep 2006 02:42:32 +0200
# Compiling Time: 0.10 SBU
# Maintainer: VMiklos <vmiklos@frugalware.org>

pkgname=xml-commons-resolver
pkgver=1.1
pkgrel=1
pkgdesc="Thirdparty libraries for xml-commons"
url="http://xml.apache.org/commons/"
groups=('devel-extra')
archs=('i686')
up2date="lynx -dump http://www.apache.org/dist/xml/commons/|grep resolver-[0-9]|Flasttar"
source=(http://www.apache.org/dist/xml/commons/xml-commons-resolver-$pkgver.tar.gz build.xml)
signatures=($source.sig '')
Finclude java

build()
{
	Fcd
	cp $Fsrcdir/build.xml ./
	Fjavacleanup
	Fant jar
	Fjar dist/*.jar
}

# optimization OK
