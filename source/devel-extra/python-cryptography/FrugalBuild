pkgname=python-cryptography
_F_archive_name="cryptography"
pkgver=1.6
pkgrel=1
pkgdesc="cryptography expose cryptographic recipes and primitives in a python module"
depends=('python-idna' 'python-cffi' 'pyasn1' 'python-six')
makedepends=('python-distribute')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_pypi_name="cryptography"
Finclude pypi
source=(https://pypi.python.org/packages/d7/a2/b90736c37fd720db425c5e48d69da75a6eff6609b22d2123762f1ae8c5f5/cryptography-1.6.tar.gz)
sha1sums=('da131a13f595daa9fa739baffb3fb05760a58fe9')

build() {
	Fcd
	python setup.py install --root=$Fdestdir || Fdie
}

# optimization OK
