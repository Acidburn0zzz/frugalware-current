# Compiling Time: 0.01 SBU
# Maintainer: Miklos Vajna <vmiklos@frugalware.org>

pkgname=docutils
pkgver=0.13.1
pkgrel=1
pkgdesc="Documentation Utilities"
Finclude sourceforge
depends=('python-distribute>=0.6.49-3')
groups=('devel-extra')
archs=('x86_64')
sha1sums=('c16e14ef18142fa248400cd174edb4fa40e51d5b')

build()
{
	Fbuild
	cd $Fdestdir/usr/bin
	for i in *.py
	do
		ln -s $i `basename $i .py`
	done
}

# optimization OK
