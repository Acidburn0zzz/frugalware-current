pkgname=python-eyed3
pkgver=0.6.10
pkgrel=2
pkgdesc="A Python module and program for processing information about mp3 files"
url="http://eyed3.nicfit.net/"
depends=('python')
license="GPL"
groups=('devel-extra')
archs=('i686')
up2date="lynx -dump http://eyed3.nicfit.net/|grep 'The current stable version is'|sed -e 's/.*is \(.*\)/\1/' -e 's|. .*||g'"
source=("http://eyed3.nicfit.net/releases/eyeD3-$pkgver.tar.gz")
sha1sums=('9c2c67ead8fc6005ba5d58ebd072273c54bdd818')

build () {
	Fcd eyeD3-$pkgver
	Fbuild
}
