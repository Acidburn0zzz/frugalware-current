pkgname=pycountry
pkgver=17.1.8
pkgrel=1
pkgdesc="ISO country, subdivision, language, currency and script definitions and their translations"
depends=('python>=2.7.12-8')
makedepends=('python-distribute')
groups=('devel-extra')
archs=('x86_64')
Finclude pypi
