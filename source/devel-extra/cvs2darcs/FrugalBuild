pkgname=cvs2darcs
pkgver=0.7.1
pkgrel=1
pkgdesc="Checks out a CVS repository module and converts it to use Darcs"
url="http://ab-initio.mit.edu/cvs2darcs/"
depends=('cvs' 'darcs' 'cvsps' 'perl-libwww')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url |grep gz$ |sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=($url/$pkgname-$pkgver.tar.gz)
sha1sums=('8cbef2c0b5232766e7230cc666b9793be124d998')

# optimalization ok - noarch

