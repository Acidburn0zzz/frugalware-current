# Compiling time: 0 SBU
# Maintainer: Priyank <priyankmg@gmail.com>
# Contributor: Rohan Dhruva <rohandhruva@gmail.com>

pkgname=python-logilab-astng
pkgver=0.16.2
pkgrel=1
pkgdesc="Python Abstract Syntax Tree New Generation."
url="http://www.logilab.org/projects/astng/project_view"
depends=('python-logilab-common')
groups=('devel-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.logilab.org/ftp/pub/astng | Flasttar"
source=(ftp://ftp.logilab.org/pub/astng/logilab-astng-$pkgver.tar.gz)
sha1sums=('5f59844e016e1a2c99d444fcf2945e4105b37318')
Finclude python
_F_cd_path="logilab-astng-$pkgver"

build()
{
	Fbuild
	# To resolve conflict between logilab common and astng.
	# The file __init__.py generated by both common and astng
	# is exactly the same, so we delete it.
	Frm $_F_python_libdir/logilab/__init__.py
}
