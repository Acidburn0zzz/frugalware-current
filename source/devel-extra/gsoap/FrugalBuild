# Compiling Time: 0.93 SBU
# Maintainer: VMiklos <vmiklos@frugalware.org>

pkgname=gsoap
pkgver=2.7.9
pkgrel=1
pkgdesc="A cross-platform development environment for C and C++ SOAP/XML Web services."
depends=('libstdc++')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_sourceforge_dirname="gsoap2"
Finclude sourceforge
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=52781|grep stable |sed 's/.*P \([^ ]*\) .*/\1/;q'"
source=(${source%gs*}/${pkgname}_$pkgver.tar.gz gsoap-2.7.9-gcc41.diff)

build()
{
	unset MAKEFLAGS
	Fcd $pkgname-${pkgver%.*}
	Fbuild
}

# optimization OK
sha1sums=('d7886ab18e88828c58548678f0c72e5575ac6086' \
	  '84cabaee90d617933907868bff1435feb6d57c46')
