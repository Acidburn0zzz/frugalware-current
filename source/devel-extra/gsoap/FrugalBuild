# Compiling Time: 0.93 SBU
# Maintainer: Miklos Vajna <vmiklos@frugalware.org>

pkgname=gsoap
pkgver=2.7.13
extrapkgver=
pkgrel=1
pkgdesc="A cross-platform development environment for C and C++ SOAP/XML Web services."
depends=('libstdc++')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_sourceforge_dirname="gsoap2"
Finclude sourceforge
source=(${source%gs*}/${pkgname}_$pkgver$extrapkgver.tar.gz)
sha1sums=('4c91ce860ee6853f785a11484c7298d294a53ef6')

build()
{
	unset MAKEFLAGS
	Fcd $pkgname-${pkgver%.*}
	Fbuild
}

# optimization OK
