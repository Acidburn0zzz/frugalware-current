groups=('x11' 'xorg-core' 'xorg-proto')
archs=('i686' 'x86_64')
depends=()
makedepends=('pkgconfig')
up2date="lynx -dump 'http://xorg.freedesktop.org/releases/individual/proto/?C=M;O=D' | grep ]$pkgname|sed -n -e 's/.*$pkgname-\(.*\)\.tar\.gz.*/\1/' -e '1 p'"
source=(http://xorg.freedesktop.org/releases/individual/proto/$pkgname-$pkgver.tar.gz)
sha1sums=('028e2783768c9e97177da0f6f81859a734524d1d')

