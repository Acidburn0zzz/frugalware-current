pkgname=xkbcomp
pkgver=1.0.5
pkgrel=1
pkgdesc="X.Org xkbcomp application"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-apps')
archs=('i686' 'x86_64' 'ppc')
depends=('libx11>=1.2.1' 'libxkbfile>=1.0.5')
makedepends=('inputproto>=1.5' 'util-macros>=1.2.1')
Finclude xorg
source=(${source[@]} fix-configrootpath.patch)
sha1sums=('9485bb7763ae06e48f12638deeed5b42abe61cbd' \
          'e528c8dee53724a7f573c35e908c3a16a16bfad2')

build(){
	Fcd
	Fpatchall
	Fautoreconf
	Fmake
	Fmakeinstall
}

# optimization OK
