pkgname=xkbcomp
pkgver=1.1.0
pkgrel=1
pkgdesc="X.Org xkbcomp application"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-apps')
archs=('i686' 'x86_64' 'ppc')
depends=('libx11>=1.2.99.901' 'libxkbfile>=1.0.5')
makedepends=('inputproto>=1.99.902' 'util-macros>=1.2.1')
Finclude xorg
sha1sums=('78d710d64e64df1894dd2b49ff9b0113b3af4746')

build(){
	Fcd
	Fpatchall
	Fautoreconf
	Fmake
	Fmakeinstall
}

