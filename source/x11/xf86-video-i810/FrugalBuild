pkgname=xf86-video-i810
pkgver=1.4.1.3
pkgrel=1
pkgdesc="X.Org driver for i810 cards"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-drivers')
archs=('i686' 'x86_64')
depends=('xorg-server' 'libxvmc')
makedepends=('randrproto' 'renderproto' 'videoproto' 'xf86driproto' 'xf86dgaproto' 'glproto')
Finclude xorg

build() {
	Fbuild --enable-dri
}

