pkgname=libxext
realname=libXext
rname=libXext
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org Xext library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'xextproto')
makedepends=('pkgconfig' 'xproto' 'inputproto')
Finclude xorg

build() {
	Fcd $rname-$pkgver
	Fbuild
}

# optimization OK

