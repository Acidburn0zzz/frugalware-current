pkgname=libxext
realname=libXext
rname=libXext
pkgver=1.0.0
pkgrel=1
pkgdesc="X.Org Xext library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'xextproto')
makedepends=('pkgconfig' 'xproto' 'inputproto')
Finclude xorg
sha1sums=('fe73e84d56a4c0886dc5c98217399bd7257a4ab5')

build() {
	Fcd $rname-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

