pkgname=libapplewm
realname=libAppleWM
pkgver=1.0.0
pkgrel=1
pkgdesc="X.Org AppleWM library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libxext')
makedepends=('inputproto' 'xextproto' 'applewmproto')
Finclude xorg

build()
{
	Fcd libAppleWM-$pkgver
	Fbuild
}

