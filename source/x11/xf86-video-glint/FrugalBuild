pkgname=xf86-video-glint
pkgver=1.0.1.3
pkgrel=1
pkgdesc="X.Org driver for glint cards"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-drivers')
archs=('i686' 'x86_64')
depends=('xorg-server')
makedepends=('randrproto' 'renderproto' 'xf86dgaproto' 'videoproto' 'xf86driproto' 'glproto')
Finclude xorg

build() {
	Fbuild --enable-dri
}

