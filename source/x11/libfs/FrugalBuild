# Maintainer: Christian Hamar alias krix <krics@linuxforum.hu>

pkgname=libfs
realname=libFS
pkgver=1.0.0
pkgrel=1
pkgdesc="X.Org FS library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('fontsproto' 'xproto' 'xtrans')
makedepends=('pkgconfig')
Finclude xorg

build() {
	Fcd libFS-$pkgver
	Fmake --enable-ipv6 --enable-unix-transport
	Fmakeinstall
}

# optimalization OK

# vim: ft=sh
sha1sums=('603ce6e8883b2629ff603f7709427b062be437fe')
