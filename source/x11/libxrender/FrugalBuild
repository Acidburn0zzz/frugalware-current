pkgname=libxrender
realname=libXrender
rname=$realname
pkgver=0.9.1
pkgrel=1
pkgdesc="X.Org Xrender library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'renderproto' 'xproto')
makedepends=('pkgconfig' 'inputproto')
Finclude xorg

build() {
	Fcd $rname-$pkgver
	Fbuild
}

# optimization OK

sha1sums=('170e1aca18f282018e8e675e453ece1779168ceb')
