pkgname=libxrender
realname=libXrender
rname=$realname
pkgver=0.9.0.2
pkgrel=1
pkgdesc="X.Org Xrender library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'renderproto' 'xproto')
makedepends=('pkgconfig' 'inputproto')
Finclude xorg
sha1sums=('6466ec00dea637c39dd89bd82f9f3851476d81a7')

build() {
	Fcd $rname-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

