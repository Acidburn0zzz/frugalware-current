pkgname=libxdmcp
realname=libXdmcp
rname=libXdmcp
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org Xdmcp library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('xproto')
makedepends=('pkgconfig')
Finclude xorg

build() {
	Fcd $rname-$pkgver
	Fbuild
}

