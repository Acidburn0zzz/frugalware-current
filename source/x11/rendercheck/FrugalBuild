depends=('libxrender')
makedepends=('inputproto')
Finclude xorg
# to be removed on next 'stable not mixed bump'
up2date="lynx -dump http://xorg.freedesktop.org/releases/individual/app/|grep $pkgname|Flasttarbz2"
source="http://xorg.freedesktop.org/releases/individual/app/$pkgname-$pkgver.tar.bz2"
_F_cd_path="$pkgname-$pkgver"
