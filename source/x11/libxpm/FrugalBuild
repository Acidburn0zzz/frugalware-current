pkgname=libxpm
realname=libXpm
pkgver=3.5.4.2
pkgrel=1
pkgdesc="X.Org Xpm library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxt' 'libxext')
makedepends=('pkgconfig' 'xproto' 'inputproto')
Finclude xorg

build() {
	Fcd libXpm-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

