pkgname=xkbdata
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org keyboard data"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-data')
archs=('i686' 'x86_64')
depends=('xkbcomp')
makedepends=('pkgconfig')
Finclude xorg

build() {
	Fbuild
	Fln ../../../../var/lib/xkb /usr/share/X11/xkb/compiled
}

# optimalization OK

