pkgname=libxrandr
realname=libXrandr
pkgver=1.1.1
pkgrel=1
pkgdesc="X.Org Xrandr library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxext' 'libxrender' 'randrproto' 'xproto' 'xextproto')
makedepends=('pkgconfig' 'renderproto' 'inputproto')
Finclude xorg

build() {
	Fcd libXrandr-$pkgver
	Fbuild
}

# optimization OK

sha1sums=('48fdb18667b9d9e1423a3dbb05be246fa6ec47b2')
