pkgname=libxres
realname=libXres
pkgver=1.0.0
pkgrel=1
pkgdesc="X.Org XRes library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxext' 'resourceproto')
makedepends=('pkgconfig' 'xproto' 'inputproto')
Finclude xorg

build() {
	Fcd libXres-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

