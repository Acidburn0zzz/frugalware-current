pkgname=libxaw
realname=libXaw
pkgver=1.0.2
pkgrel=1
pkgdesc="X.Org Xaw library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxt' 'libxpm' 'libxmu' 'libxp')
makedepends=('pkgconfig' 'xproto' 'ed' 'printproto' 'inputproto')
Finclude xorg

build() {
	Fcd libXaw-$pkgver
	Fbuild --enable-xaw8 --enable-xprint
}

# optimization OK

sha1sums=('d15a520d714a4125fa550cfdb391f8a03cd3e26d')

