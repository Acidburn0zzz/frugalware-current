pkgname=libxtrap
realname=libXTrap
pkgver=1.0.0
pkgrel=1
pkgdesc="X.Org Xext library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxt' 'libxext')
makedepends=('pkgconfig' 'trapproto' 'xextproto' 'inputproto')
Finclude xorg

build() {
	Fcd libXTrap-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

