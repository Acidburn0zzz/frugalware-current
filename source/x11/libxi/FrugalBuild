pkgname=libxi
realname=libXi
pkgver=1.0.0
pkgrel=1
pkgdesc="X.Org Xi library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxext' 'inputproto')
makedepends=('pkgconfig' 'xproto')
Finclude xorg
sha1sums=('a423bbd03c06ca87e04d9ef2cdddedd2b9a7c59c')

build() {
	Fcd libXi-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

