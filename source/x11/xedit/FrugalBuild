pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org xedit application"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-apps')
archs=('i686' 'x86_64')
depends=('libxprintutil' 'libxaw')
Finclude xorg

build() {
	Fbuild --enable-xprint
}

