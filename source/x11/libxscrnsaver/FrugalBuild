pkgname=libxscrnsaver
realname=libXScrnSaver
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org XScrnSaver library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxext' 'scrnsaverproto')
makedepends=('pkgconfig' 'inputproto')
Finclude xorg

build() {
	Fcd libXScrnSaver-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

