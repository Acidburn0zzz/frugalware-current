pkgname=xman
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org xman application"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-apps')
archs=('i686' 'x86_64')
depends=('libxprintutil' 'libxaw')
makedepends=('man')
Finclude xorg

build() {
	Fbuild --enable-xprint
}

