pkgname=libsm
realname=libSM
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org SM library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('xproto' 'xtrans' 'libice')
makedepends=('pkgconfig')
Finclude xorg

build() {
	Fcd libSM-$pkgver
	Fbuild --enable-IPv6
}

# optimization OK

sha1sums=('1f03c03bb5fbe731ad3427e1eaa3115a7c3f2a82')

