pkgname=libxcursor
realname=libXcursor
pkgver=1.1.7
pkgrel=1
pkgdesc="X.Org Xcursor library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxfixes' 'libxrender')
makedepends=('pkgconfig' 'xproto' 'inputproto')
Finclude xorg
sha1sums=('22e8ceeea2b6a22898c6b6427e1a9a3bcb6fab27')

build() {
	Fcd libXcursor-$pkgver
	Fbuild
}

