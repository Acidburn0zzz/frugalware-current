pkgname=libxmu
realname=libXmu
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org Xmu library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxt' 'libxext')
makedepends=('pkgconfig' 'xproto' 'inputproto')
Finclude xorg

build() {
	Fcd libXmu-$pkgver
	Fbuild --enable-IPv6
}

# optimization OK

sha1sums=('4b0be07ad4593452cb8f5afe0f8ea52cc2718fdd')
