pkgname=libxmu
realname=libXmu
pkgver=1.0.0
pkgrel=1
pkgdesc="X.Org Xmu library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxt' 'libxext')
makedepends=('pkgconfig' 'xproto' 'inputproto')
Finclude xorg

build() {
	Fcd libXmu-$pkgver
	Fmake --enable-IPv6
	Fmakeinstall
}

# optimalization OK

