Finclude cross32
Fconfopts="	--with-noarch-pkgconfigdir=/usr/lib/pkgconfig"
F32confopts="     --with-noarch-pkgconfigdir=/usr/lib32/pkgconfig"
_F_cross32_delete=('usr/share/')

