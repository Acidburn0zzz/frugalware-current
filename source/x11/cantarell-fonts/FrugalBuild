# Compiling Time: 0.02 SBU
# Maintainer: Baste <baste@frugalware.org>
# Contributor: bouleetbil <bouleetbil@frogdev.info>

pkgname=cantarell-fonts
pkgver=0.0.20
pkgrel=1
pkgdesc="Contemporary humanist sans-serif font"
groups=('x11' 'xorg-core' 'xorg-fonts')
archs=('i686' 'x86_64' 'arm')
_F_gnome_devel="n"
_F_fonts_subdir="cantarell"
_F_gnome_ext=".tar.xz"
Finclude gnome fonts
makedepends=(${makedepends[@]} 'font-util>=1.3.1-2')
sha1sums=('12ca0bb5136ea943eb9c5d35af474a182d141e75')

build() {
	cd $pkgname-$pkgver || Fdie
	Fmake
	rm -f *.bdf || Fdie
	cd .. || Fdie
	Fbuild_fonts
}

# optimization OK
