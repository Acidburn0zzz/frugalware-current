pkgname=libice
realname=libICE
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org ICE library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('xproto' 'xtrans')
makedepends=('pkgconfig')
Finclude xorg

build() {
	Fcd libICE-$pkgver
	Fbuild --enable-IPv6
}

# optimization OK

sha1sums=('2b7b19c4cb46eba5e2079fb00ef223c4cdd2b341')
