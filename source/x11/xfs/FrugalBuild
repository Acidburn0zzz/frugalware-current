pkgname=xfs
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org xfs application"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-apps')
archs=('i686' 'x86_64')
depends=('libfs' 'libxfont')
Finclude xorg

build() {
	Fbuild --libdir=/etc
}

