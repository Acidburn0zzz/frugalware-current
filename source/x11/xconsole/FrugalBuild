pkgname=xconsole
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org xconsole application"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-apps')
archs=('i686' 'x86_64')
depends=('libxaw' 'libxp')
Finclude xorg

build() {
	Fbuild --enable-xprint
}

