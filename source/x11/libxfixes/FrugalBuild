pkgname=libxfixes
realname=libXfixes
pkgver=4.0
pkgrel=1
pkgdesc="X.Org Xfixes library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'fixesproto>=4.0' 'xproto' 'xextproto')
makedepends=('pkgconfig' 'inputproto')
Finclude xorg
sha1sums=('bbbf4384d3782193f3ea680747bef468b93c997b')

build() {
	Fcd libXfixes-$pkgver
	Fmake
	Fmakeinstall
}

# optimization OK
