pkgname=libxv
realname=libXv
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org Xv library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxext' 'videoproto')
makedepends=('pkgconfig' 'xproto' 'inputproto')
Finclude xorg

build() {
	Fcd libXv-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

