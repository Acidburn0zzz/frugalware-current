pkgname=libxt
realname=libXt
pkgver=1.0.0
pkgrel=1
pkgdesc="X.Org Xt library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libsm' 'xproto')
makedepends=('pkgconfig' 'inputproto')
Finclude xorg

build() {
	Fcd libXt-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

