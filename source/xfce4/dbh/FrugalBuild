# XFCE Build Environment - dbh
# Last modified: Fri, 25 Mar 2005 07:51:37 +0100
# Build time: 8.634u
# Maintainer: Krisztian VASAS <iron@ironiq.hu>
pkgname=dbh
pkgver=1.0.22
pkgrel=2
xfcever=4.2.1.1
official=1
pkgdesc="A library to create Disk Based Hashtables on POSIX systems"
url="http://dbh.sourceforge.net/"
depends=('glibc')
groups=('xfce4' 'xfce4-core')
[ -e ../up2date.xfce ] && . ../up2date.xfce
source=(http://www.us.xfce.org/archive/xfce-$xfcever/src/$pkgname-$pkgver.tar.gz)

build() {
  cd $startdir/src/$pkgname-$pkgver
  ./configure --prefix=/usr
  make || return 1
  make DESTDIR=$startdir/pkg install
}
md5sums=('2dfd35a284068ed0e5a545d18f9bf0f2')
