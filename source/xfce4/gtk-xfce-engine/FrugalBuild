# XFCE Build Environment - gtk-xfce-engine
# Last modified: Mon, 16 May 2005 22:19:48 +0200
# Build time: 10.300u
# Maintainer: Krisztian VASAS <iron@ironiq.hu>
pkgname=gtk-xfce-engine
pkgver=2.2.7
pkgrel=1
xfcever=4.2.2
official=1
pkgdesc="A port of Xfce4 engine to GTK+-2.0"
url="http://www.xfce.org/"
groups=('xfce4-core')
archs=('i686')
depends=('gtk+2')
[ -e ../up2date.xfce ] && . ../up2date.xfce
source=(http://public.fh-wolfenbuettel.de/~luedickj/xfce4/xfce-$xfcever/src/$pkgname-$pkgver.tar.gz)

build() {
  cd $startdir/src/$pkgname-$pkgver
  ./configure --prefix=/usr
  make || return 1
  make DESTDIR=$startdir/pkg install
}
md5sums=('f9d592aa714bb003b6b012aa9002bbf7')
