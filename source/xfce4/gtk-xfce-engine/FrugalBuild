# XFCE Build Environment - gtk-xfce-engine
# Last modified: Fri, 25 Mar 2005 08:07:18 +0100
# Build time: 10.300u
# Maintainer: Krisztian VASAS <iron@ironiq.hu>
pkgname=gtk-xfce-engine
pkgver=2.2.6
pkgrel=2
xfcever=4.2.1.1
official=1
pkgdesc="A port of Xfce4 engine to GTK+-2.0"
url="http://www.xfce.org/"
groups=('xfce4-core')
depends=('gtk+2')
[ -e ../up2date.xfce ] && . ../up2date.xfce
source=(http://www.xfce.org/archive/xfce-$xfcever/src/$pkgname-$pkgver.tar.gz)

build() {
  cd $startdir/src/$pkgname-$pkgver
  ./configure --prefix=/usr
  make || return 1
  make DESTDIR=$startdir/pkg install
}
md5sums=('9c713ba355d0c108599c4a8abe13db47')
