# Build time: 8.838u
pkgname=xfce-mcs-manager
pkgver=4.2.2
pkgrel=2
xfcever=4.2.2
official=1
pkgdesc="A multi channel settings manager for Xfce4"
url="http://www.xfce.org/"
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
depends=('libxfcegui4' 'libxfce4mcs' 'startup-notification')
[ -e ../up2date.xfce ] && . ../up2date.xfce
source=(http://public.fh-wolfenbuettel.de/~luedickj/xfce4/xfce-$xfcever/src/$pkgname-$pkgver.tar.gz)

build() {
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild || return 1
}
md5sums=('9f6cdb3f11fc57a458fe7c91540181c9')
