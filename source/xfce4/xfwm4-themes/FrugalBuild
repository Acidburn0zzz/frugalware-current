# XFCE Build Environment - xfwm4-themes
# Last modified: Mon, 16 May 2005 22:34:34 +0200
# Maintainer: Krisztian VASAS <iron@ironiq.hu>
# Build time: 12.245u
pkgname=xfwm4-themes
pkgver=4.2.2
pkgrel=1
xfcever=4.2.2
official=1
pkgdesc="A set of additionnal themes for the Xfce4 window manager"
url="http://www.xfce.org/"
groups=('xfce4' 'xfce4-core')
depends=('xfwm4')
[ -e ../up2date.xfce ] && . ../up2date.xfce
source=(http://public.fh-wolfenbuettel.de/~luedickj/xfce4/xfce-$xfcever/src/$pkgname-$pkgver.tar.gz)

build() {
  cd $startdir/src/$pkgname-$pkgver
  ./configure --prefix=/usr
  make || return 1
  make DESTDIR=$startdir/pkg install
}
md5sums=('440977408bd1395f4464f18f01faf644')
