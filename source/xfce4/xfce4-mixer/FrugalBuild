# XFCE Build Environment - xfce4-mixer
# Last modified: Wed, 31 Aug 2005 11:58:29 +0200
# Build time: 0.36 SBU
# Maintainer: Krisztian VASAS <iron@ironiq.hu>

pkgname=xfce4-mixer
pkgver=4.2.2
pkgrel=2
pkgdesc="The volume control plugin for the Xfce4 panel"
url="http://www.xfce.org/"
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
depends=('libxml2' 'libxfcegui4' 'startup-notification' 'libxfce4mcs')
makedepends=('xfce4-panel')
Finclude xfce4
source=($dlurl/$pkgname-$pkgver.tar.gz)
sha1sums=('8538d82910cb1699ad368c64469d25d26b65b74d')

build() {
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild || return 1
}

# optimalization OK

