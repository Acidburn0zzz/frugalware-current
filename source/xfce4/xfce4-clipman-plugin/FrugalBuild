# XFCE Build Environment - xfce4-clipman-plugin
# Last modified: Wed, 08 Jun 2005 13:16:29 +0000
# Maintainer: Krisztian VASAS <iron@ironiq.hu>
# Build time: 6.030u
pkgname=xfce4-clipman-plugin
pkgver=0.4.1
pkgrel=2
official=0
goodies=1
pkgdesc="A clipboard plugin for the Xfce4 panel"
url="http://xfce-goodies.berlios.de/"
groups=('xfce4' 'xfce4-goodies')
archs=('i686' 'x86_64')
depends=('xfce4-panel')
[ -e ../up2date.xfce ] && . ../up2date.xfce
source=(http://download.berlios.de/xfce-goodies/$pkgname-$pkgver.tar.gz)

build() {
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild || return 1
}
md5sums=('664f6ca8500d8f240289c27d7bf4f419')
