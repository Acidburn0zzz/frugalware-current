pkgname=xfdesktop
pkgver=4.5.92
pkgrel=1
pkgdesc="A desktop manager for Xfce4."
depends=('libxml2' 'libxfcegui4>=4.5.92' 'xfconf>=4.5.92' \
	'dbus' 'thunar>=0.9.92' 'libexo>=0.3.92' 'libglade' \
	'libxfce4menu>=4.5.92' 'libwnck')
makedepends=('xfce4-panel>=4.5.92')
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
_F_gnome_iconcache=y
Finclude xfce4 gnome-scriptlet
source=(${source[@]} xfce4-fw.png)
sha1sums=('733617ac126669526cc7c89534203d34c17ee745' \
          '5cc04b415cf32353333106fb200244765f86b844')

build()
{
	Fbuild
	Ficon xfce4-fw.png
	Fbuild_gnome_scriptlet
}

# optimization OK
