pkgname=xfdesktop
pkgver=4.6.2
pkgrel=1
pkgdesc="A desktop manager for Xfce4."
depends=('libxml2' 'libxfcegui4>=4.6.4-1' 'xfconf>=4.6.2-1' \
	'dbus' 'thunar>=1.0.2-1' 'libexo>=0.3.101-3' 'libglade>=2.6.4-3' \
	'libxfce4menu>=4.6.2-1' 'libwnck>=2.30.0-2')
makedepends=('xfce4-panel>=4.6.1-2' 'intltool')
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64' 'ppc')
_F_gnome_iconcache=y
Finclude xfce4 gnome-scriptlet
source=(${source[@]} xfce4-fw.png)
sha1sums=('cefcd1c1386d34386d4e900cbf88b7c24ef3bafb' \
          '5cc04b415cf32353333106fb200244765f86b844')

build()
{
	Fbuild
	Ficon xfce4-fw.png
	Fbuild_gnome_scriptlet
}

# optimization OK
