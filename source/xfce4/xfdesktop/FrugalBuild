pkgname=xfdesktop
pkgver=4.7.4
pkgrel=1
pkgdesc="A desktop manager for Xfce4."
depends=('libxml2' 'libxfce4ui>=4.7.5' 'xfconf>=4.7.4' \
	'dbus' 'thunar>=1.1.5' 'libexo>=0.5.5' 'libglade>=2.6.4-3' \
	'libxfce4menu>=4.6.2-2' 'libwnck>=2.30.0-2')
makedepends=('xfce4-panel>=4.6.1-2' 'intltool')
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64' 'ppc')
_F_gnome_iconcache=y
Finclude xfce4 gnome-scriptlet
source=(${source[@]} xfce4-fw.png)
sha1sums=('2fe732e3554963f513c2de2cf54c8445efc23d24' \
          '5cc04b415cf32353333106fb200244765f86b844')

build()
{
	Fbuild
	Ficon xfce4-fw.png
	Fbuild_gnome_scriptlet
}

# optimization OK
