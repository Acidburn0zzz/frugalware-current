pkgname=xfdesktop
pkgver=4.5.91
pkgrel=1
pkgdesc="A desktop manager for Xfce4."
depends=('libxml2' 'libxfcegui4>=4.5.91' 'xfconf>=4.5.91' \
	'dbus' 'thunar>=0.9.91' 'libexo>=0.3.91' 'libglade' \
	'libxfce4menu>=4.5.91' 'libwnck')
makedepends=('xfce4-panel>=4.5.91')
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
_F_gnome_iconcache=y
Finclude xfce4 gnome-scriptlet
source=(${source[@]} xfce4-fw.png)
sha1sums=('369970dd42414a583ffb7e63ef4272a751692300' \
          '5cc04b415cf32353333106fb200244765f86b844')

build()
{
	Fbuild
	Ficon xfce4-fw.png
	Fbuild_gnome_scriptlet
}

# optimization OK
