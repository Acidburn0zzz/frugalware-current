pkgname=xfce4-toys
pkgver=4.2.2
pkgrel=2
xfcever=4.2.2
official=1
pkgdesc="Utilities and additions for the Xfce4 panel"
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
depends=('libxfcegui4' 'startup-notification' 'xfce4-panel')
url="http://www.xfce.org/"
[ -e ../up2date.xfce ] && . ../up2date.xfce
source=(http://public.fh-wolfenbuettel.de/~luedickj/xfce4/xfce-$xfcever/src/$pkgname-$pkgver.tar.gz)

build() {
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild || return 1
}
md5sums=('b8f10d38a94622f38d4a064bcae6fdc6')
