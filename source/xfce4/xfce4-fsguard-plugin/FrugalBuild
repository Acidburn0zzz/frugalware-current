pkgname=xfce4-fsguard-plugin
pkgver=0.2.1
pkgrel=1
pkgdesc="File system usage monitor plugin for the Xfce4 panel"
url="http://xfce-goodies.berlios.de/"
groups=('xfce4' 'xfce4-goodies')
archs=('i686' 'x86_64')
depends=('xfce4-panel')
Finclude xfce4
up2date="0.2.1"
# The 0.3.0 version is development version, created for a developmen version of xfce4 panel
source=($dlurl/$pkgname-$pkgver.tar.gz)
sha1sums=('86ad7752fdab209cd460f2da562f979196a1e643')

build() {
	Fcd $pkgname-$pkgver
	Fbuild
}

