# XFCE Build Environment - xfce4-trigger-launcher
# Last modified: Wed, 31 Aug 2005 12:59:45 +0200
# Build time: 0.14 SBU
# Maintainer: Krisztian VASAS <iron@ironiq.hu>

pkgname=xfce4-trigger-launcher
pkgver=4.2.2
pkgrel=1
xfcever=4.2.2
official=1
pkgdesc="A trigger launcher for the Xfce4 panel"
url="http://www.xfce.org/"
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
depends=('xfce4-panel')
[ -e ../up2date.xfce ] && . ../up2date.xfce
source=(http://public.fh-wolfenbuettel.de/~luedickj/xfce4/xfce-$xfcever/src/$pkgname-$pkgver.tar.gz)
sha1sums=('a903753f8b1d5468bd9f7879cd7a45779c384898')

build() {
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild || return 1
}

# optimalization OK

