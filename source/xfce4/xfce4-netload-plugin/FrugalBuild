# XFCE Build Environment - xfce4-netload-plugin
# Last modified: Wed, 08 Jun 2005 13:34:48 +0000
# Maintainer: Krisztian VASAS <iron@frugalware.org>
# Build time: 0.06 SBU
pkgname=xfce4-netload-plugin
pkgver=0.3.3
pkgrel=1
pkgdesc="A netload plugin for the Xfce4 panel"
url="http://xfce-goodies.berlios.de/"
groups=('xfce4' 'xfce4-goodies')
archs=('i686' 'x86_64')
depends=('xfce4-panel')
Finclude xfce4
source=($dlurl/$pkgname-$pkgver.tar.bz2)

build() {
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild || return 1
}
sha1sums=('7f43826f7a5cc2256ac27657aa45fede2f2d764f')

# optimalization OK

