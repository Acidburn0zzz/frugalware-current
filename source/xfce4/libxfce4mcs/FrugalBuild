pkgname=libxfce4mcs
pkgver=4.2.3
pkgrel=1
pkgdesc="Multi-channel settings management support for Xfce4"
url="http://www.xfce.org/"
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
depends=('x' 'libxfce4util')
Finclude xfce4
source=($dlurl/$pkgname-$pkgver.tar.gz)
sha1sums=('e7e926cbc319a893d53c5dabed5f0651acb20f6e')

build()
{
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild
}

# optimalization OK

