# Last modified: Mon, 21 Nov 2005 13:42:43 +0100

pkgname=xfprint
pkgver=4.2.3
pkgrel=1
pkgdesc="A print dialog and a printer manager for Xfce4"
url="http://www.xfce.org/"
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
depends=('libxfce4mcs' 'libxfcegui4' 'cups' 'a2ps')
makedepends=('xfce-mcs-manager')
Finclude xfce4
source=($dlurl/$pkgname-$pkgver.tar.gz)
sha1sums=('c4f0c16904bdc8ca179e221094d0466b03b5fa29')

build()
{
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild || return 1
}

# optimalization OK

