_F_gnustep_name=GMines
pkgver=0.1
pkgrel=1
pkgdesc="The well-known minesweeper game for GNUstep"
Finclude gnustep
depends=('libobjc>=4.4.0-6' 'gnustep-gui>=0.16.0')
url="http://gnustep.it/marko/GMines/index.html"
groups=(${groups[@]} 'gnustep-games')
archs=('i686')
up2date=$pkgver
source=(http://gnustep.it/marko/$_F_gnustep_name/$_F_gnustep_name.tgz)
sha1sums=('39afd4bf78f02b8c97b2f268f2906197eb834ae3')
_F_cd_path=$_F_gnustep_name

