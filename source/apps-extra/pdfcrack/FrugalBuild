options+=('asneeded')

pkgname=pdfcrack
pkgver=0.12
pkgrel=1
pkgdesc="Password recovery tool for PDF-files"
depends=('glibc')
groups=('apps-extra')
archs=('i686' 'x86_64')
Finclude sourceforge
sha1sums=('e641448db98a6b7c7cfd2136104860e826ba428b')

build() {
  Fmake
  Fexerel /usr/bin/$pkgname
}
