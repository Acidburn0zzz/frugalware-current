#: rc.microcode_ctl:46
msgid "missing microcode data file! ($DATAFILE)"
msgstr "hiányzó microcode adat fájl! ($DATAFILE)"

#: rc.microcode_ctl:62
msgid "microcode device $DEVICE does not exists?"
msgstr "a microcode eszköz $DEVICE nem létezik?"

#: rc.microcode_ctl:66
msgid "$DEVICE not a character device?"
msgstr "a $DEVICE nem character eszköz?"

#: rc.microcode_ctl:76
msgid "kernel does not have CPU microcode device support"
msgstr "a kernel nem támogatja a CPU microcode eszközt"

#: rc.microcode_ctl:88
msgid "Removing microcode_ctl lock."
msgstr "microcode_ctl lock eltávolítása."

#: rc.microcode_ctl:90
msgid "You need to reboot to clear uploaded microcode."
msgstr "A microcode újraindításnál törlõdik."
