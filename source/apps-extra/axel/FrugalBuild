source=(http://alioth.debian.org/frs/download.php/2718/$pkgname-$pkgver.tar.bz2)
sha1sums=('be50169a622d70a365ef794b82b351e1da503a22')
Fconfopts="${Fconfopts[@]} --etcdir=/etc"
