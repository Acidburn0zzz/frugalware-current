pkgver=2.3
pkgrel=1
pkgdesc="Download accelerator"
url="http://axel.alioth.debian.org/"
depends=('glibc')
options=('scriptlet')
groups=('apps-extra')
archs=('i686' 'x86_64')
backup=('etc/axelrc')
