pkgname=unrtf
pkgver=0.20.2
pkgrel=1
pkgdesc="A command-line program which converts documents in Rich Text  format to HTML, LaTeX, PostScript, and other formats."
url="http://www.gnu.org/software/unrtf/unrtf.html"
depends=('glibc')
license="GPL"
groups=('apps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url|grep 'Last release'|sed 's/.*se \(.*\)>.*/\1/'"
source=(http://ftp.gnu.org/gnu/unrtf/${pkgname}_$pkgver.tar.gz)
_F_cd_path="${pkgname}_$pkgver"
signatures=("$source.sig")

# optimization OK
