url="http://http-fetcher.sourceforge.net"
depends=('http-fetcher')
groups=('apps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/downloads.html | Flasttar"
source=(http://cs.nmu.edu/~lhanson/fetch/dls/$pkgname-$pkgver.tar.gz)
