pkgname=fetch
pkgver=1.0
pkgrel=1
pkgdesc="fetch is a simple, fast, and flexible HTTP download tool built on the HTTP Fetcher library."
url="http://cs.nmu.edu/~lhanson/fetch/index.shtml"
depends=('http-fetcher')
groups=('apps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://cs.nmu.edu/~lhanson/fetch/index.shtml | grep tar.gz | sed 's/.*-\(.*\).t.*/\1/' | tail -n1"
source=(http://cs.nmu.edu/~lhanson/fetch/dls/$pkgname-$pkgver.tar.gz)
sha1sums=('edac1ee54b24bb9d094682743c193eaa47eb5a48')

# optimization OK
