pkgname=ykclient
pkgver=2.15
pkgrel=1
pkgdesc='Yubico YubiKey C client'
archs=('i686' 'x86_64')
groups=('apps-extra')
depends=('libyubikey')
makedepends+=('asciidoc' 'docbook-xsl' 'help2man')
_F_github_author="Yubico"
_F_github_name="yubico-c-client"
_F_github_ver="$pkgname-$pkgver"
_F_github_tag="y"
_F_archive_grepv="pre"
Finclude github
up2date="$up2date | cut -d '_' -f 2"
sha1sums=('cbe8ca74b1cd1fbd07589db51bb5ab8db1f85401')

build() {
	Fcd
	Fautoreconf
	Fbuild
}

# optimization OK
