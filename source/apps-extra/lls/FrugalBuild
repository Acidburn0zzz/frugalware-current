# Compiling Time: 0.01 SBU
# Maintainer: Slown <slown@frugalware.org>

pkgname=lls
pkgver=0.8
pkgrel=1
pkgdesc="(ls) command alternative, more colors and additional info."
url="http://code.google.com/p/lls"
depends=()
groups=('apps-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=(http://lls.googlecode.com/files/lls)
sha1sums=('74cb0c5c7790933055a3af688d00cdd1d69a4f4e')

build() {
	Fexe lls /usr/bin/$pkgname
}
