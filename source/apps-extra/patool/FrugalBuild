pkgname=patool
pkgver=0.19
pkgrel=1
pkgdesc="patool is portable command line archive file manager"
depends=('python')
groups=('apps-extra')
archs=('i686' 'x86_64')
_F_github_author="wummel"
Finclude github
sha1sums=('14dc5906b454e1b1d15ce8afd9c9709feb12cff3')

build() {
	Fcd
	python setup.py install --root=$Fdestdir || Fdie
}
