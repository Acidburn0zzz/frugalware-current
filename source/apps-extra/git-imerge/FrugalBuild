pkgname=git-imerge
pkgver=0.7.0
pkgrel=1
pkgdesc="Incremental merge and rebase for git"
groups=('apps-extra')
archs=('i686' 'x86_64')
rodepends=('git' 'python')
_F_github_author="mhagger"
_F_github_tag=y
Finclude github
sha1sums=('b3bab94743a79426ea79c1b5e503020ef7fbf2ec')

build() {
	Fcd
	Fsed '/usr/local' "$Fdestdir/usr" Makefile
	Fmkdir usr/bin
	Fbuild
}


# optimization OK
