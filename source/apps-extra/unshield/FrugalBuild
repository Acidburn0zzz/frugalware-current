pkgrel=1
pkgdesc="Extracts CAB files from InstallShield installers"
depends=('zlib')
_F_sourceforge_dirname="synce"
Finclude sourceforge
up2date="lynx -dump http://sourceforge.net/projects/synce/files/Unshield/| grep 'http.*synce/.*Unshield/.*/$'|sed 's|.*/\(.*\)/|\1|;q' "
groups=('apps-extra')
archs=('i686' 'x86_64')
sha1sums=('3e1197116145405f786709608a5a636a19f4f3e1')

