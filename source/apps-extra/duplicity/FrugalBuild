pkgname=duplicity
pkgver=0.6.09
pkgrel=2
pkgdesc="Encrypted bandwidth-efficient backup using the rsync algorithm."
url="http://duplicity.nongnu.org/"
depends=('python>=2.6.4' 'librsync>=0.9.6' 'gnupg' 'ncftp' 'python-boto>=2.0b1' 'python-gnupginterface>=0.3.2')
groups=('apps-extra')
archs=('i686' 'x86_64')
up2date="Flasttar http://savannah.nongnu.org/download/$pkgname"
#up2date=$pkgver
source=(http://savannah.nongnu.org/download/$pkgname/$pkgname-$pkgver.tar.gz)
sha1sums=('0347cdd1d5cc8512ac90b806ab1f80956c6c3e4a')

build() {
	Fcd
	python setup.py install --root=$Fdestdir --optimize=1 || Fdie
}

# optimization OK
