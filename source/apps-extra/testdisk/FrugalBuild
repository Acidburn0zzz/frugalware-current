up2date="lynx -dump 'http://www.cgsecurity.org/wiki/TestDisk_Download'|grep 'testdisk-\(.*\).tar.bz2$'|grep -v WIP | grep -v static | grep -v darwin|sed 's/.*-\(.*\).t.*/\1/'"
source=(http://www.cgsecurity.org/$pkgname-$pkgver.tar.bz2)
