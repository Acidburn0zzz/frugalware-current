groups=('apps-extra')
depends=('ykclient')
_F_github_author="Yubico"
_F_github_name="yubikey-personalization"
_F_github_ver="v$pkgver"
_F_github_tag="y"
_F_archive_grepv="pre"
Finclude github
up2date="$up2date | sed 's/v//'"
 _F_cd_path="$_F_github_name-$pkgver"
