pkgname=bashrun
pkgver=0.16
pkgrel=1
pkgdesc="A lite application launcher based on bash"
groups=('apps-extra')
depends=('xdotool')
archs=('i686' 'x86_64')
Finclude sourceforge
sha1sums=('4846aa188e348478c2874ab679619831b042f7a6')
build()
{
	Fcd
 	./install.sh --prefix=$Fdestdir/usr || Fdie
}
