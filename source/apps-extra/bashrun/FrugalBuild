pkgname=bashrun
pkgver=0.13
pkgrel=1
pkgdesc="A lite application launcher based on bash"
groups=('apps-extra')
depends=('xdotool')
archs=('i686' 'x86_64')
Finclude sourceforge
sha1sums=('28d601e824c3d2a7729d80e21327b90ad4b303d8')
build()
{
	Fcd
 	./install.sh --prefix=$Fdestdir/usr || Fdie
}
