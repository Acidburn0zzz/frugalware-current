pkgname=bashrun
pkgver=0.15
pkgrel=1
pkgdesc="A lite application launcher based on bash"
groups=('apps-extra')
depends=('xdotool')
archs=('i686' 'x86_64')
Finclude sourceforge
sha1sums=('42f8d35e31ef22f3dd2dc3136fa8227f4fb506ac')
build()
{
	Fcd
 	./install.sh --prefix=$Fdestdir/usr || Fdie
}
