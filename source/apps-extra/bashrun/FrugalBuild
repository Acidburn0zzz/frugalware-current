pkgname=bashrun
pkgver=0.14
pkgrel=1
pkgdesc="A lite application launcher based on bash"
groups=('apps-extra')
depends=('xdotool')
archs=('i686' 'x86_64')
Finclude sourceforge
sha1sums=('8e0d973fc2c4cf2532d11ff2c50e0933b7adaafd')
build()
{
	Fcd
 	./install.sh --prefix=$Fdestdir/usr || Fdie
}
