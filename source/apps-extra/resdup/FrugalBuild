pkgname=resdup
pkgver=0.0.2
pkgrel=1
pkgdesc="A commandline tool to remove duplicated lines from a file, with many sort options."
depends=('perl')
groups=('apps-extra')
archs=('i686' 'x86_64')
_F_googlecode_dirname="trizen"
Finclude googlecode
sha1sums=('7e936e4f299ecc96d1d1b36b2d555bd701d3bd24')

build() {
	Fexe $pkgname /usr/bin/$pkgname
}
