options+=('asneeded')

pkgname=fbgrab
pkgver=1.2
pkgrel=1
pkgdesc="A framebuffer screenshot program"
url="http://fbgrab.monells.se"
depends=('libpng>=1.4.3')
makedepends=('libpng' 'zlib')
groups=('apps-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url"
source="$url/fbgrab-1.2.tar.gz"
sha1sums=('41fb3a8820573aba547581d055210b93bc7dc1c7')
_F_cd_path=$pkgname

# optimization OK
