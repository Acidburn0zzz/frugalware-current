pkgname=tmux
pkgver=2.5
pkgrel=2
pkgdesc="A terminal multiplexer"
url="http://sourceforge.net/projects/tmux/"
depends=('ncurses>=6.0-12' 'libevent')
_F_github_tag="y"
Finclude github
groups=('apps-extra')
archs=('x86_64')
sha1sums=('f2d854fd4291b3795be9aead61d3156687315f0d')

build() {

	Fbuild
	Finstallrel 0644 /usr/share/$pkgname/example_tmux.conf
}

# optimization OK
