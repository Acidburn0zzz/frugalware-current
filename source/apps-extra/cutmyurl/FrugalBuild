pkgname=cutmyurl
pkgver=1.6
pkgrel=1
pkgdesc="Shortener url in command line"
url="http://ubunblox.servhome.org"
depends=('curl' 'xclip')
groups=('apps-extra')
archs=('i686' 'x86_64')
up2date="Flasttar http://ubunblox.free.fr/UbunBlox/AUR/"
source=(http://ubunblox.free.fr/UbunBlox/AUR/$pkgname-$pkgver.tar.gz)
sha1sums=('5c64fc128f1591094e344f5904d98e4ecd042811')

build()
{
	Fexe $pkgname usr/bin/$pkgname
}
