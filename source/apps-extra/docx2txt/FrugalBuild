pkgname=docx2txt
pkgver=0.4
pkgrel=1
pkgdesc="Extract text contents from .docx file."
_F_sourceforge_prefix="v"
_F_sourceforge_ext=".tgz"
Finclude sourceforge
depends=('perl')
groups=('apps-extra')
archs=('i686' 'x86_64')
sha1sums=('52ab509efb844e4df533c298fb08b28aa3bcfec6')

build()
{
	Fcd
	Fmakeinstall INSTALLDIR=$Fdestdir/$Fprefix/bin
}
