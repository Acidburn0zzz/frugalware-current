pkgname=docx2txt
pkgver=1.0
pkgrel=1
pkgdesc="Extract text contents from .docx file."
_F_sourceforge_ext=".tgz"
Finclude sourceforge
depends=('perl')
groups=('apps-extra')
archs=('i686' 'x86_64')
sha1sums=('109ec659769089779c352cb28a89cf400840791c')

build()
{
	Fcd
	Fmakeinstall INSTALLDIR=$Fdestdir/$Fprefix/bin
}
