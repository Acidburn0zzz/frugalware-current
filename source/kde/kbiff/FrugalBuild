archs=('i686' 'x86_64')
up2date="lynx -dump http://www.granroth.org/kbiff/ |grep kbiff.*tar |sed -n -e 's/.*f-\(.*\).t.*/\1/'  -e '1 p'"
source=(http://dl.sourceforge.net/sourceforge/kbiff/$pkgname-$pkgver.tar.bz2)
