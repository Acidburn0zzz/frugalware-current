pkgname=kdesdk
pkgver=3.4.3
pkgrel=1
pkgdesc="KDE Software Development Kit."
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.3/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.4.3')
up2date="lynx -dump http://www.kde.org/download/ |grep kdesdk|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('9aff17827728aca5e388e778484f3f576ecd52b3')

build() {
	Fbuild --disable-debug
}

