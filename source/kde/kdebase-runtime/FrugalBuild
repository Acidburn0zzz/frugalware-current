pkgname=kdebase-runtime
pkgrel=1
pkgdesc="Programs and libraries needed at runtime by other KDE4 applications"
groups=('kde')
archs=('i686' 'x86_64' 'ppc')
Finclude kde java
depends=('hicolor-icon-theme' "libqt3support>=$_F_kde_qtver" \
	'strigi>=0.7.2-2' 'oxygen-icons' 'samba' 'libgcc' 'libice' 'libxrender' 'libuuid' 'libxi'
	'alsa-lib' "libqtscript>=$_F_kde_qtver" 'libgl' 'sqlite3' "libqtxmlpatterns>=$_F_kde_qtver" \
	'xine-lib' 'clucene' 'e2fsprogs' 'libssh')
makedepends=("${makedepends[@]}" "kdepimlibs>=${_F_kde_ver}" 'openslp')
rodepends=("${rodepends[@]}" 'eject')
replaces=('ksudo')
source=("${source[@]}" 01-nepomukstrigiservice-disable-autostart.patch)
sha1sums=('56623a20d92d15921fe9a97247dc1f289c15ab69' \
          'ae0a25253cd18c2672291403952f65082dbfc930')

build()
{
	KDE_build
	Fln /usr/lib/kde4/libexec/kdesu /usr/bin/kdesu
}

