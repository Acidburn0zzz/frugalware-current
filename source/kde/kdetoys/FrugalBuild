pkgname=kdetoys
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Toys for KDE."
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.5.0')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('749d455c301930fa8ab7446aa2a4e777ae745c98')


build() 
{
	Fbuild --disable-dependency-tracking \
               --disable-debug \
               --enable-new-ldfalgs \
               --with-gnu-ld

}

