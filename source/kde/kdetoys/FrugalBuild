pkgname=kdetoys
pkgver=3.4.3
pkgrel=1
pkgdesc="Toys for KDE."
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.3/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.4.3')
up2date="lynx -dump http://www.kde.org/download/ |grep kdetoys|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('85865a9dda77012e79e3e676bf56b471460755dc')

build() {
	Fbuild --disable-debug
}

