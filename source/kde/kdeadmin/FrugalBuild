pkgname=kdeadmin
pkgver=3.4.3
pkgrel=1
pkgdesc="KDE Administration Programs."
url="http://www.kde.org"
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.3/src"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.4.3')
up2date="lynx -dump http://www.kde.org/download/ |grep kdeadmin|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('39ba98e6f08a068d0493d0307d83453d24f95d97')

build() {
	Fbuild --disable-debug --with-shadow --without-rpm
}

