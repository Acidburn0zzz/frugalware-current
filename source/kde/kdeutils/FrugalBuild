pkgname=kdeutils
pkgver=3.4.3
pkgrel=1
pkgdesc="KDE General Utilities."
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.3/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.4.3')
makedepends=('gnupg')
up2date="lynx -dump http://www.kde.org/download/ |grep kdeutils|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('095a31fd24ebb2f3933f0e321fbda62d0ff8df09')

build() {
	Fbuild --disable-debug
}

