pkgname=kdeutils
pkgver=3.4.2
pkgrel=2
pkgdesc="KDE General Utilities."
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.2/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.4.2')
makedepends=('gnupg')
up2date="lynx -dump http://www.kde.org/download/ |grep kdeutils|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('685223a74b0401901910242924220bea6debcf8a')

build() {
	Fbuild --disable-debug
}

