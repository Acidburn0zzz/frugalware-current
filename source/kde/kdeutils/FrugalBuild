# Compiling Time: 1.68 SBU
# Maintainer: crazy <malagarax@gmx.net>

pkgname=kdeutils
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="KDE General Utilities."
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686')
depends=('kdebase=3.5.0' 'python' 'gmp')
makedepends=('gnupg' 'xmms' 'doxygen' 'graphviz')
conflicts=('superkaramba')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('194d20c78d61b41dda6c3a6f55f0e10b4718d0a8')

build() 
{
     
	Fbuild CXXFLAGS="$CXXFLAGS -Wno-deprecated" \
               --disable-debug \
               --disable-dependency-tracking \
               --enable-new-ldflags \
                DO_NOT_COMPILE="khexedit" \
               --with-gnu-ld 
}

# vim: ft=sh

# optimalization OK
