pkgname=potracegui
pkgver=1.3.4
pkgrel=2
pkgdesc="Frontend for potrace"
url="http://potrace.sourceforge.net"
depends=('potrace' 'kdelibs')
groups=('kde-extra')
archs=('i686' 'x86_64')
options=('scriptlet')
Finclude sourceforge
sha1sums=('64af4d2519cd90bc436516b0e3eab51360dc52fc')

build (){
	Fcd
	echo 'OnlyShowIn=KDE;' >> src/potracegui.desktop
	Fbuild
}

# optimalization OK

