# Last Modified: Fri, 14 Oct 2005 21:11:19 +0200
# Compiling Time: ~2.83 SBU
# Maintainer: Kapolnasi Tamas <ktamas@tdc.hu>

pkgname=kdeaccessibility
pkgver=3.4.3
pkgrel=1
pkgdesc="KDE accessibility programs"
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.3/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.4.3')
up2date="lynx -dump http://www.kde.org/download/ |grep kdeaccessibility|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('96798c5e5357534b7401f570bfa5b97f72c2e2aa')

build() {
	cd $startdir/src/$pkgname-$pkgver
	./configure --prefix=/usr --disable-debug
	
	make || return 1
	
	make DESTDIR=$startdir/pkg install || return 1
}

# vim: ft=sh

# optimalization OK
