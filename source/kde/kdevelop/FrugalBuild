pkgname=kdevelop
pkgver=3.2.3
pkgrel=1
pkgdesc="The KDevelop integrated C/C++ application development environment"
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.3/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.4.3')
up2date="lynx -dump http://www.kde.org/download/ |grep kdevelop|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('b6bc6ccde4901de00880346391bd6cbc1e2e2e11')

build()
{
	Fbuild --disable-debug
	# Not needed...
	Frm /usr/kdevbdb
}

