pkgname=kdevelop
pkgver=3.2.1
pkgrel=3
pkgdesc="The KDevelop integrated C/C++ application development environment"
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.1/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.4.1')
up2date="lynx -dump http://www.kde.org/download/ |grep kdevelop|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2 $pkgname-$pkgver-gcc4.patch)
md5sums=('7a5e9f2fb8a9539a41541dba566a234a' '72bf5d9a24a821f9584ec59ccf1a2d6e')

build()
{
	Fbuild --disable-debug
	# Not needed...
	Frm /usr/kdevbdb
}

