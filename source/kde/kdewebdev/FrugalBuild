pkgname=kdewebdev
pkgver=3.4.2
pkgrel=2
pkgdesc="A web development tool for the K Desktop Environment."
url="http://www.kde.org"
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.2/src"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdelibs=3.4.2')
replaces=('quanta')
up2date="lynx -dump http://www.kde.org/download/ |grep kdewebdev|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('ae419846b45933a0aa0708e3323e76d8ff02f91a')

build() {
	Fbuild --disable-debug
}

