pkgname=smb4k
pkgver=0.6.3
pkgrel=1
pkgdesc="Smb4K is a SMB/CIFS share browser for KDE."
url="http://smb4k.berlios.de/"
depends=('qt' 'kdelibs' 'kdebase')
groups=('kde' 'kde-apps')
archs=('i686' 'x86_64')
up2date="lynx -dump http://smb4k.berlios.de/download.html |grep smb4k.*tar |sed -n -e 's/.*k-\(.*\).t.*/\1/;1 p'"
source=(http://download.berlios.de/smb4k/$pkgname-$pkgver.tar.gz)
sha1sums=('7b72c9b85410b6b0c74ee2ca8ea6c317282f2438')

build()
{
	Fbuild --disable-debug
}

# optimalization OK

