pkgname=kdeartwork
pkgver=3.5.0
pkgrel=1
kdever=3.5
pkgdesc="Additional artwork for KDE."
url="http://www.kde.org"
pkgurl="ftp://ftp.solnet.ch/mirror/KDE/stable/$kdever/src"
groups=('kde' 'kde-core')
archs=('i686')
depends=('kdebase=3.5.0')
up2date=$pkgver
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
sha1sums=('62052ec019384b70973ea8ace4491e0da7163e7c')

build() 
{
        Fbuild CXXFLAGS="$CXXFLAGS -Wno-deprecated" \
               --disable-debug \
               --disable-dependency-tracking \
               --with-dpms \
               --disable-warnings 	
}

# vim: ft=sh

# optimalization OK
