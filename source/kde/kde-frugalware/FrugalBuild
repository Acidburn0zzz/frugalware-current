pkgname=kde-frugalware
pkgver=0.4
pkgrel=1
pkgdesc="KDE artwork for Frugalware Linux."
url="http://ftp.frugalware.org/pub/other/kde/kde-frugalware/"
depends=()
options=('nodocs')
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
up2date="lynx -dump $url|Flasttar"
source=(http://ftp.frugalware.org/pub/other/kde/$pkgname/$pkgname-$pkgver.tar.bz2)
sha1sums=('b711893d4de437f4f418449baf71d4329c259672')

build()
{
	Fcd
	cp -Rad usr $Fdestdir

}

