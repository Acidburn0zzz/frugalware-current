options+=('asneeded')

pkgname=kdnssd
pkgrel=1
pkgdesc="KDNSSD is a DNS Service Discovery KDE."
groups=('kde' 'kde-minimal')
archs=('i686' 'x86_64')
Finclude kde
depends=("${depends[@]}" "libdnssd>=$_F_kde_ver")
makedepends=("${makedepends[@]}" "kdelibs>=$_F_kde_ver" "kdelibs-compiletime>=$_F_kde_ver")

# optimization OK
