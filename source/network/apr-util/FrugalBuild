# Maintainer: crazy <crazy@frugalware.org>

pkgname=apr-util
pkgver=1.3.10
pkgrel=1
pkgdesc="Apache Portable Runtime"
url="http://apr.apache.org/"
depends=('apr=1.4.2' 'db>=4.7.25' 'gdbm' 'expat')
groups=('network')
archs=('i686' 'x86_64' 'ppc')
up2date="Flasttar http://apache.mirror.iphh.net/apr/"
source=(http://apache.mirror.iphh.net/apr/$pkgname-$pkgver.tar.bz2)
Fconfopts="$Fconfopts --with-apr=/usr \
	--with-gdbm \
	--with-expat=/usr \
	--with-berkeley-db \
	--enable-layout=RedHat"
sha1sums=('9a720cf77f1d0fdc5b0f6ce6d656cc864d7d4833')

# optimization OK
