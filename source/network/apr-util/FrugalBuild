# Compiling Time: 0.09 SBU
# Maintainer: crazy <crazy@frugalware.org>

pkgname=apr-util
pkgver=1.2.10
pkgrel=1
pkgdesc="Apache Portable Runtime"
url="http://apr.apache.org/"
depends=('apr=1.2.9' 'db>=4.6.18' 'gdbm' 'expat')
groups=('network')
archs=('i686' 'x86_64')
up2date="lynx -dump http://apache.mirror.iphh.net/apr/|grep 'apr-util-\(.*\).tar.bz2$'|Flasttarbz2"
source=(http://apache.mirror.iphh.net/apr/$pkgname-$pkgver.tar.bz2)
Fconfopts="$Fconfopts --with-apr=/usr \
	--with-gdbm \
	--with-expat=/usr \
	--with-berkeley-db \
	--with-threads \
	--enable-layout=RedHat"
sha1sums=('2ef492de9c7d77b0b2d9b22b532c5e1256ebfd87')

# optimization OK
