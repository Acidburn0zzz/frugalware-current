pkgname=rrdtool
pkgver=1.2.11
pkgrel=1
pkgdesc="a program to generate fancy graphs from network usage or from any thing is able to meter"
url="http://people.ee.ethz.ch/~oetiker/webtools/rrdtool/pub"
depends=('glibc' 'gd' 'cgilib') 
groups=('network')
archs=('i686' 'x86_64')
up2date="lynx -dump $url| grep tar.gz$|sed -n 's/.*-\(.*\)\.t.*/\1/;2 p'"
source=($url/$pkgname-$pkgver.tar.gz)
md5sums=('d61c5755cb77207f9ad3584b26e8bf08')

