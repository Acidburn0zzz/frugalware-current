pkgname=mrtg
pkgver=2.15.1
pkgrel=1
pkgdesc="a program to generate beauty graphs from network usage"
url="http://oss.oetiker.ch/mrtg/"
depends=('glibc' 'gd') 
groups=('network')
archs=('i686' 'x86_64')
up2date="lynx -dump http://oss.oetiker.ch/mrtg/pub/?M=D|grep -m1 '.zip'|sed 's/.*-\(.*\).zip .*/\1/'"
source=($url/pub/$pkgname-$pkgver.tar.gz)
sha1sums=('9ebfe300e916c3e1529fd410d531877608e10131')

