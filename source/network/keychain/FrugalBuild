# Maintainer: voroskoi <voroskoi@frugalware.org>

pkgname=keychain
pkgver=2.5.4.1
pkgrel=1
pkgdesc="ssh-agent manager"
url="http://www.gentoo.org/proj/en/keychain/"
depends=('openssh' 'bash' 'grep')
groups=('network')
up2date="lynx -dump http://dev.gentoo.org/~agriffis/keychain/|grep keychain/keychain |grep tar |sed -n -e 's/.*n-\(.*\)\.t.*/\1/' -e '1 p'"
source=(http://dev.gentoo.org/~agriffis/keychain/$pkgname-$pkgver.tar.bz2)
#see http://dev.gentoo.org/~agriffis/keychain/
md5sums=('93498f35f8d4cf1abb1941587e68076b')

build() {
	Fcd	
	Fmkdir /usr/bin
	Fmkdir /usr/man/man1
	
	mv keychain $Fdestdir/usr/bin/
	mv keychain.1 $Fdestdir/usr/man/man1/
}

# optimalization OK - noarch

# vim: ft=sh
