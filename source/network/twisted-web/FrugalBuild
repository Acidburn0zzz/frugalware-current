pkgname=twisted-web
pkgver=0.5.0
pkgrel=1
pkgdesc="An HTTP protocol implementation together with clients and servers."
url="http://twistedmatrix.com/projects/web/"
depends=('twisted')
groups=('network')
archs=('i686' 'x86_64')
up2date="lynx -dump http://twistedmatrix.com/projects/web/|grep bz2$|sed 's/.*-\(.*\)\.t.*/\1/'"
source=(http://tmrc.mit.edu/mirror/twisted/Web/0.5/TwistedWeb-0.5.0.tar.bz2)
sha1sums=('144657d7e6ca6cbb2fd20b9a07e1a83544867099')

build() {
	Fcd TwistedWeb-$pkgver
	Fmakeinstall
}

# optimization OK
