pkgname=ncftp
pkgver=3.1.9
pkgrel=1
pkgdesc="Console based FTP client"
url="http://www.ncftp.com/"
depends=('ncurses')
groups=('network')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/download/|grep src.tar.gz$|sed -n 's/.*-\(.*\)-.*/\1/;1 p'"
source=(ftp://ftp.ncftp.com/ncftp/$pkgname-$pkgver-src.tar.bz2)
md5sums=('66cf8dacec848eb11a70632fe9f21807')

# optimalization ok

