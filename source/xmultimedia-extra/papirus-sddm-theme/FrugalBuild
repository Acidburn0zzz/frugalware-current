# Compiling Time: 0.50 SBU
# Maintainer: DeX77 <dex77@frugalware.org>

pkgname=papirus-sddm-theme
pkgver=0.1.0.g933e1b0
pkgrel=1
pkgdesc='Papirus theme for SDDM'
archs=('i686' 'x86_64')
groups=('xmultimedia-extra')
depends=('glibc')
rodepends=('sddm')
_F_github_author="PapirusDevelopmentTeam"
_F_github_devel=yes
Finclude github

build() {
        Funpack_scm
        Finstall 755 "$pkgname/papirus/*" /usr/share/sddm/themes/$pkgname/

}
# optimization OK
