# Compiling Time: 0.50 SBU
# Maintainer: DeX77 <dex77@frugalware.org>

pkgname=solarized_sddm_theme
pkgver=0.1.4
pkgrel=1
pkgdesc='theme is inspired by Numix sddm theme'
archs=('i686' 'x86_64')
groups=('xmultimedia-extra')
depends=('glibc')
rodepends=('sddm')
_F_github_author="MalditoBarbudo"
_F_github_tag=y
Finclude github
sha1sums=('36816f51ac3697369a519deede6bc6f54fe7372b')

build() {
        Fcd
        Finstall 755 "$pkgname-$pkgver/*" /usr/share/sddm/themes/$pkgname/

}
# optimization OK
