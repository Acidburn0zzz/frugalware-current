pkgname=abstractdark-sddm-theme
pkgver=0.1.0.ge817d4b
pkgrel=2
pkgdesc='Abstract Dark theme for SDDM'
archs=('x86_64')
groups=('xmultimedia-extra')
depends=('glibc')
rodepends=('sddm')
_F_github_author="3ximus"
_F_github_devel=yes
Finclude github

build() {
        Funpack_scm
        Finstall 755 "$pkgname/*" /usr/share/sddm/themes/$pkgname/

}
# optimization OK
