pkgname=gnome-common
pkgver=2.26.0
pkgrel=2
pkgdesc="Common macros for GNOME"
url="http://www.gnome.org/"
depends=('glibc')
groups=('gnome-extra')
archs=('i686' 'x86_64' 'ppc')
Finclude gnome
source=(${source[@]} gnome.sh fix_automake111.diff)

build() {
	Fbuild
	Fexe /etc/profile.d/gnome.sh
}

