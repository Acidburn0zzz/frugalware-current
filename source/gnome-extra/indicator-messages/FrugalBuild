pkgname=indicator-messages
pkgver=0.3.2
pkgrel=1
pkgdesc="A place on the user's desktop that collects messages that need a response."
url="https://launchpad.net/indicator-messages"
depends=('libindicator' 'libindicate')
makedepends=('intltool')
groups=('gnome-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url"
source=($url/0.3/$pkgver/+download/$pkgname-$pkgver.tar.gz)
Fconfopts="${Fconfopts[@]} --disable-static"
sha1sums=('dcaf44c20ff0d85d6bd13e65bfb4c0ab1bb4851b')

