pkgname=gtk+2-engines-ubuntulooks
pkgver=0.9.11
pkgrel=1
pkgdesc="Ubuntulooks is the Ubuntu theme engine"
url="http://www.ubuntu.com"
depends=('gtk+2>=2.10.0')
groups=('gnome-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://archive.ubuntu.com/ubuntu/pool/main/u/ubuntulooks/ | grep orig | sed 's/_/-/g' | Flasttar | sed 's/.orig//'"
source=(http://archive.ubuntu.com/ubuntu/pool/main/u/ubuntulooks/ubuntulooks_$pkgver.orig.tar.gz)
sha1sums=('dcc3790ddb62afeb6b49b319eebd0ff2ffe04e3d')
_F_cd_path="ubuntulooks-$pkgver"
Fconfopts="$Fconfopts --enable-animation"

# optimization OK
