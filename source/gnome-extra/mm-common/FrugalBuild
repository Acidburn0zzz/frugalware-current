pkgname=mm-common
pkgver=0.9.10
pkgrel=2
pkgdesc="Common build files of the C++ bindings"
groups=('gnome-extra')
archs=('i686' 'x86_64')
depends=('libsigc++2')
makedepends=('curl')
_F_gnome_ext=".tar.xz"
_F_gnome_devel="y"
Finclude gnome
sha1sums=('72625160de9249cf7df86a1f860b5d4db94f0d6e')

build(){
	Fcd
	Fsed '$(datadir)/pkgconfig' '$(libdir)/pkgconfig' Makefile.{am,in}
	Fbuild
}

# optimization OK
