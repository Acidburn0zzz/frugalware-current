pkgname=gnome-js-common
pkgver=0.1.1
pkgrel=2
pkgdesc="Common JavaScript modules for GNOME."
depends=('glibc')
if [ $CARCH != "ppc" ]; then
	depends=(${depends[@]} 'seed')
fi
makedepends=('gtk-doc' 'intltool')
groups=('gnome-extra')
archs=('i686' 'x86_64' 'ppc')
options=('scriptlet')
_F_gnome_devel="y"
Finclude gnome
sha1sums=('0297b768f5a91b8d9e4bbc6e7b9c73f3e7f915ba')

# optimization OK
