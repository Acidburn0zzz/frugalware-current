_F_gnome_schemas=('/etc/gconf/schemas/indicator-applet.schemas')
Finclude gnome-scriptlet
up2date="Flasttar $url"
source=($url/0.3/$pkgver/+download/$pkgname-$pkgver.tar.gz)
