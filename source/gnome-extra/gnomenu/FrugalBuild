pkgname=gnomenu
pkgver=2.6.1
pkgrel=1
pkgdesc="A consolidated menu for gnome that brings eye candy to the world of the Gnome menus."
url="https://launchpad.net/gnomenu"
depends=('python>=2.6' 'pyxdg' 'pycairo' 'python-xlib' 'deskbar-applet')
groups=('gnome-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url"
source=(http://launchpad.net/gnomenu/trunk/2.6/+download/$pkgname-$pkgver.tar.gz)
sha1sums=('de3f3e9d6f742a508f88a6ed7234a7d9e9aa3cf9')

build() {
  Fcd $pkgname
  Fmakeinstall
}
