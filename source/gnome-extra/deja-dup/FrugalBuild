pkgname=deja-dup
pkgver=34.1
pkgrel=3
pkgdesc="Déjà Dup is a simple backup program and uses duplicity as the backend."
depends=('gtk+3' 'gnome-keyring' 'libpeas' 'libnotify' 'libsecret-vala' 'packagekit-glib')
makedepends=('gnome-doc-utils' 'intltool' 'vala' 'elinks' 'help2man' 'itstool' 'nautilus' 'x11-protos')
rodepends=('duplicity')
groups=('gnome-extra')
archs=('i686' 'x86_64')
_F_launchpad_ext=".tar.xz"
_F_launchpad_branch="34"
_F_gnome_iconcache="y"
_F_gnome_glib="y"
Finclude launchpad gnome-scriptlet cmake
#source=("$source" cmake-fix.patch)
sha1sums=('41cd6a3f09a1e9e552f5baa1f1d8df68193db68e')

build() {
	CMake_build
	Fbuild_gnome_scriptlet
	Fmv usr/etc etc
}

# optimization OK
