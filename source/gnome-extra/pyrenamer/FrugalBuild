pkgname=pyrenamer
pkgver=0.5.0
pkgrel=1
pkgdesc="A mass file renamer written in PyGTK."
url="http://www.infinicode.org/code/pyrenamer/"
depends=('pygtk>=2.12.1' 'gconf>=2.20.1' 'python-eyed3')
makedepends=('perl-xml-parser')
groups=('gnome-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url | grep -m1 tar.gz | Flasttar"
source=($url/$pkgname-$pkgver.tar.gz)
_F_gnome_schemas=('/etc/gconf/schemas/pyrename-schemas')
Finclude gnome-scriptlet
sha1sums=('db918ff266713816eb468c2d1d2c352e38580970')
