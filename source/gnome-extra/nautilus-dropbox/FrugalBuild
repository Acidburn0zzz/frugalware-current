pkgname=nautilus-dropbox
pkgver=0.6.4
pkgrel=1
pkgdesc="Dropbox for Linux - Nautilus extension"
url="https://www.dropbox.com"
depends=('libnotify' 'nautilus' 'pygtk')
makedepends=('docutils')
options=('scriptlet')
groups=('gnome-extra')
archs=('i686' 'x86_64')
_F_gnome_iconcache="y"
_F_gnome_desktop="y"
Finclude gnome-scriptlet
up2date="Flasttar $url/downloading?os=lnx"
source=($url/download?dl=packages/$pkgname-$pkgver.tar.bz2)
sha1sums=('b6d67a31e8b7d36dd4a626ddb419fc295168d798')

# optimization OK
