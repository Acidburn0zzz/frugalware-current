pkgname=parcelle
pkgver=1.0.1
pkgrel=1
pkgdesc="A PyGTK application that captures and saves clipboard history."
url="http://code.google.com/p/xyhthyx"
archs=('i686')
groups=('gnome-extra')
rodepends=('pygtk' 'pyxml')
up2date="lynx -dump $url/downloads/list | grep -v svn | Flasttar"
source=(http://xyhthyx.googlecode.com/files/$pkgname-$pkgver.tar.gz)
sha1sums=('b2ffcd21e835e842bd3ed2f94136c1b6c2eeba65')
_F_cd_path=$pkgname
