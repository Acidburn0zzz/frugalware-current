pkgname=gok
pkgver=2.28.0
pkgrel=1
pkgdesc="a full featured on-screen keyboard for Unix and Unix-like operating systems."
url="http://www.gnome.org/"
depends=('gconf>=2.28.0' 'libusb' 'libxevie' 'libgnomeui' 'libglade' 'libwnck' 'gail' 'gnome-speech' 'at-spi' \
	'libcanberra-gtk')
makedepends=('intltool' 'rarian')
rodepends=('rarian')
groups=('gnome-extra')
archs=('i686' 'x86_64')
_F_gnome_scrollkeeper="y"
_F_gnome_schemas=('/etc/gconf/schemas/gok.schemas')
_F_gnome_iconcache="y"
Finclude gnome gnome-scriptlet
Fconfopts="$Fconfopts --enable-libusb-input"
sha1sums=('3a46035cb781bcb330b1881ccdaad1cad3febc56')

