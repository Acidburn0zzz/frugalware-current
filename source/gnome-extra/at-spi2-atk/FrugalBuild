pkgname=at-spi2-atk
pkgver=1.91.6
pkgrel=1
pkgdesc="Gnome Accessibility Toolkit"
depends=('atk' 'dbus-glib' 'gconf' 'gtk+3>=2.99.3')
makedepends=('perl-xml-parser' 'intltool')
groups=('gnome-extra')
archs=('i686' 'x86_64' 'ppc')
_F_gnome_schemas=('/etc/gconf/schemas/at-spi2.schemas')
Finclude gnome gnome-scriptlet
Fconfopts="$Fconfopts --disable-gtk-doc"
sha1sums=('5786e86a3ca49da55865731b0d3e8e7de7acb902')

build() {
	Fbuild
	#provided by at-spi for gtk+2
	Frm usr/lib/gtk-2.0
	Fbuild_gnome_scriptlet
}

# optimization OK
