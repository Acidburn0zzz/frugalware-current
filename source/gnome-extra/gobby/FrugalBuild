pkgname=gobby
pkgver=0.4.1
pkgrel=2
pkgdesc="Gobby is a collaborative text editor"
url="http://releases.0x539.de/gobby/"
depends=('obby' 'libgnomeprint>=2.12.1-2' 'gtkmm>=2.10.1' 'gtksourceview' 'libxml++' 'gnome-vfs>=2.15.92')
groups=('gnome-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url | sed '/latest/d' | sed '/stable/d' | Flasttar"
source=($url/$pkgname-$pkgver.tar.gz)
sha1sums=('2d4d9746231de413fcb05a09fd1427400acc6121')
_F_gnome_desktop=y
Finclude gnome-scriptlet
Fconfopts="$Fconfopts --with-gnome"

# optimization OK
