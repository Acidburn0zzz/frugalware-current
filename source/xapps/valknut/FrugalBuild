pkgname=valknut
pkgver=0.3.7
pkgrel=1
pkgdesc="Direct Connect 4 Linux File Sharing program."
url="http://dcgui.berlios.de/"
makedepends=('libxml2' 'qt' 'dclib')
depends=('glibc' 'qt' 'freetype2' 'x' 'libxml2' 'fontconfig' 'expat' 'bzip2' 'openssl' 'zlib' 'gcc'
'dclib' 'libpng')
groups=('xapps')
up2date="lynx -dump http://developer.berlios.de/project/showfiles.php?group_id=370 |grep valknut.*tar |sed -n -e 's/.*t-\(.*\)\.t.*/\1/;1 p'"
source=(http://download.berlios.de/dcgui/$pkgname-$pkgver.tar.bz2)
md5sums=('848f9b3f25af15c3f1837133ac4b9415')

build() {
	Fmake
	Fmakeinstall
}

