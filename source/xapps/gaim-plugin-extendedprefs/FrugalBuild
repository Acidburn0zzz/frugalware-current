pkgname=gaim-plugin-extendedprefs
pkgver=0.5
pkgrel=1
pkgdesc="Gaim Extended Preferencies Plugin"
_F_sourceforge_name="gaim-extprefs"
Finclude sourceforge
depends=('gaim')
groups=('xapps')
archs=('i686' 'x86_64')
sha1sums=('de37f0745fdc24cb0ed66fb480d45c38ff6170f5')

build() {
	Fcd $_F_sourceforge_name-$pkgver
	Fmake
	Fmakeinstall
}

# optimization OK
