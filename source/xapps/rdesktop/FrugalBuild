pkgname=rdesktop
pkgver=1.4.0
pkgrel=1
pkgdesc="A Remote Desktop Protocol Client"
url="http://www.rdesktop.org"
depends=('glibc' 'x' 'openssl')
groups=('xapps')
up2date=`lynx -dump 'http://prdownloads.sourceforge.net/rdesktop/?sort_by=date&sort=desc'|grep .tar.gz$|sed -ne 's/.*p-\(.*\)\.t.*/\1/;1 p'`
source=(http://download.sourceforge.net/$pkgname/$pkgname-$pkgver.tar.gz)
md5sums=('542cb9d9b4dd1ecaf3ed4ff753fea7b8')

