pkgname=gqview
pkgver=2.0.0
pkgrel=1
pkgdesc="An image browser"
url="http://gqview.sourceforge.net/"
depends=('gtk+2')
groups=('xapps')
up2date=`lynx -dump http://gqview.sourceforge.net | grep "(stable)" | sed -n "s/.*GQview \(.*\) (stable).*/\1/; 1 p"`
source=(http://dl.sourceforge.net/sourceforge/$pkgname/$pkgname-$pkgver.tar.gz)

