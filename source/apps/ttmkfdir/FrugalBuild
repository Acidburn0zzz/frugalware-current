pkgname=ttmkfdir
pkgver=3.0.9
pkgrel=1
pkgdesc="A fonts.scale generation utility"
url="http://ftp.frugalware.org/pub/other/$pkgname/"
depends=('freetype2' 'gcc' 'zlib')
up2date="lynx -dump $url|grep tar.bz2$|sed -n 's/.*-\(.*\)\.t.*/\1/;$ p'"
archs=('i686' 'x86_64')
groups=('apps')
source=(http://ftp.frugalware.org/pub/other/$pkgname/$pkgname-$pkgver.tar.bz2 \
	$pkgname-$pkgver-big.patch)
md5sums=('7fbf13e432aa519669899b13d00673f3' 'c8acc05d0159edcb8b0a0fdd8cbc86bc')

