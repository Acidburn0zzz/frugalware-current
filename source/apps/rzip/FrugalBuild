pkgname=rzip
pkgver=2.0
pkgrel=1
pkgdesc="A large-file compression program"
url="http://rzip.samba.org"
groups=('apps')
depends=('bzip2')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/ftp/rzip/ |grep \"]rzip\"|sed 's/.*-\(.*\)\.t.*/\1/'"
source=($url/ftp/rzip/$pkgname-$pkgver.tar.gz)
md5sums=('8a88b445afba919b122a3899d6d26b2a')
license="GPL2"

