# Last Modified: Tue, 24 May 2005 14:03:02 +0200
# Compiling Time: ~5 minutes
# Maintainer: VMiklos <mamajom@axelero.hu>

pkgname=zsh
pkgver=4.2.5
pkgrel=2
pkgdesc="Zsh is a shell designed for interactive use"
url="http://www.zsh.org/"
depends=('glibc')
groups=('apps')
up2date="lynx -dump $url/pub/|grep zsh-[0-9\.]*tar.bz2$|sed -n -e 's/.*h-\(.*\)\.t.*/\1/' -e '1 p'"
source=(http://www.zsh.org/pub/$pkgname-$pkgver.tar.bz2 \
	http://www.zsh.org/pub/$pkgname-$pkgver-doc.tar.bz2)
install=$pkgname.install
md5sums=('e2060f743dcdf3b383e80e862a6548fe' '6dc896b52a3bc20a514148d621e13282')

build()
{
	Fbuild
	Fmkdir /etc
	Fln profile /etc/zprofile
}

# optimalization ok

# vim: ft=sh
