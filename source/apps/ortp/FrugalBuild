# Last Modified: Fri, 05 May 2006 16:40:30 +0200
# Compiling Time: 0.13 SBU
# Maintainer: crazy <crazy@frugalware.org>

pkgname=ortp
pkgver=0.9.1
pkgrel=1
pkgdesc="A Real-time Transport Protocol stack."
url="http://www.linphone.org/ortp/"
depends=('glibc')
groups=('apps')
archs=('i686')
up2date="lynx -dump $url/sources/|grep 'sources'|sort -n -r|grep -m1 '.tar.gz'|sed 's/.*-\(.*\).tar.gz/\1/'"
source=($url/sources/$pkgname-$pkgver.tar.gz)
sha1sums=('72a60bbf8bd5cdde3fa899eb537a6bc59b6013e8')

build()
{
	Fbuild --disable-ewarning --enable-ipv6 \
		--with-gnu-ld --disable-gtk-doc
}

# optimization OK
