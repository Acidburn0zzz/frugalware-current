pkgname=breeze-grub
pkgver=5.9.4
pkgrel=1
pkgdesc="Breeze theme for GRUB"
groups=('plasma')
archs=("x86_64")
_F_kde_project="plasma"
Finclude kf5
rodepends=('grub2')
sha1sums=('9e43c684a9634b4db877a5f021105faba2a55b23')

build(){
	Fcd
	Fmkdir usr/share/grub/themes/
	Fcprel breeze usr/share/grub/themes/
}

# optimization OK
