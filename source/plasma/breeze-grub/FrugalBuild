pkgname=breeze-grub
pkgver=5.11.3
pkgrel=1
pkgdesc="Breeze theme for GRUB"
groups=('plasma')
archs=("x86_64")
_F_kde_project="plasma"
Finclude kf5
rodepends=('grub2')
sha1sums=('9c9a7ea75160df18090c27f45f16ceb2e5975c90')

build(){
	Fcd
	Fmkdir usr/share/grub/themes/
	Fcprel breeze usr/share/grub/themes/
}

# optimization OK
