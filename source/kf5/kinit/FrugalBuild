pkgname=kinit
pkgver=5.24.0
pkgrel=2
pkgdesc="KInit."
groups=('kf5')
archs=('i686' 'x86_64')
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('34edb1e53245c3ae5b4ecc052373bc4cec0ebdbd')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
