pkgname=kinit
pkgver=5.37.0
pkgrel=1
pkgdesc="KInit."
groups=('kf5')
archs=("x86_64")
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('c9e08c3f18d762667249fc7120276418ebaeeab4')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
