pkgname=kinit
pkgver=5.26.0
pkgrel=2
pkgdesc="KInit."
groups=('kf5')
archs=('i686' 'x86_64')
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('d1196e05d061b08a481546cb0478eaa434d8ce5e')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
