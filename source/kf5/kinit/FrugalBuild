pkgname=kinit
pkgver=5.30.0
pkgrel=1
pkgdesc="KInit."
groups=('kf5')
archs=("x86_64")
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('87881698b4549754fe0c9c456be1dd433b415b89')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
