pkgname=kinit
pkgver=5.32.0
pkgrel=1
pkgdesc="KInit."
groups=('kf5')
archs=("x86_64")
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('b0d1bf856f6411a7f36c26bead024b9111244b75')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
