pkgname=kinit
pkgver=5.36.0
pkgrel=1
pkgdesc="KInit."
groups=('kf5')
archs=("x86_64")
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('3b00a7f4f93b78fc6c8afca343eb6a34c242502c')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
