pkgname=kinit
pkgver=5.31.0
pkgrel=1
pkgdesc="KInit."
groups=('kf5')
archs=("x86_64")
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('04235748d267ff43ad0246e9413a22d1c4a32bf8')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
