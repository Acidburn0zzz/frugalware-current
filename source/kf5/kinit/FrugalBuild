pkgname=kinit
pkgver=5.27.0
pkgrel=1
pkgdesc="KInit."
groups=('kf5')
archs=("x86_64")
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('d8eeb18a2e2f4b33d87e5f78a78cf2f1da7f3c4e')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
