pkgname=kinit
pkgver=5.34.0
pkgrel=1
pkgdesc="KInit."
groups=('kf5')
archs=("x86_64")
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('1c46590fefe91a6436c843e7922c1e3d16518199')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
