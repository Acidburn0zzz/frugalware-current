pkgname=kinit
pkgver=5.38.0
pkgrel=1
pkgdesc="KInit."
groups=('kf5')
archs=("x86_64")
_F_kde_project="frameworks"
Finclude kf5
depends=("kio>=$_F_kf5_full")
makedepends+=("kdoctools>=$_F_kf5_full")
sha1sums=('781c59e28b6d2963b969c32b6f2d5a76cdcd92ac')

build()
{
	export DESTDIR="$Fdestdir"
        KDE_build
}
# optimization OK
