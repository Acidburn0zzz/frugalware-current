# Compiling Time: 0.10 SBU
# Maintainer: Miklos Vajna <vmiklos@frugalware.org>

pkgname=libetonyek
pkgver=0.1.6
pkgrel=2
pkgdesc="Libetonyek is library providing ability to interpret and import Apple Keynote presentations into various applications."
url="https://wiki.documentfoundation.org/DLP/Libraries/libetonyek"
depends=('libwpd>=0.10.1-5' 'libboost>=1.61.0')
makedepends=('boost>=1.61.0' 'cppunit' 'gperf' 'glm>=0.9.7.2-2' 'mdds>=1.2.1-2' 'liblangtag>=0.6.0-3')
groups=('xoffice')
archs=('i686' 'x86_64')
up2date="Flasttar http://dev-www.libreoffice.org/src/libetonyek"
source=(http://dev-www.libreoffice.org/src/libetonyek/libetonyek-$pkgver.tar.xz)
sha1sums=('c2f8420f1e77141097a10a33d3b83d41c38b65f1')
Fconfopts="$Fconfopts --disable-werror"

build() {

	Fcd
	Fsed 'mdds-1.0' 'mdds-1.2' configure.ac
	Fautoreconf
	Fbuild
}

# optimization OK
