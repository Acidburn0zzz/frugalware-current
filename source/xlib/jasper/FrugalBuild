pkgname=jasper
pkgver=1.900.0
pkgrel=1
pkgdesc="Jasper is a implementation of the codec specified in the JPEG-2000 Part-1 standard."
url="http://www.ece.uvic.ca/~mdadams/jasper/"
up2date="lynx -dump http://www.ece.uvic.ca/~mdadams/jasper/|grep -m1 '.zip'|sed 's/.*-\(.*\).z.*/\1/'"
source=($url/software/$pkgname-$pkgver.zip)
depends=('libjpeg' 'freeglut')
makedepends=('unzip')
groups=('xlib')
archs=('i686' 'x86_64')
sha1sums=('1959a9e5d35d15b8be8789c993e743cb657207ef')
Fconfopts="$Fconfopts --enable-shared --disable-static"

