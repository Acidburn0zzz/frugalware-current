pkgname=pwlib
pkgver=1.11.1
realver=`echo $pkgver | sed 's/\./_/g'`
pkgrel=1
pkgdesc="Portable Windows Library"
url="http://openh323.sourceforge.net/"
groups=('xlib')
archs=('i686' 'x86_64')
depends=('openldap' 'sdl')
makedepends=('alsa-lib' 'libdv' 'libavc1394' 'esd' 'aalib')
source=(http://heanet.dl.sourceforge.net/sourceforge/openh323/pwlib-v$realver-src.tar.gz)
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=80674 | grep 'pwlib' | grep 'Release Notes' | sed 's/[^]]*][^]]*]\([^ ]*\) .*/\1/;s/-/_/g;q'"
sha1sums=('1cdda88e2de3be0186811d466052569b3a771668')
Fconfopts="$Fconfopts --libdir=/usr/lib --enable-ipv6 --enable-v4l --enable-avc --enable-plugins --enable-v4l2 --enable-oss --enable-sdl"
_F_cd_path="${pkgname}_v${realver}"

# optimization OK
