pkgver=1.3.3
pkgrel=1
pkgdesc="A multilingual text processing library."
url="http://www.m17n.org/"
depends=('anthy' 'libxaw' 'fribidi' 'gd' 'libotf' 'libxft' 'fontconfig' 'm17n-db>=1.3.3')
groups=('xlib')
archs=('i686')
up2date="lynx -dump $url/m17n-lib/download.html|grep -m1 'm17n-lib-'|sed 's/.*-\(.*\).t.*/\1/'"
source=($url/m17n-lib/download/$pkgname-$pkgver.tar.gz)
