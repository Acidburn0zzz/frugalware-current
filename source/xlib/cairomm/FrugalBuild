pkgname=cairomm
pkgver=1.2.4
pkgrel=1
pkgdesc="C++ bindings for cairo"
url="http://cairographics.org"
depends=('cairo')
makedepends=('doxygen' 'graphviz')
groups=('xlib')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/releases/ |grep ]LATEST-cairomm|sed 's/.*mm-\([^ ]*\) .*/\1/'"
source=($url/releases/$pkgname-$pkgver.tar.gz)

# optimization OK
