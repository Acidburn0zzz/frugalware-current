pkgname=libast
pkgver=0.6.1
pkgrel=2
pkgdesc="The library of assorted spiffy things"
url="http://www.eterm.org/download/"
depends=('x' 'imlib2' 'pcre' 'freetype2')
groups=('xlib')
archs=('i686' 'x86_64')
up2date="lynx -dump 'http://www.eterm.org/download/'|grep libast-[0-9\.]*.tar.gz|sed -ne 's/.*t-\(.*\)\.t.*/\1/;1 p'"
source=(http://www.eterm.org/download/$pkgname-$pkgver.tar.gz)
md5sums=('1cbbf86a0afb74a5905dc92982117250')

build () {
	Fbuild --enable-mmx || return 1
}

