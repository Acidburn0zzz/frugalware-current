pkgname=qt5-virtualkeyboard
pkgver=5.7.1
pkgrel=3
depends=('qt5-declarative>=5.7.1-3' 'qt5-svg>=5.7.1-3' 'hunspell')
makedepends+=('x11-protos')
Finclude qt5
sha1sums=('63f6aeb4757dea0f913b9b53bf31914b86e0eeb6')

build() {

	Fcd
	qmake-qt5 "CONFIG+=lang-all"
	make || Fdie
	make INSTALL_ROOT=$Fdestdir install || Fdie
}

# optimization OK

