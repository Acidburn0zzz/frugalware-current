pkgname=libwnck3
_F_gnome_name=libwnck
pkgver=3.20.1
pkgrel=1
pkgdesc="Window Navigator Construction Kit (Gtk+3)"
depends=('gtk+3>=3.16.5-1' 'startup-notification>=0.12-3' 'libxres>=1.0.7-3' 'libffi>=3.2.1')
groups=('xlib')
makedepends=('perl-xml-parser' 'intltool' 'gobject-introspection>=1.46.0-2' 'x11-protos' 'xcb-util>=0.4.0-2')
archs=('i686' 'x86_64')
_F_gnome_ext=".tar.xz"
Finclude gnome
up2date=$pkgver
_F_cd_path="$_F_gnome_name-$pkgver"
Fconfopts+=" --disable-gtk-doc"
sha1sums=('50cdfa9ebfeda66f4ebdc49dd14b6a3679442835')
CFLAGS+=" -fno-strict-aliasing"

build() {

	Fbuild
	Frm usr/bin
}

# optimization OK
