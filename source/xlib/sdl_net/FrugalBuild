pkgname=sdl_net
realname=SDL_net
pkgver=1.2.6
pkgrel=1
pkgdesc="A small sample cross-platform networking library"
url="http://www.libsdl.org/projects/SDL_net/"
depends=('sdl' 'glibc')
groups=('xlib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url|grep -m1 '.tar.gz$'|sed 's/.*-\(.*\).t.*/\1/'"
source=($url/release/$realname-$pkgver.tar.gz)
sha1sums=('bac98a482ffeec405c8405669dc2bb5d2e7db451')

build() {
	Fcd $realname-$pkgver
	Fbuild
}


# optimization OK
