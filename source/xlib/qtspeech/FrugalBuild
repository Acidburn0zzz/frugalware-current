pkgname=qtspeech
pkgver=5.9.0.g15e98bd
pkgrel=2
pkgdesc="Qt tts (text to speech) module, speech recognition may be introduced"
archs=("x86_64")
groups=("xlib")
depends=('qt5-base>=5.7.1' 'speech-dispatcher' 'qt5-multimedia>=5.7.1')
_F_github_author="qtproject"
_F_github_devel="yes"
Finclude qt5 github
options+=('force')
replaces=("jovie")
conflicts=("${replaces[@]}")
unset source

build()
{
	Funpack_scm
	make clean
        build_qt5
}

