depends=('libsvg' 'cairo' 'libjpeg>=8b-1')
groups=('xlib')
archs=('i686' 'x86_64')
up2date="lynx -dump '$url/snapshots/?C=M;O=D' |grep '$pkgname-[0-9\.]*tar.gz$' |sed -e 's/.*libsvg-cairo-\(.*\).tar.*/\1/;q'"
source=($url/snapshots/$pkgname-$pkgver.tar.gz)
