pkgname=tasklist
pkgver=0.5
pkgrel=1
pkgdesc="ROX Applet for displaying a list of windows."
_F_rox_name=Tasklist
_F_rox_subdir=Applets/
options=('scriptlet')
Finclude rox
rodepends=('rox-lib')
depends=('libwnck')
groups=(${groups[@]} 'rox-applets')
_F_archive_name=$_F_rox_name
up2date="Flasttar ftp://ftp.berlios.de/pub/rox4debian/apps"
source=(ftp://ftp.berlios.de/pub/rox4debian/apps/$_F_rox_name-$pkgver.tgz)
archs=('i686')
sha1sums=('695f75ea14abb48949722a7f1d56d0f873c2152a')

# optimization OK
