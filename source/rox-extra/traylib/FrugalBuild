pkgname=traylib
pkgver=0.3.2.1
pkgrel=1
pkgdesc="A library for some rox applets."
_F_rox_name=TrayLib
Finclude rox
rodepends=('rox-filer' 'pygtk')
groups=(${groups[@]} 'rox-other')
archs=('i686')
url="ftp://ftp.berlios.de/pub/rox4debian/libs"
_F_archive_name=$_F_rox_name
up2date="Flasttar $url"
source=($url/$_F_rox_name-$pkgver.tgz)
sha1sums=('d38e225fb77b254b33922de001fdbd8559d7f965')
