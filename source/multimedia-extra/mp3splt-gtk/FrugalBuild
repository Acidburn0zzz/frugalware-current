pkgname=mp3splt-gtk
pkgver=0.6
pkgrel=1
pkgdesc="mp3splt-gtk is a interface for mp3splt, a utility for mp3/ogg splitting without decoding"
_F_sourceforge_dirname="mp3splt"
Finclude sourceforge
depends=('gtk+2' 'zlib' 'libmp3splt=0.6' 'audacious' 'libmowgli' 'dbus' 'gstreamer')
groups=('multimedia-extra')
archs=('i686' 'x86_64')
Fconfopts="$Fconfopts --enable-audacious --disable-gstreamer --disable-mp3splttest"
