# Compiling Time: 0.06 SBU
# Maintainer: Devil505 <devil505linux@gmail.com>
# Contributor: James Buren <flakeroats@hotmail.com>

pkgname=tintin
pkgver=1.98.1
pkgrel=1
pkgdesc="a console mud client from which other mud clients have been written"
Finclude sourceforge
depends=('zlib')
groups=('games-extra')
archs=('i686')
sha1sums=('7687eb8f5d145cd2c614d96e862cefd3571885eb')

build()
{
	Fcd tt/src
	Fmake CC="gcc $CFLAGS"
	Fexerel /usr/bin/tt++
	cd ..  || Fdie
	Fdocrel README
	Fdocrel FAQ
	cd docs || Fdie
	Fdocrel tintin19.txt
	Fdocrel chat_protocol.txt
}
