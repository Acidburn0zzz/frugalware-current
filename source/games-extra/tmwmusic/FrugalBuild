# Compiling Time: 0.01 SBU
# Maintainer: Devil505 <devil505linux@gmail.com>

pkgname=tmwmusic
pkgver=0.2
pkgrel=2
pkgdesc="Music for The Mana World."
_F_sourceforge_dirname="themanaworld"
_F_sourceforge_realname="TMW Music"
Finclude sourceforge
url="http://themanaworld.org"
depends=()
groups=('games-extra')
archs=('i686' 'x86_64')
sha1sums=('c60eb1a55222f3ade8ddf04f619998b5e3cb1d08')
build()
	{
		Fcd $pkgname-$pkgver/data/music
		Fmkdir usr/share/tmw/data/music
		install -m 755 * $Fdestdir/usr/share/tmw/data/music || Fdie
	}
