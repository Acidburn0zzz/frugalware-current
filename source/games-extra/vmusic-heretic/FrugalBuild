pkgname=vmusic-heretic
pkgver=1.0
pkgrel=1
pkgdesc="Heretic music for vavoom"
_F_sourceforge_realname="vmusic"
_F_sourceforge_dirname="vavoom"
_F_sourceforge_ext=".zip"
Finclude sourceforge
rodepends=('vavoom')
groups=(games-extra)
archs=('i686' 'x86_64')
sha1sums=('2f9f3be58c80f7be848ce995630d2d638d53f262')
build()
{
       Fmkdir usr/share/vavoom
       Fcprel basev /usr/share/vavoom
}
