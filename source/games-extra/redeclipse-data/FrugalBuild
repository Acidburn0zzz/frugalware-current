# Compiling Time: 0.19 SBU
# Maintainer: DeX77 <dex77@frugalware.org>

pkgname=redeclipse-data
pkgver=1.5.2
pkgrel=1
pkgdesc="Data for Red Eclipse"
depends=('glibc')
makedepends=('ca-certificates')
groups=('games-extra')
archs=('i686' 'x86_64')
_F_github_author=red-eclipse
_F_github_name=data
_F_github_tag=y
_F_github_tag_v=y
Finclude github
sha1sums=('b83b0db0c64ae145360b47467a245d1abf498273')
options=('nobuild')

build() {
	Fmkdir usr/share/redeclipse/data/
	Fcp "data-$pkgver/*" usr/share/redeclipse/data/
}

# optimization OK
