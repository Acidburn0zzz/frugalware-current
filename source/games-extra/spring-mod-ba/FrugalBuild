pkgname=spring-mod-ba
pkgver=6.95
pkgrel=1
pkgdesc="Balanced Annihilation mod for Spring"
url="http://springrts.com/wiki/Balanced_Annihilation"
rodepends=('spring')
depends=()
groups=('games-extra')
archs=('i686' 'x86_64')
up2date=$pkgver
source=("http://spring.vsync.de/5/BA695.sd7")
sha1sums=('33fe3ebd0bb05e12b60e2a040e4068b8c52da2af')

build() {
  Fmkdir usr/share/spring/mods/
  Finstall 644 BA695.sd7 usr/share/spring/mods/BA695.sd7
}


