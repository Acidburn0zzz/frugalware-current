# Compiling Time: 0.08 SBU
# Maintainer: gholafox <j3d1f0x@free.fr>

pkgname=arora
pkgver=0.5
pkgrel=1
pkgdesc="Web browser written in Qt and which use webkit."
_F_gnome_iconcache="y"
Finclude googlecode
makedepends=('qt4-linguist')
depends=('libqtwebkit')
option=('scriptlet')
groups=('xapps-extra')
archs=('i686' 'x86_64')
sha1sums=('9809e608edcbd9eae1e2a758a6135bf0534245ae')
build()
{
	Fcd
	Fpatchall
	qmake PREFIX=/usr arora.pro || Fdie
	Fmake
	make INSTALL_ROOT=$Fdestdir install || Fdie
}
