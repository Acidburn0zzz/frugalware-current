# Compiling Time: 0.75 SBU
# Maintainer: gholafox <gholafox@free.fr>

pkgname=arora
pkgver=0.10.2
pkgrel=2
pkgdesc="Web browser written in Qt and which use webkit."
_F_gnome_iconcache="y"
Finclude googlecode
makedepends=('qt4-linguist')
depends=('libqtwebkit>=4.6.2-3' 'libqtphonon' 'libqtcore' 'libqtscripttools' 'qt4-plugin-mysql')
options=('scriptlet')
groups=('xapps-extra')
archs=('i686' 'x86_64')
sha1sums=('843d6d3e073588b5f6164985170a3525450f6615')

build() {
	Fsed "/usr/local" "/usr" install.pri
	qmake $pkgname.pro || Fdie
	Fmake
	Fmakeinstall INSTALL_ROOT=$Fdestdir
}

# optimization OK
