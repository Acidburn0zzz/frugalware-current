pkgname=rrdtool
pkgver=1.6.0
pkgrel=1
pkgdesc="a program to generate fancy graphs from network usage or from any thing is able to meter"
url="http://oss.oetiker.ch/$pkgname/pub"
depends=('pango>=1.38.1' 'libxml2>=2.9.3-3' 'python>=2.7.11-4')
makedepends=('x11-protos' 'groff')
groups=('xapps-extra')
archs=('x86_64')
up2date="lynx -dump '$url/?C=M;O=A' | grep '$pkgname-.*\.tar.gz$' | sed -ne 's/.*-\(.*\)\.t.*/\1/;$ p'"
source=($url/$pkgname-$pkgver.tar.gz)
sha1sums=('9866b41bda9416188f236d61d24f185b173fd571')

# optimization OK
