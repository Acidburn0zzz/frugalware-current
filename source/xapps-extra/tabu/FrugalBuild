pkgname=tabu
pkgver=2.0
pkgrel=1
pkgdesc="A GStreamer audio player with client side decorations and controls drawn with Cairo"
url="http://www.kalmbach.com.ar/"
depends=('cairo>=1.8.6' 'gtk+2>=2.16.2-2' 'taglib' 'gstreamer>=0.10.23')
makedepends=('intltool')
groups=('xapps-extra')
archs=('i686' 'x86_64')
options=('scriptlet')
up2date="lynx -dump $url | grep -m1 tar.gz | sed 's/.*r-\(.*\).t.*/\1/'"
source=($url/download/tabu_audio_player-${pkgver}.tar.gz)
sha1sums=('b3ff0ace3f5bb24c3bd2e6844b92907e94ffc6b2')
_F_cd_path="${pkgname}_audio_player-${pkgver}"

# optimization OK
