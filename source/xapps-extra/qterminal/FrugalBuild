pkgname=qterminal
## there is 0.6.0 from 2014 , we use master and set minor+1
## I do not like this Finclude scm
pkgver=0.6.1
pkgrel=2
pkgdesc="The lightweight Qt5 terminal emulator"
url="https://github.com/lxde/qterminal"
depends=('qtermwidget>=0.6.1-2' 'qt5-x11extras')
makedepends+=('qt5-tools' 'x11-protos')
groups=('xapps-extra')
archs=('i686' 'x86_64')
up2date="$pkgver" ## none .. check manually for changes in git
source=(https://github.com/lxde/qterminal/archive/master.zip)
sha1sums=('4bd40b7b99e9d77a096be21dc35cfcc0a795097d')
_F_cd_path="$pkgname-master"
options+=('force')
Finclude cmake


replaces=('quadkonsole4')
conflicts=("${replaces[@]}")


# optimization OK
