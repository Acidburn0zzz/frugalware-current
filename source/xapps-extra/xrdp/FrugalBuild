pkgname=xrdp
pkgver=0.9.1
pkgrel=1
pkgdesc="RDP Server - An open source RDP server and X server capable of accepting connections from rdesktop and ms terminal server clients."
depends=('libxfixes' 'libxrandr' 'pam' 'openssl')
makedepends=('x11-protos')
groups=('xapps-extra')
archs=('x86_64')
_F_github_author="neutrinolabs"
_F_github_tag_v=y
Finclude github
sha1sums=('9e95ff6a0e5b6773cda160c2d7979a169c50c9fc')

build()
{
	Fcd
	./bootstrap
	Fbuild
}

