pkgname=gogglesmm
pkgver=0.10.26
pkgrel=1
pkgdesc="Music Manager and Player"
depends=('fox>=1.6.37-2' 'libxext' 'libxrender' 'libglu' 'xine-lib' 'dbus' 'taglib' 'sqlite3' 'libxfixes')
_F_googlecode_ext=".tar.bz2"
_F_gnome_iconcache="y"
_F_gnome_desktop="y"
Finclude googlecode gnome-scriptlet
groups=('xapps-extra')
archs=('i686' 'x86_64')
sha1sums=('6070fae7855e844b7c008755ffeb7ee7768f9463')
replaces=('musicmanager')

build()	{
	Fcd
	Fmake
	make DESTDIR=$Fdestdir install || Fdie
	Fbuild_gnome_scriptlet
}