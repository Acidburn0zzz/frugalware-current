pkgname=gmpc-coveramazon
pkgver=0.15.5.0
pkgrel=1
pkgdesc="A gmpc plugin that fetches cover art and album info from amazon."
url="http://sarine.nl/amazon-provider/"
depends=('gtk+2' 'gmpc=0.15.5.0' 'libxml2' 'libmpd')
groups=('xapps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://sarine.nl/gmpc-plugins-downloads | grep -m1 $pkgname | Flasttar"
source=(http://download.sarine.nl/gmpc-0.15.5/$pkgname-$pkgver.tar.gz)
sha1sums=('572191b61ad7b68abb6933e2520206427726a878')

# optimization OK
