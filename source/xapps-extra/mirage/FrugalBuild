pkgname=mirage
pkgver=0.8.1
pkgrel=1
pkgdesc="Mirage is a fast and simple GTK+ image viewer."
url="http://mirageiv.berlios.de/index.html"
depends=('gtk+2' 'pygtk')
groups=('xapps-extra')
archs=('i686')
up2date="lynx -dump http://mirageiv.berlios.de/download.html | grep -m1 tar.bz2 | sed 's/.*-\(.*\).t.*/\1/'"
source=(http://download.berlios.de/mirageiv/$pkgname-$pkgver.tar.bz2)
sha1sums=('4d02b2493dab9660e05f84dcb0be990466802521')

# optimization OK

