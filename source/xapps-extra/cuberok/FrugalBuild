# Compiling Time: 0.11 SBU
# Maintainer: Devil505 <devil505linux@gmail.com>

pkgname=cuberok
pkgver=0.0.11
pkgrel=1
pkgdesc="Yet another music player based on Qt4."
depends=('gstreamer' 'libx11' 'audiere' 'libqtphonon' 'libqtxml' 'taglib' 'libqtsql' 'libqtnetwork')
options=('scriptlet')
Finclude googlecode
groups=('xapps-extra')
archs=('i686' 'x86_64')
sha1sums=('eead90a0400c6487f71093c66f2e33f3f5fb71ad')

build() {
  Fcd
  ./unix_build.sh || Fdie
  make INSTALL_ROOT=$Fdestdir/usr install || Fdie
}

# optimization OK
