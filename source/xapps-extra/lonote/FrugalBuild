pkgname=lonote
pkgver=1.6.5
pkgrel=1
pkgdesc="LoNote is a personal note-taking software."
depends=('pyqt4')
makedepends=('python-3.0' '7zip')
groups=('xapps-extra')
archs=('i686' 'x86_64')
_F_googlecode_ext=".7z"
Finclude googlecode
sha1sums=('73f99268c32ae8776ad87684669d55785a254a26')

build() {
	Fcd
	python3 setup.py install --root=$Fdestdir || Fdie
}
