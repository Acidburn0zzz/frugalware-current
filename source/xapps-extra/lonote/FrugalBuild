pkgname=lonote
pkgver=1.6.8
pkgrel=1
pkgdesc="LoNote is a personal note-taking software."
depends=('pyqt4')
makedepends=('python-3.0' '7zip')
groups=('xapps-extra')
archs=('i686' 'x86_64')
_F_googlecode_ext=".7z"
Finclude googlecode
sha1sums=('14b50f1ae270250df9e8e8df9e5d5e7b32a45fd7')

build() {
	Fcd
	python3 setup.py install --root=$Fdestdir || Fdie
}
