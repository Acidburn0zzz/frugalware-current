pkgname=lonote
pkgver=1.6.2
pkgrel=1
pkgdesc="LoNote is a personal note-taking software."
depends=('pyqt4')
makedepends=('python-3.0' '7zip')
groups=('xapps-extra')
archs=('i686' 'x86_64')
_F_googlecode_ext=".7z"
Finclude googlecode
sha1sums=('bda2da169dd4da97452b844e9c3804138ac79422')

build() {
	Fcd
	python3 setup.py install --root=$Fdestdir || Fdie
}
