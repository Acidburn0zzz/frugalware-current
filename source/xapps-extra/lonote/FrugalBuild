pkgname=lonote
pkgver=1.5.10
pkgrel=1
pkgdesc="LoNote is a personal note-taking software."
depends=('pyqt4')
makedepends=('python-3.0' '7zip')
groups=('xapps-extra')
archs=('i686' 'x86_64')
_F_googlecode_ext=".7z"
Finclude googlecode
sha1sums=('d2f364add097489fbf97384bd3165432524f7d42')

build() {
	Fcd
	python3 setup.py install --root=$Fdestdir || Fdie
}
