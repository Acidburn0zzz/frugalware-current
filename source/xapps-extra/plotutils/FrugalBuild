pkgname=plotutils
pkgver=2.6
pkgrel=1
pkgdesc="Utils and library for exporting 2-D vector graphics in many file formats"
url="http://www.gnu.org/software/plotulils/"
depends=('libxaw' 'libsm' 'libxext' 'libpng' 'libstdc++' 'util-linux-ng')
groups=('xapps-extra')
archs=('i686' 'x86_64')
Fup2gnugz
Fconfopts="$Fconfopts --with-x --enable-libxmi --enable-libplotter"
source=(http://ftp.gnu.org/gnu/$pkgname/$pkgname-$pkgver.tar.gz)
sha1sums=('7921301d9dfe8991e3df2829bd733df6b2a70838')
# optimization OK
