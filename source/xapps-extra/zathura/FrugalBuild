options+=('asneeded')

pkgname=zathura
pkgver=0.2.5
pkgrel=1
pkgdesc="PDF Document viewer"
url=("https://pwmt.org/projects/zathura")
archs=('i686' 'x86_64')
groups=('xapps-extra')
depends=('poppler-glib>=0.20.3' 'cairo' 'glib2' 'girara' 'sqlite3')
up2date="Flasttar $url/download/"
source="$url/download/$pkgname-$pkgver.tar.gz"
sha1sums=('ce795ca03140778f442d796a9e807f283a69253a')

# optimization OK
