options+=('asneeded')

pkgname=zathura
pkgver=0.2.6
pkgrel=1
pkgdesc="PDF Document viewer"
url=("https://pwmt.org/projects/zathura")
archs=('i686' 'x86_64')
groups=('xapps-extra')
depends=('poppler-glib>=0.20.3' 'cairo' 'glib2' 'girara>=0.1.9' 'sqlite3')
up2date="Flasttar $url/download/"
source="$url/download/$pkgname-$pkgver.tar.gz"
sha1sums=('d84878388969d523027a1661f49fd29638bd460b')

# optimization OK
