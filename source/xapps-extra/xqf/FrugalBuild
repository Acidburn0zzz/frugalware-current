# Maintainer: voroskoi <voroskoi@frugalware.org>

pkgname=xqf
pkgver=1.0.5
pkgrel=1
pkgdesc="A game server browser and launcher for many popular games."
depends=('gdk-pixbuf' 'geoip' 'gtk+2')
makedepends=('perl-xml')
rodepends=('qstat')
groups=('xapps-extra')
archs=('i686' 'x86_64')
_F_sourceforge_broken_up2date=1
Finclude sourceforge

build () {
	Fbuild --enable-gtk2 --enable-geoip
	Fdesktop xqf /usr/share/pixmaps/xqf.png Games
}
sha1sums=('052351ff26cf2f79f6bc2de5aacf63e76a02eb62')

# optimization OK
