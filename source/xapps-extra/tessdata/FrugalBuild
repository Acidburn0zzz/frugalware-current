pkgname=tessdata
pkgver=3.04.00
pkgrel=1
pkgdesc="Language specific files for tesseract"
archs=('x86_64')
groups=('xapps-extra')
depends=('glibc')
_F_github_author="tesseract-ocr"
_F_github_tag=y
Finclude github
