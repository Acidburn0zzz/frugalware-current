# Compiling Time: 0.05 SBU
# Maintainer: Devil505 <devil505linux@gmail.com>

pkgname=jargoninformatique
pkgver=1.3.6
pkgrel=2
pkgdesc="French computering dictionary."
url="http://jargon.tuxfamily.org/jargon/index.php"
depends=('fltk>=1.1.10' 'libxpm' 'libstdc++')
archs=('i686' 'x86_64')
groups=('xapps-extra')
up2date="lynx -dump $url/download.php | Flasttar"
source=(http://download.tuxfamily.org/jargon/$pkgname-$pkgver.tar.gz)
sha1sums=('adeade5bb37e22aa50b88612256dd86918bf4bdb')

build()
{
        Fcd
        Fconf
        Fmake
	make install PREFIX=$Fdestdir/usr || Fdie
}

# optimization OK
