pkgname=pytrainer
pkgver=1.6.0.9
pkgrel=1
pkgdesc="Tool to log all your sport excursion"
depends=('pysqlite2' 'pygtk>=2.14.1-2' 'matplotlib' 'soappy')
_F_sourceforge_prefix="PyTrainer "
Finclude sourceforge
options=('scriptlet')
groups=('xapps-extra')
archs=('i686' 'x86_64')
sha1sums=('8b7b53c0b8ea09f51364b2a3e516e6297a76e18a')

build() {
  Fcd
  python setup.py install --prefix=/usr --root=$Fdestdir || Fdie
}

