pkgname=pytrainer
pkgver=1.6.0.8
pkgrel=1
pkgdesc="Tool to log all your sport excursion"
depends=('pysqlite2' 'pygtk' 'matplotlib' 'soappy')
_F_sourceforge_prefix="PyTrainer "
Finclude sourceforge
options=('scriptlet')
groups=('xapps-extra')
archs=('i686' 'x86_64')
sha1sums=('b6fb7c41ae3b4945f77d3cab7e6893fe7a365821')

build() {
  Fcd
  python setup.py install --prefix=/usr --root=$Fdestdir || Fdie
}

