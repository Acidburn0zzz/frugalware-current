pkgname=zulucrypt
_F_archive_name="zuluCrypt"
pkgver=4.7.0
pkgrel=2
pkgdesc="A cli and GUI front end to cryptsetup"
url="https://github.com/mhogomchungu/zuluCrypt"
depends=('cryptsetup-luks>=1.6.3-3' 'libqtgui' 'libqtnetwork' 'libpwquality' 'libsecret')
groups=('xapps-extra')
archs=('i686' 'x86_64')
up2date="Flasttar $url/releases"
source=($url/releases/download/$pkgver/${_F_archive_name}-${pkgver}.tar.bz2)
Finclude cmake
# unset buildflags
unset CFLAGS
unset CXXFLAGS
unset LDFLAGS
unset MAKEFLAGS
