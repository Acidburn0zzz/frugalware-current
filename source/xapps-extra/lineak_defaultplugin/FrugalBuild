pkgname=lineak_defaultplugin
pkgver=0.8.4
pkgrel=1
pkgdesc="Default plugin for linEAK."
_F_sourceforge_name="lineak"
Finclude sourceforge
url="http://lineak.sourceforge.net/"
depends=('glibc' 'lineakd>=0.8.4' 'libxtst')
groups=('xapps-extra')
archs=('i686')
up2date="lynx -dump $url/index.php?nav=download | grep -m1 $pkgname | sed 's/.*gin \(.*\) \[5.*/\1/'"
source=(${source/lineak-/lineak_defaultplugin-})
Fconfopts="${Fconfopts[@]} --with-lineak-includedir=/usr/include/lineak/"
sha1sums=('67815d71f34fee90dda34b64200daf2b9bccf2c4')

# optimization OK
