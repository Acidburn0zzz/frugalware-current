pkgname=tintwizard
pkgver=0.3.3
pkgrel=1
pkgdesc="A GUI wizard which generates config files for tint2 panels."
url="http://mookooh.org/coverfinder/"
depends=('pygtk' 'tint2')
options=('scriptlet')
Finclude googlecode
groups=('xapps-extra')
archs=('i686' 'x86_64')
_F_cd_path="../"
sha1sums=('d72c7a7bb6428dc290ad051ca255d7863bbe7a2a')

build() {
	Fexe $pkgname.py usr/bin/$pkgname
}
