pkgname=osmo
pkgver=0.1.1
pkgrel=1
pkgdesc="A handy personal organizer which includes calendar, tasks manager and address book modules."
url="http://clay.ll.pl/osmo/"
depends=('gtk+2' 'libxdamage' 'libxml2')
options=('scriptlet')
groups=('xapps-extra')
archs=('i686')
up2date="lynx -dump $url | grep tar.gz | Flasttar"
source=(http://clay.ll.pl/osmo/$pkgname-$pkgver.tar.gz)
sha1sums=('53baff2148b63ab682272b9a542f34fe7009a5e2')



