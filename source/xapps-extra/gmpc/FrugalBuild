pkgname=gmpc
pkgver=0.14.0
pkgrel=1
pkgdesc="Gnome Music Player Client."
url="http://sarine.nl/gmpc"
makedepends=('intltool')
depends=('gtk+2' 'libglade' 'libmpd' 'curl')
rodepends=('mpd')
groups=('xapps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump download.sarine.nl/?M=D | grep -m1 $pkgname | sed 's/.*c-\(.*\)\/.*/\1/'"
source=(http://download.sarine.nl/$pkgname-$pkgver/$pkgname-$pkgver.tar.gz)
sha1sums=('99ffb52e2333eab039f96209d4f64fd921cca797')

# optimization OK
