pkgname=gmpc
pkgver=0.15.5.0
pkgrel=1
pkgdesc="Gnome Music Player Client."
url="http://sarine.nl/gmpc"
makedepends=('intltool')
depends=('gtk+2' 'libglade' 'libmpd>=0.15.0' 'curl')
rodepends=('mpd')
groups=('xapps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://sarine.nl/gmpc-downloads | grep -m1 tar.gz | sed 's/.*c-\(.*\).t.*/\1/'"
source=(http://download.sarine.nl/$pkgname-0.15.5/$pkgname-$pkgver.tar.gz)
sha1sums=('3ac0755da2215b0d037427ee50bea55d953e2454')

# optimization OK
