pkgname=bleachbit
pkgver=0.6.3
pkgrel=1
pkgdesc="BleachBit deletes unnecessary files to free valuable disk space and maintain privacy."
depends=('pygtk>=2.14.1-2')
options=('scriptlet')
_F_sourceforge_ext=".tar.bz2"
Finclude sourceforge
groups=('xapps-extra')
archs=('i686' 'x86_64')
sha1sums=('3441fcceb8abaee6578457289a8f54583e6dc671')

build() {
	Fsed "prefix ?= /usr/local" "prefix ?= /usr" Makefile
	Fbuild
}
