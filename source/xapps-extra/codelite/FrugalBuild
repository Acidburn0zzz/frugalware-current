pkgname=codelite
pkgver=2.2.0.3681
pkgrel=1
pkgdesc="IDE for C/C++ languages."
Finclude sourceforge
depends=('wxgtk-u8')
groups=('xapps-extra')
archs=('i686')
sha1sums=('3a20ddbebfec410a74328bb95565f70baf0370bc')
unset MAKEFLAGS

build() {
	Fsed "-O3" "$CFLAGS" configure
	Fbuild
}

# optimization OK
