pkgname=codelite
pkgver=2.1.0.3584
pkgrel=1
pkgdesc="IDE for C/C++ languages."
Finclude sourceforge
depends=('wxgtk-u8')
groups=('xapps-extra')
archs=('i686')
sha1sums=('0126896dc2e98c23672a58ab992ad8be3de3d56b')
unset MAKEFLAGS

build() {
	Fsed "-O3" "$CFLAGS" configure
	Fbuild
}

# optimization OK
