pkgname=cairo-dock-plugins
pkgver=2.3.0
pkgrel=1
pkgdesc="A set of plugins for cairo-dock."
url="http://www.glx-dock.org/"
depends=("cairo-dock>=$pkgver" 'vte>=0.20.5-2' 'libexif>=0.6.17' 'libxklavier>=3.9' \
	'alsa-lib>=1.0.20')
makedepends=('intltool')
groups=('xapps-extra')
options=('scriptlet')
archs=('i686' 'x86_64')
Finclude cmake
up2date="Flasttar https://launchpad.net/cairo-dock-plug-ins/ | sed 's/~1//g'"
source=(http://launchpad.net/cairo-dock-plug-ins/2.3/2.3.0/+download/$pkgname-${pkgver}~1.tar.gz)
_F_cd_path="$pkgname-${pkgver}~1"
sha1sums=('45674e58b6d17709653a56ce78af31dd4032cb17')

