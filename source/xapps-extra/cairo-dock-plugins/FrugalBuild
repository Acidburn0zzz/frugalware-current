pkgname=cairo-dock-plugins
pkgver=2.2.0_2
pkgrel=1
pkgdesc="A set of plugins for cairo-dock."
url="http://www.glx-dock.org/"
depends=("cairo-dock>=$pkgver" 'vte>=0.20.5-2' 'libexif>=0.6.17' 'libxklavier>=3.9' \
	'alsa-lib>=1.0.20')
makedepends=('intltool')
groups=('xapps-extra')
options=('scriptlet')
archs=('i686' 'x86_64')
Finclude cmake
up2date="Flasttar https://launchpad.net/cairo-dock-plug-ins | sed 's/_lucid//g'"
source=(http://launchpad.net/cairo-dock-plug-ins/2.2/2.2.0/+download/$pkgname-${pkgver//_/-}.tar.gz)
_F_cd_path="$pkgname-${pkgver//_/-}"
sha1sums=('b4292b103bd920bd841be91450b7a6e36587fc28')

