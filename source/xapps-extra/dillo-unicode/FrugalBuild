pkgname=dillo-unicode
pkgver=3.0.1
pkgrel=1
pkgdesc="Dillo aims to be a multi-platform browser alternative that's small, stable, developer-friendly, usable, fast, and extensible."
url="http://www.dillo.org"
depends=('libjpeg>=8a' 'libpng>=1.4.1' 'libxft' 'fltk' 'openssl>=1.0.0-6')
groups=('xapps-extra')
archs=('i686' 'x86_64' 'ppc')
conflicts=('dillo')
provides=('dillo')
_F_archive_name="dillo"
up2date="Flasttar $url/download"
source=($url/download/$_F_archive_name-$pkgver.tar.bz2)
sha1sums=('fd7324d0ceee05b4dc189f06ed0483ef5ac2c28a')

build() {
	Fbuild --sysconfdir=/etc/dillo
}

# optimization OK
