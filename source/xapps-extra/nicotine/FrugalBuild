pkgname=nicotine
pkgver=1.0.8
pkgrel=2
pkgdesc="Nicotine is a SoulSeek client written in Python"
url="http://nicotine.thegraveyard.org"
depends=('pygtk')
groups=('xapps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://nicotine.thegraveyard.org | grep nicotine-[0-9*].*.gz | sed -ne 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=(http://nicotine.thegraveyard.org/$pkgname-$pkgver.tar.gz)
sha1sums=('ec951e38e872d2be662d8b9e7237a4e25a72ed26')

