_F_sourceforge_ext=".tar.bz2"
_F_sourceforge_dirname="wxdcgui"
_F_sourceforge_name="$pkgname"
Finclude sourceforge
options=('scriptlet')
groups=('xapps-extra')
archs=('i686' 'x86_64')
