pkgname=popt
pkgver=1.16
pkgrel=9
pkgdesc="A commandline option parser"
url="http://rpm5.org"
depends=('glibc>=2.25-5')
makedepends=('gettext>=0.19.8.1-5')
groups=('base' 'chroot-core')
archs=('x86_64')
up2date="lynx -dump http://rpm5.org/files/popt|Flasttar"
source=(http://rpm5.org/files/popt/$pkgname-$pkgver.tar.gz)
sha1sums=('cfe94a15a2404db85858a81ff8de27c8ff3e235e')
options+=('static')
_F_cross32_simple_auto="true"
Finclude cross32

build() {
        Fbuild_cross32
	Fsubmv "lib32-$pkgname" usr/lib/pkgconfig usr/lib32/
}

# optimization OK
