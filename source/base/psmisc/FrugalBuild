pkgname=psmisc
pkgver=22.7
pkgrel=1
pkgdesc="Miscellaneous procfs tools"
depends=('ncurses>=5.6-4')
groups=('base')
archs=('i686' 'x86_64' 'ppc')
Finclude sourceforge

build()
{
	Fcd
	Fmake
	Fmakeinstall
	Frm /usr/{man/man1/pidof.1,bin/pidof}
}

sha1sums=('8226ecc284c8cd1e151523df56a1ba9f9c0fa3f5')
# optimization OK
