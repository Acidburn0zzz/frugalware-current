pkgname=psmisc
pkgver=22.12
pkgrel=1
pkgdesc="Miscellaneous procfs tools"
depends=('ncurses>=5.6-4')
groups=('base')
archs=('i686' 'x86_64' 'ppc')
Finclude sourceforge

build()
{
	Fcd
	Fmake
	Fmakeinstall
	Frm /usr/{man/man1/pidof.1,bin/pidof}
}

sha1sums=('662e332f7140252b65aca0c44096ee8d2f80b3c2')
# optimization OK
