pkgname=psmisc
pkgver=22.6
pkgrel=1
pkgdesc="Miscellaneous procfs tools"
depends=('ncurses')
groups=('base')
archs=('i686' 'x86_64' 'ppc')
Finclude sourceforge

build()
{
	Fcd
	Fmake
	Fmakeinstall
	Frm /usr/{man/man1/pidof.1,bin/pidof}
}

sha1sums=('cb15c23e9cfe618e9de5f066525bce7dc7b77421')
# optimization OK
