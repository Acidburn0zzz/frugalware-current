pkgname=psmisc
pkgver=22.5
pkgrel=1
pkgdesc="Miscellaneous procfs tools"
depends=('ncurses')
groups=('base')
archs=('i686' 'x86_64' 'ppc')
Finclude sourceforge

build()
{
	Fcd
	Fmake
	Fmakeinstall
	Frm /usr/{man/man1/pidof.1,bin/pidof}
}

sha1sums=('0fd6c52dd0dad464a9be7baa79fb810caf43c923')
# optimization OK
