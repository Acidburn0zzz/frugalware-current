pkgname=psmisc
pkgver=22.10
pkgrel=1
pkgdesc="Miscellaneous procfs tools"
depends=('ncurses>=5.6-4')
groups=('base')
archs=('i686' 'x86_64' 'ppc')
Finclude sourceforge

build()
{
	Fcd
	Fmake
	Fmakeinstall
	Frm /usr/{man/man1/pidof.1,bin/pidof}
}

sha1sums=('20bfa81ec36603c6a141bd7e6137cf9d3b52c3d9')
# optimization OK
