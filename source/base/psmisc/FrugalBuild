pkgname=psmisc
pkgver=22.9
pkgrel=1
pkgdesc="Miscellaneous procfs tools"
depends=('ncurses>=5.6-4')
groups=('base')
archs=('i686' 'x86_64' 'ppc')
Finclude sourceforge

build()
{
	Fcd
	Fmake
	Fmakeinstall
	Frm /usr/{man/man1/pidof.1,bin/pidof}
}

sha1sums=('aed1bd0631239c4f8e28764ad55d4b4ad02671f0')
# optimization OK
