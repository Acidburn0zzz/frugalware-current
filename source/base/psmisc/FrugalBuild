pkgname=psmisc
pkgver=22.13
pkgrel=1
pkgdesc="Miscellaneous procfs tools"
depends=('ncurses>=5.6-4')
groups=('base')
archs=('i686' 'x86_64' 'ppc')
Finclude sourceforge

build()
{
	Fcd
	Fmake
	Fmakeinstall
	Frm /usr/{man/man1/pidof.1,bin/pidof}
}

sha1sums=('5d53c8bb5a279176be3515c07d833cdf116dd516')
# optimization OK
