pkgname=gdbm
pkgver=1.8.3
pkgrel=2
pkgdesc="GNU database library"
url="http://www.gnu.org/software/gdbm/gdbm.html"
depends=('glibc')
groups=('base')
archs=('i686' 'x86_64')
up2date="lynx -dump 'http://ftp.gnu.org/pub/gnu/gdbm/?M=D'|grep tar.gz$|sed -n 's/.*-\(.*\)\.t.*/\1/;1 p'"
source=(ftp://ftp.gnu.org/pub/gnu/gdbm/$pkgname-$pkgver.tar.gz)
md5sums=('1d1b1d5c0245b1c00aff92da751e9aa1')

build()
{
	Fmake || return 1
	Fmakeinstall install-compat
}

# optimalization ok

