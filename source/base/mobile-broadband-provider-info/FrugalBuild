# Compiling Time: 0 SBU
# Maintainer: crazy <crazy@frugalware.org>

pkgname=mobile-broadband-provider-info
pkgver=20151214
pkgrel=3
pkgdesc="Contains service provider specific settings of mobile broadband providers in different countries."
groups=('base')
depends=()
archs=('i686' 'x86_64')
_F_gnome_devel="y"
Finclude gnome
source=(http://ftp.gnome.org/pub/GNOME/sources/$pkgname/$pkgver/$pkgname-$pkgver.tar.xz)
sha1sums=('4457b1afa262cd707cd601b1f96024999445b450')

build() {
	Fcd
	Fsed '$(datadir)/pkgconfig' '/usr/lib//pkgconfig' Makefile.{in,am}
	Fautoreconf
	Fbuild
}
# optimization OK
