unset depends makedepends
pkgver=$_F_kernelmod_ver
_F_kernel_stable=
pkgrel=$_F_kernelmod_rel
_F_kernel_patches=(6e38433357e2381bb278a418fb7e2fd201475101.diff fwh_detect.diff CVE-2006-6333.diff BTS-bug-1505.patch)
[ "$CARCH" == "x86_64" ] && \
	_F_kernel_patches=(${_F_kernel_patches[@]} linux-2.6.11-x86_64_config.patch0)
Finclude kernel
