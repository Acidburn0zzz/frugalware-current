USE_DEVEL=${USE_DEVEL:-"n"}

if ! Fuse $USE_DEVEL; then
	_F_kernel_patches=('fix-i915.patch')
else
	# example for a tagged rc release: 2.6.32.rc5
	# example for a random snapshot (based on git describe output): 2.6.32.rc5.81.g964fe08
	pkgver=2.6.32.rc5
	pkgrel=1
	_F_scm_type="git"
	_F_scm_url="git://git.kernel.org/pub/scm/linux/kernel/git/torvalds/linux-2.6"
fi

Finclude kernel

if ! Fuse $USE_DEVEL; then
sha1sums=('d6aedad90c386600076b3fd976d9c66be57c136f' \
          '7a2d9a8cbc71df32d2c382af485684bca0caa28d' \
          '97edf61e5219d6465ca8cef42ec686c4c9f3dd49' \
          '1e99e9ba7b2a6dccc7312293ab2cf770a6c469f3' \
          '754e42240154842808a47115e692620903816471' \
          '5766ba918b9b965534fd3281e23846fb5094496c')
fi
