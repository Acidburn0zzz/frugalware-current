Finclude kernel-module
unset depends makedepends
pkgver=$_F_kernelmod_ver
_F_kernel_stable=2
pkgrel=$_F_kernelmod_rel
_F_kernel_patches=(fwh_detect.diff BTS-bug-1505.patch \
	fuse-2.6.1.patch.bz2)
[ "$CARCH" == "x86_64" ] && \
	_F_kernel_patches=(${_F_kernel_patches[@]} linux-2.6.11-x86_64_config.patch0)
Finclude kernel
