# Last Modified: Sun, 08 May 2005 14:12:29 +0200
# Compiling Time: ~2 minutes
# Maintainer: VMiklos <vmiklos@frugalware.org>

pkgname=xfsprogs-acl
pkgver=2.2.15
pkgrel=2
pkgdesc="Dynamic library for access control list support."
url="http://oss.sgi.com/projects/xfs/"
depends=('xfsprogs-attr')
groups=('base')
archs=('i686' 'x86_64')
up2date="wget --passive-ftp -O - -q ftp://oss.sgi.com/projects/xfs/latest/cmd_tars/MD5SUMS|grep acl|cut -d ' ' -f 3|sed 's/.src.tar.gz//'|cut -d - -f 2"
source=(ftp://oss.sgi.com/projects/xfs/latest/cmd_tars/acl-$pkgver.src.tar.gz)
md5sums=('666889d62382a7c486f88f0cdcbc3a75')

build()
{
	Fcd acl-$pkgver
	Fmake || return 1
	Fmkdir /usr/sbin
	Fmakeinstall install-lib
}

# optimalization ok

