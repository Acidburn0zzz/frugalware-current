pkgname=unzip
pkgver=5.51
pkgrel=1
pkgdesc="Info-ZIP's unzip utility"
depends=('glibc')
up2date=`lynx -dump http://www.info-zip.org/UnZip.html|grep UnZip|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 3`
source=(ftp://ftp.info-zip.org/pub/infozip/src/unzip551.tar.gz)
md5sums=('8a25712aac642430d87d21491f7c6bd1')

build() {
  cd $startdir/src/$pkgname-$pkgver
  make -f unix/Makefile LOCAL_UNZIP="$CFLAGS" prefix=/usr linux || return 1
  make -f unix/Makefile prefix=$startdir/pkg/usr install
}
