if [ -e /etc/sysconfig/desktop ]; then
	. /etc/sysconfig/desktop
	$desktop
	logger "Failed to use '$desktop' session manager."
fi
logger "No /etc/sysconfig/desktop, can't use runlevel 4 without it."
sleep 5m
