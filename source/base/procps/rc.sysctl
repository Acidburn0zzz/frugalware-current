. /etc/rc.d/rc.functions

# chkconfig: 2345 85 15
# description: Load in sysctl settings from /etc/sysctl.conf.

if [ "$1" = "stop" ]; then
	exit 0
else # start
	start "$startsysctl"
	sysctl -n -p >/dev/null
	ok $?
fi
