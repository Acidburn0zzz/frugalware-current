pkgname=eject
pkgver=2.0.13
pkgrel=2
pkgdesc="Allows removable media to be ejected under software control."
url="http://members.rogers.com/jefftranter/eject.html"
depends=('glibc')
groups=('base')
up2date="lynx -dump 'http://www.ibiblio.org/pub/Linux/utils/disk-management/?M=D' |grep eject.*tar.gz$|sed -n 's/.*t-\(.*\)\.t.*/\1/;1 p'"
source=(http://www.ibiblio.org/pub/Linux/utils/disk-management/$pkgname-$pkgver.tar.gz)
md5sums=('b796ad77beb4e7bdd08d6149701ab778')

