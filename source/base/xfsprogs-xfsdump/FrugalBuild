# Last Modified: Wed, 18 Jan 2006 22:54:30 +0100
# Compiling Time: ~3 minutes
# Maintainer: VMiklos <vmiklos@frugalware.org>

pkgname=xfsprogs-xfsdump
pkgver=2.2.33
pkgrel=1
pkgdesc="Administrative utilities for the XFS filesystem."
url="http://oss.sgi.com/projects/xfs/"
depends=('xfsprogs-dmapi' 'xfsprogs-attr' 'ncurses')
groups=('base')
archs=('i686' 'x86_64')
up2date="lynx -dump ftp://oss.sgi.com/projects/xfs/cmd_tars/|grep xfsdump-.*gz$|sed 's/.*-\(.*\)\.s.*/\1/'"
source=(ftp://oss.sgi.com/projects/xfs/cmd_tars/xfsdump-$pkgver.src.tar.gz)
sha1sums=('06607a7e4ddd2a5e67143b293265fa69b3003449')

build()
{
	Fcd xfsdump-$pkgver
	Fbuild
	Frm /usr/bin/{xfsrestore,xfsdump}
}

# optimalization OK

