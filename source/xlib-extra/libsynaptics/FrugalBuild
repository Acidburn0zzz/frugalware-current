pkgname=libsynaptics
pkgver=0.14.4d
pkgrel=1
pkgdesc="A small C++ library usable by the synaptics driver."
url="http://qsynaptics.sourceforge.net/"
depends=('synaptics' 'libstdc++')
groups=('xlib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/index.html |grep KSynaptics |sed 's/.* \(.*\)/\1/;q'"
source=($url/$pkgname-$pkgver.tar.gz)
sha1sums=('6ad0ee687568d623077d4f9439cbccfa415410d6')

