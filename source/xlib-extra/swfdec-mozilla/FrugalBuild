pkgname=swfdec-mozilla
pkgver=0.5.1
pkgrel=1
pkgdesc="Mozilla plugin that uses the Swfdec library for playing SWF files in a browser capable of loading NS-Plugins"
url="http://swfdec.freedesktop.org/"
license="LGPL"
depends=('swfdec=0.5.1' 'firefox')
options=('scriptlet')
groups=('xlib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://swfdec.freedesktop.org/|grep -om1 'swfdec-\(.*\).tar.gz$'|Flasttar"
source=($url/download/$pkgname/0.5/$pkgname-$pkgver.tar.gz)
sha1sums=('41c8e53f3ef6c64d82fc8d72304c78cc37a4effb')

# optimization OK
