# Compiling Time: 64.56 SBU
# Maintainer: Marius Cirsta <mcirsta@frugalware.org>

pkgname=qt5-xmlpatterns
_F_archive_name=qt-everywhere-opensource-src
pkgver=5.5.1
pkgrel=1
pkgdesc="The Qt5 toolkit, QtXmlPatterns"
url="http://www.qt.io"
depends=('qt5-base')

groups=('xlib-extra')
archs=('i686' 'x86_64')
options=('nodocs')
up2date="Flasttar $url/download-open-source/"
source=(http://download.qt.io/archive/qt/5.5/${pkgver}/submodules/qtxmlpatterns-opensource-src-${pkgver}.tar.xz)
sha1sums=('15d2aeaaee4840aa3b015dfc7502982a0e5685f5')
_F_cd_path=qtxmlpatterns-opensource-src-${pkgver}


build()
{
	Fcd
	qmake-qt5 || Fdie
	Fmake
	make  INSTALL_ROOT=$Fdestdir install || Fdie
	for i in ${Fdestdir}/usr/lib/qt5/bin/*; do
		q5=`basename ${i}`
		Fln /usr/lib/qt5/bin/${q5} /usr/bin/${q5}-qt5
        done
}

# optimization OK

