# Compiling Time: 0 SBU
# Maintainer: DNAku <DNAku@frugalware.org>
# Contributor: Tuxbubling <tuxbubling@tiscali.fr>

pkgname=feedparser
pkgver=4.1
pkgrel=2
pkgdesc="Python RSS and Atom feeds parser."
url="http://www.feedparser.org/"
depends=('python')
groups=('xlib-extra')
archs=('i686' 'x86_64')
_F_sourceforge_ext=".zip"
Finclude sourceforge
sha1sums=('3a7174e11c8c523ea461c9d47a80526ba29a6544')

build()
{
	Fmkdir usr/share/doc/$pkgname-$pkgver
	mv docs/ $Fdestdir/usr/share/doc/$pkgname-$pkgver/
	python setup.py build
	cd build
	mv lib $Fdestdir/usr/
}

# optimalization OK --noarch
