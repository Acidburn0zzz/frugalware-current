pkgname=feedparser
pkgver=4.1
pkgrel=5
pkgdesc="Python RSS and Atom feeds parser."
depends=('python')
groups=('xlib-extra')
archs=('i686' 'x86_64' 'ppc')
_F_googlecode_ext=".zip"
Finclude googlecode python
url="http://www.feedparser.org/"
sha1sums=('3a7174e11c8c523ea461c9d47a80526ba29a6544')

build()
{
	Fdocrel docs
	mkdir build
	mv * build
	cd build
	Fbuild
}
