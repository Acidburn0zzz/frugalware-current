pkgname=gtkdatabox
pkgver=0.9.1.1
pkgrel=2
pkgdesc="widget for live display of large amounts of fluctuating numerical data"
url="http://sourceforge.net/projects/gtkdatabox/"
depends=('gtk+2>=2.20.1')
options=('scriptlet')
Finclude sourceforge
groups=('xlib-extra')
archs=('i686' 'x86_64')
sha1sums=('dd086f64af52f2a70b2a776d607c870419474289')

build() {
	Fcd
	Fsed "examples" "" Makefile* || Fdie
	Fbuild
}

# optimization OK
