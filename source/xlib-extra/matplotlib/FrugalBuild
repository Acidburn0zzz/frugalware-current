# Compiling Time: 0.10 SBU
# Maintainer: Priyank Gosalia <priyankmg@gmail.com>
# Contributor: enda <enda@netou.com>

pkgname=matplotlib
pkgver=0.98.5
pkgrel=4
pkgdesc="A python 2D plotting library for easy plotting of plots, histograms, charts."
depends=('numpy' 'pygtk>=2.14.1-2' 'python-dateutil')
makedepends=('setuptools')
Finclude sourceforge python
groups=('xlib-extra')
archs=('i686' 'x86_64')
sha1sums=('b494ccceba1d9f7ab531dca80446f81a52e6430e')

build()
{
	Fcd
	rm Makefile || Fdie
	Fbuild
	# Remove conflicting files (#3784)
	Frm $_F_python_libdir/dateutil
}

# optimization OK
