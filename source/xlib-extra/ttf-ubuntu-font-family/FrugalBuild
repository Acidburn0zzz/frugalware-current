pkgname=ttf-ubuntu-font-family
_F_archive_name="ubuntu-font-family"
pkgver=0.71.2
pkgrel=1
pkgdesc="Ubuntu font family"
url="http://font.ubuntu.com/"
depends=('fontconfig')
_F_fonts_subdir="ubuntu-font-family"
Finclude fonts
groups=('xlib-extra')
archs=('i686')
Fpkgversep="-sources_"
up2date="Flasttar $url | sed 's/.orig//g'"
source=($url/download/$_F_archive_name$Fpkgversep$pkgver.orig.tar.gz)
sha1sums=('d33ed13417cc23b3eb1caf0d551fc6d8c41e59c3')

