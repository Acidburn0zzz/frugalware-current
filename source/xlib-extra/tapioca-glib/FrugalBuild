pkgname=tapioca-glib
pkgver=20070326
pkgrel=1
url="http://tapioca-voip.sourceforge.net"
pkgdesc="Core Tapioca library"
depends=('libtelepathy' 'farsight' 'gtk+2' 'dbus-glib>=0.72')
groups=('xlib-extra')
archs=('i686')
up2date="$pkgver"
source=(http://ftp.frugalware.org/pub/other/sources/$pkgname/$pkgname-$pkgver.tar.bz2)
signatures=($source.asc)

build() {
	Fcd
	NOCONFIGURE=y ./autogen.sh || Fdie
	Fbuild
}

# optimization OK
