pkgname=flam3
pkgver=3.0.1
pkgrel=1
pkgdesc="Tools and a library for creating flame fractal images"
url="http://flam3.com/"
depends=('libjpeg>=8a' 'libxml2' 'libpng>=1.4.1')
Finclude googlecode
groups=('xlib-extra')
archs=('i686' 'x86_64')
sha1sums=('8814515f2e49e034e47cf97e9d2c0e932844abb9')
build()
{
	Fcd $pkgname-$pkgver/src
	Fbuild
}
# optimization OK
