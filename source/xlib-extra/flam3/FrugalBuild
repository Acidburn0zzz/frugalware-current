pkgname=flam3
pkgver=3.0
pkgrel=3
pkgdesc="Tools and a library for creating flame fractal images"
url="http://flam3.com/"
depends=('libjpeg>=8a' 'libxml2' 'libpng>=1.4.1')
Finclude googlecode
groups=('xlib-extra')
archs=('i686' 'x86_64')
sha1sums=('5b3317bdf51ecce7414a728c9d3735122a083b23')
build()
{
	Fcd $pkgname-$pkgver/src
	Fbuild
}
# optimization OK
