options+=('asneeded')

pkgname=allegro5
pkgver=5.0.10
pkgrel=1
pkgdesc="Allegro graphics library"
depends=('libsm' 'libxxf86vm' 'libxxf86dga' 'alsa-lib' 'libpng' 'libglu' 'libvorbis' 'libxcursor')
makedepends=('mesa-libgl-headers')
groups=('xlib-extra')
archs=('i686' 'x86_64')
_F_sourceforge_name="allegro"
_F_sourceforge_dirname="alleg"
Finclude cmake sourceforge
up2date=$pkgver
sha1sums=('f2b4535ac6fc6810f915dd7e75b27f967161726f')

